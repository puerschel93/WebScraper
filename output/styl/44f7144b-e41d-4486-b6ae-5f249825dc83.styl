.appear
	position absolute
	display none
	width 100%
	height 100%
	z-index 9999999
	position fixed
	background: rgba(0,0,0,0.7)
	top 0
	.large
		position relative
		width 60%
		height 100%
		left 0
		right 0
		margin 0 auto
		margin-top 60px
		img.big
			position absolute
			left 0
			right 0
			margin 0 auto
			width 100%
			border 10px solid white
			border-radius 10px
		img.close
			position absolute
			top -12px
			right -15px
			cursor pointer
