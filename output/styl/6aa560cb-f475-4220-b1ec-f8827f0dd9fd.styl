.sidebar2
    dn();

.sidebar
    w(60); mxw(200); rl(); z(1); fx(60,0,0,0,2); tr-cl();
    //bs(#000 0 4px 8px 0px !important);

    html.nav-expanded &
        w(70%);

    .dock-scroll-top
        dn(); ab(5,0,,0,1); tac();

    .dock-scroll-bottom
        dn(); ab(,0,5,0,1); tac();

    .dock
        p(0,0,150,0); fg(gray);
        // overflow-y: scroll;
        // overflow-x: hidden;

        li
            rl();
            &:before
                ct(""); ab(0,0,,0); h(1); bg(#404040); bg(rgba(255, 255, 255, 0.07));
            &:after
                ct(""); ab(,0,0,0); h(1); bg(#131313); bg(rgba(0, 0, 0, 0.17));

        li.separator
            tac(); fy(italic); lh(2); h(60); db();
            i
                db(); tac();


        li.launcher
            tsn();


            > a, > a i
                tr-cl(); fg(gray);

            > a
                rl(); db(); h(60); cur(pointer);
                tdn(); fs(0.8em)

                > i
                    wh(60); ab(0,0,0,0); fs(2.5em); tac(); ptb(15);

                > i.counter
                    rad(12); bg(black); fg(white); fs(13); dib(); p(2,7); ab(-5,3,,,1); op(0.7);
                    b(auto); l(auto); wh(auto);
                > span
                    dn(); fs(14); fy(italic); ab(20,,,60); tuc();

                    html.nav-expanded &
                        db();
            // &.home
            //     > a i
            //         fs(15);

            &:hover, &.active
                > a > i, > a
                    fg(darker-color); bg(#EEE)

                // &:before
                //     ts(rotate(45deg)); ct(" "); dib(); wh(10); bg(#EEE);
                //     ab(); r(-6); mt(22);

            &.disabled, &.denied
                rl(); dn();
                a
                    op(0.5); cur(not-allowed)
                &:hover, &.active
                    > a > i, > a
                        fg(white);

             &.denied
                a
                    op(0.2);
                &:before
                    ab(,15,5,,1); fg(#5D5D5D);
                    text-shadow: rgba(0, 0, 0, 0.78) 0px 0px 14px;
                    font: normal normal normal 25px/1 FontAwesome;
                    content: "\f023";
                    cur(not-allowed)

            // &:hover

            //     > ul.dropdown-menu
            //         op(1);
            //         tr(all 0.2s ease-in);
            //         visibility: visible;

