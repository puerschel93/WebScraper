.ruler
    margin-bottom: 40px
    &__container
        display block
        max-width: 1245px
        margin: 0 auto
        @media(max-width: 1299px)
            max-width: 991px
    &__element
        display: flex !important
        align-items: center
        justify-content: center;
        border: 1px solid #cecece
        border-radius: 5px
        height 65px
        max-width: 290px
        margin 0 10px
        span
            display: block
            font-size: 14px
            color: #4c4c4c
            line-height: 16px
        &.capital
            &:before
                content: '';
                display: inline-block;
                min-width: 33px
                min-height:34px
                margin-right: 15px
                background-image: url('data:image/svg+xml;base64,');
                background-repeat: no-repeat;
                background-position: center;
                background-size: 33px 34px
        &.brasil
            &:before
                content: '';
                display: inline-block;
                min-width: 40px
                min-height:30px
                margin-right: 15px
                background-image: url('data:image/svg+xml;base64,');
                background-repeat: no-repeat;
                background-position: center;
                background-size: 40px 30px
        &.card
            &:before
                content: '';
                display: inline-block;
                min-width: 35px
                min-height:32px
                margin-right: 15px
                background-image: url('data:image/svg+xml;base64,');
                background-repeat: no-repeat;
                background-position: center;
                background-size: 35px 32px
        &.desconto
            &:before
                content: '';
                display: inline-block;
                min-width: 43px
                min-height:35px
                margin-right: 15px
                background-image: url('data:image/svg+xml;base64,');
                background-repeat: no-repeat;
                background-position: center;
                background-size: 43px 35px
    &__destaque
        font-size: 16px !important
        font-weight: 600
        text-transform uppercase
    &__text
        color: #a5a5a5 !important
    .info__list
        border: 1px solid $cor01
        display: flex
        justify-content: space-between
        align-items: center
        flex-wrap: wrap
        padding: 25px 20px 
        margin-bottom 40px

        li  
            display: flex
            align-items: center 

            @media(max-width: 992px)
                justify-content: center

            span
                font-size: 14px
                line-height: 16px
                color: #6D6A5C
                text-shadow: 0 0
            
            &:nth-child(1)
                &:before
                    @extend $ico-before
                    // background-image svg-inline(r1)
                    transition: $transition
                    width: 35px
                    height: 35px
                    background-size: 35px
                    margin-right 5px

            &:nth-child(2)
                &:before
                    @extend $ico-before
                    // background-image svg-inline(r2)
                    transition: $transition
                    width: 35px
                    height: 35px
                    background-size: 35px
                    margin-right 5px

            &:nth-child(3)
                &:before
                    @extend $ico-before
                    // background-image svg-inline(r3)
                    transition: $transition
                    width: 35px
                    height: 35px
                    background-size: 24px
                    margin-right 5px

            &:nth-child(4)
                &:before
                    @extend $ico-before
                    // background-image svg-inline(r4)
                    transition: $transition
                    width: 35px
                    height: 35px
                    background-size: 35px
                    margin-right 5px

            &:nth-child(5)
                &:before
                    @extend $ico-before
                    // background-image svg-inline(r5)
                    transition: $transition
                    width: 35px
                    height: 35px
                    background-size: 35px
                    margin-right 5px  

            strong  
                display: block
                text-transform: uppercase
                color: $cor01

    @media(max-width: 768px)
        &__list
            max-height 60px
            overflow hidden
        &__destaque,
        &__text
            font-size: 12px !important
        &__element
            padding 0px !important
            min-width: 200px !important;
            margin-bottom 10px
            height: 55px 
            &:before
                min-width: 30px !important
                min-height: 30px !important
                background-size: 30px !important
                margin-right: 10px !important
.fbits-categoria
    .ruler
        border-top: 1px solid #cecece

.slick-dots li button:before
    display: none!important