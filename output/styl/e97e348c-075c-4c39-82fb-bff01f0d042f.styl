@import nib

.grid
  position fixed
  top -320em
  right -320em
  bottom -320em
  left -320em
  margin auto
  height 320em
  width 320em
  background-color darken(neon, 55%)
  //background-image url(../images/matrix-mask-large.png), radial-gradient(transparent, darkness), radial-gradient(transparent, darkness)
  background-image url("data:image/png;base64,"), radial-gradient(transparent, darkness), radial-gradient(transparent, darkness)
  background-size 3.75em 3.75em, auto, auto
  transform translateZ(-50em)
