@import url('//fonts.googleapis.com/css?family=Josefin+Sans:300,400,700');

body {
  overflow-y:auto;
  overflow-x:hidden;
  /*background-image:url('img/background_tile.png');*/
  background: #333333;
  height:100%;
  width:100%;
  position:absolute;
  font-family:"Josefin Sans", "Helvetica Neue", Helvetica, Arial, sans-serif;
}
*, *:before, *:after {
  -moz-box-sizing:border-box;
  box-sizing:border-box;
}
.navbar .navbar-inner {
  border:1px solid #080808;
  border-radius:0;
  background:#292929;
}
.navbar .brand {
  color: #FAEBD7;
  text-shadow:0 1px 0 #8A8277;
  opacity:0.9;
  font-weight:bold;
}

.navbar .brand:hover {
  color: #edb76f;
}

#slideout {
  position:absolute;
  width:262px;
  height:100%;
  top:0;
  left:-222px;
}
#slideclick {
  position:relative;
  top:150px;
  float:right;
  height:40px;
  width:40px;
  background:#ee8d40;
  background-image:url("img/camera.png");
  background-repeat:no-repeat;
  background-position:2px 3px;
  border-top-right-radius:10px;
  border-bottom-right-radius:10px;
  box-shadow:0 0 3px rgba(85, 85, 85, 0.29), 0 2px 4px #666666, 0 12px 18px rgba(85, 85, 85, 0.29);
}
#slideout.popped > #slideclick {
  background:rgba(127, 128, 134, 0.6);
  background-image:url("img/arrow-close.png");
  background-repeat:no-repeat;
  background-position:4px 5px;
  opacity:0.7;
}
#slidecontent {
  border:1px solid rgba(196, 189, 191, 0.31);
  background: #3f3f3f;
  width:220px;
  height:100%;
  overflow-y:auto;
  a {
    display:block;
    color:#faebd7;
    font-size:1.2em;
    font-weight:bold;
    text-decoration:none;
    border-bottom:1px solid rgba(41, 41, 41, 0.53);
    padding:10px;
  }
  a:hover {
    color: #edb76f;
  }
  li.active > a {
    color:#ee8d40;
    display:block;
    font-size:1.3em;
    font-weight:bold;
    text-decoration:none;
  }
  h3 {
    color: #edb76f;
    font-size:20px;
    padding-left:20px;
    margin:0;
    font-weight:bold;
    background:rgba(127, 128, 134, 0.6);
    text-shadow:0 1px 0 #000000;
    opacity:0.9;
  }
}
.icon-camera, .icon-folder-close, .icon-folder-open, .icon-home, .icon-picture {
  opacity:0.60;
}
.icon-picture {
  background-position: -457px -50px;
}

.sublist {
  list-style:none;
}

#quote {
  position:relative;
  top:150px;
  float:none;
  margin-left:auto;
  margin-right:auto;
  .well {
    border: 1px solid #4e4e4e;
    border-radius:0;
    background: #3f3f3f;
    color:#faebd7;
    text-shadow: 0 1px 0 #37342f;
    font-family:"Josefin Sans", "Helvetica Neue", Helvetica, Arial, sans-serif;
  }
}
#loading {
  display:none;
  width:32px;
  height:32px;
  background:url('img/loader.gif') no-repeat center center;
  position:fixed;
  top:50%;
  left:50%;
  margin:-20px 0 0 -20px;
}
#gallery {
  position:relative;
  height:100%;
  width:100%;
  a.static:hover, a.show:hover {
    opacity:0.9 !important;
  }
  a {
    opacity:0;
    float:left;
    background-size:cover;
    background-position:center center;
    -webkit-transform:scale(0.8);
    -ms-transform:scale(0.8);
    -o-transform:scale(0.8);
    transform:scale(0.8);
    -webkit-transition:0.4s;
    -ms-transition:0.4s;
    -o-transition:0.4s;
    transition:0.4s;
  }
  a.static {
    opacity:1;
    -webkit-transform:none;
    -ms-transform:none;
    -o-transform:none;
    transform:none;
    -webkit-transition:opacity 0.4s;
    -ms-transition:opacity 0.4s;
    -o-transition:opacity 0.4s;
    transition:opacity 0.4s;
  }
  a.next {
    background-color:#3f3f3f;
    cursor:pointer;
    background-image:url('img/arrow_next.png');
    /*border-bottom:1px solid rgba(196, 189, 191, 0.31);
    border-right:1px solid rgba(196, 189, 191, 0.31);*/
  }
  a.prev {
    background-color:#3f3f3f;
    cursor:pointer;
    background-image:url('img/arrow_prev.png');
    /*border-top:1px solid rgba(196, 189, 191, 0.31);
    border-left:1px solid rgba(196, 189, 191, 0.31);*/
  }
  a.show {
    opacity:1;
    -webkit-transform:scale(1);
    -ms-transform:scale(1);
    -o-transform:scale(1);
    transform:scale(1);
  }
}
.swipebox {
  overflow:hidden !important;
}
#swipebox-overlay {
  width:100%;
  height:100%;
  position:fixed;
  top:0;
  left:0;
  z-index:99999 !important;
  overflow:hidden;
  -webkit-user-select:none;
  -moz-user-select:none;
  user-select:none;
  background:#0d0d0d;
  img {
    border:none !important;
  }
}
#swipebox-slider {
  height:100%;
  left:0;
  top:0;
  width:100%;
  white-space:nowrap;
  position:absolute;
  display:none;
  .slide {
    background:url("img/loader.gif") no-repeat center center;
    height:100%;
    width:100%;
    line-height:1px;
    text-align:center;
    display:inline-block;
    img {
      display:inline-block;
      max-height:100%;
      max-width:100%;
      margin:0;
      padding:0;
      width:auto;
      height:auto;
      vertical-align:middle;
    }
  }
  .slide:before {
    content:"";
    display:inline-block;
    height:50%;
    width:1px;
    margin-right:-1px;
  }
}
#swipebox-action {
  position:absolute;
  left:0;
  z-index:999;
  height:50px;
  width:100%;
  bottom:-50px;
  text-shadow:1px 1px 1px black;
  background-color:#0d0d0d;
  background-image:-webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #0d0d0d), color-stop(100%, #000000));
  background-image:-webkit-linear-gradient(#0d0d0d, #000000);
  background-image:-moz-linear-gradient(#0d0d0d, #000000);
  background-image:-o-linear-gradient(#0d0d0d, #000000);
  background-image:linear-gradient(#0d0d0d, #000000);
  -webkit-box-shadow:0 1px 1px 1px #212121, inset 0 1px 1px 1px black;
  -webkit-box-shadow:0 -1px -1px 1px #212121, inset 0 -1px -1px 1px black;
  -moz-box-shadow:0 1px 1px 1px #212121, inset 0 1px 1px 1px black;
  -moz-box-shadow:0 -1px -1px 1px #212121, inset 0 -1px -1px 1px black;
  box-shadow:0 1px 1px 1px #212121, inset 0 1px 1px 1px black;
  box-shadow:0 -1px -1px 1px #212121, inset 0 -1px -1px 1px black;
  filter unquote('progid:DXImageTransform.MicrosoftAlpha(Opacity=95)');
  opacity:0.95;
  #swipebox-prev.disabled, #swipebox-next.disabled {
    filter unquote('progid:DXImageTransform.MicrosoftAlpha(Opacity=30)');
    opacity:0.3;
  }
  #swipebox-prev {
    background-image:url("img/swipebox-icons.png");
    background-repeat:no-repeat;
    border:none !important;
    text-decoration:none !important;
    cursor:pointer;
    position:absolute;
    width:50px;
    height:50px;
    top:0;
    background-position:-32px 13px;
    right:100px;
  }
  #swipebox-next {
    background-image:url("img/swipebox-icons.png");
    background-repeat:no-repeat;
    border:none !important;
    text-decoration:none !important;
    cursor:pointer;
    position:absolute;
    width:50px;
    height:50px;
    top:0;
    background-position:-78px 13px;
    right:40px;
  }
  #swipebox-close {
    background-image:url("img/swipebox-icons.png");
    background-repeat:no-repeat;
    border:none !important;
    text-decoration:none !important;
    cursor:pointer;
    position:absolute;
    width:50px;
    height:50px;
    top:0;
    background-position:15px 12px;
    left:40px;
  }
}
#swipebox-caption {
  position:absolute;
  left:0;
  z-index:999;
  height:50px;
  width:100%;
  top:-50px;
  text-align:center;
  text-shadow:1px 1px 1px black;
  background-color:#0d0d0d;
  background-image:-webkit-gradient(linear, 50% 0%, 50% 100%, color-stop(0%, #0d0d0d), color-stop(100%, #000000));
  background-image:-webkit-linear-gradient(#0d0d0d, #000000);
  background-image:-moz-linear-gradient(#0d0d0d, #000000);
  background-image:-o-linear-gradient(#0d0d0d, #000000);
  background-image:linear-gradient(#0d0d0d, #000000);
  -webkit-box-shadow:0 1px 1px 1px #212121, inset 0 1px 1px 1px black;
  -moz-box-shadow:0 1px 1px 1px #212121, inset 0 1px 1px 1px black;
  box-shadow:0 1px 1px 1px #212121, inset 0 1px 1px 1px black;
  filter unquote('progid:DXImageTransform.MicrosoftAlpha(Opacity=95)');
  opacity:0.95;
  color:white !important;
  font-size:15px;
  line-height:43px;
  font-family:Helvetica, Arial, sans-serif;
}
#swipebox-action.visible-bars {
  bottom:0;
}
#swipebox-action.force-visible-bars {
  bottom:0 !important;
}
#swipebox-caption.visible-bars {
  top:0;
}
#swipebox-caption.force-visible-bars {
  top:0 !important;
}
#swipebox-slider.rightSpring {
  -moz-animation:rightSpring 0.3s;
  -webkit-animation:rightSpring 0.3s;
}
#swipebox-slider.leftSpring {
  -moz-animation:leftSpring 0.3s;
  -webkit-animation:leftSpring 0.3s;
}
