.popup-modal-input-checkbox
  position absolute
  opacity 0

.popup-modal-input-checkbox + label
  position: relative;
  cursor: pointer;
  padding: 0;
  margin-bottom: 0;

.popup-modal-input-checkbox + label:before
  content: '';
  margin-right: 10px;
  border: 2px solid rgba(255, 255, 255, 0.05);
  display: flex;
  width: 20px;
  height: 20px;
  background: #404452
  border-radius: 5px;

.popup-modal-input-checkbox:checked + label:after
  content: '';
  position: absolute;
  left: 5px;
  top: 10px;
  background: #5bdf78;
  width: 2px;
  height: 2px;
  box-shadow: -4px 0px 0 #5bdf78, 2px 0px 0 #5bdf78, -2px 0px 0 #5bdf78, 4px 0 0 #5bdf78, 4px -2px 0 #5bdf78, 4px -4px 0 #5bdf78, 4px -6px 0 #5bdf78, 4px -8px 0 #5bdf78, 4px -10px 0 #5bdf78;
  transform: rotate(45deg);

