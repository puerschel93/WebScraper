.dms-tooltip
  display flex
  justify-content center
  cursor pointer
  position relative

  .icon
    size(18px)
    fill rgba(255, 255, 255, 0.6)

.dms-tooltip-block
  position absolute
  display none
  justify-content center
  width 250px
  padding 15px
  border-radius 10px
  font-family "Open Sans Reg",sans-serif
  color colorWOpacity
  font-size 13px
  top -105px
  z-index 50

  &:before
    content: '';
    position: absolute;
    width: 0;
    height: 0;
    bottom -13.5px
    transform-origin: -1px 3px;
    transform: rotate(-45deg);
    box-shadow: -3px 5px 5px -1px rgba(34, 34, 34, 0.3);
    border: 7px solid #2B2F3F;
    border-color: transparent transparent #2B2F3F #2B2F3F;

