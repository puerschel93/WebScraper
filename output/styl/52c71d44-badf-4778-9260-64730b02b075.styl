.b-group 

	.main-text {
	  font-size: 16px;
	  color: #52007F;
	  font-family: Montserrat;
	  font-weight: 600; }

	.main-button {
	  padding: 33px  50px;
	  background: #570085;
	  box-shadow: -8px 8px 16px rgba(224, 227, 233, 0.2), 8px -8px 16px rgba(224, 227, 233, 0.2), -8px -8px 16px rgba(248, 251, 255, 0.9), 8px 8px 20px rgba(224, 227, 233, 0.9), inset 1px 1px 2px rgba(248, 251, 255, 0.3), inset -1px -1px 2px rgba(224, 227, 233, 0.5);
	  border-radius: 50px;
	  border: none;
	  color: #fff;
	  font-weight: 600;
	  font-size: 20px; }

	button {
	  cursor: pointer; }

	.container {
	  width: 1830px;
	  margin: 0 auto; }
	  
	.center_block
		display flex !important
		align-items center !important
		justify-content center
		border-radius: 0 50px 50px  0 !important
		margin-left -20px
	  	
	.center 
		display flex !important
		justify-content center
		margin-left 20px

	.container {
	  position: relative;
	  height: 100%; }

	.main {
	  margin-top: 67px !important
	  display: flex; }

	.main-left {
	  padding-top: 0px;
	  display: flex;
	  padding-left: 120px;
	  margin-right 0
	  padding-right: 71px;
	  padding-bottom: 0px;
	  background: #fff;
	  border-radius: 20px;
	  height: 100%;
	  max-height: 710px; }

	.main-left__purple {
	  position: absolute;
	  width: 100px;
	  height 710px
	  background: #570085;
	  border-radius: 20px;
	  top: 0;
	  left: 0;
	  z-index: 1;
	  display: flex;
	  align-items: center;
	  flex-direction: column;
	  justify-content space-around }

	.main-left__purple img {
	  margin-bottom: 40px;
	  width: 60px;
	  height: 60px; }

	.main-left__purple img:first-child {
	  width: 30px;
	  height: 30px; }

	.main-left__purple img:last-child {
	  margin-bottom: 0; }

	.main-left .right-col {
	  display: flex;
	  justify-content space-around  !important
	  flex-direction: column;
	  align-items: center; }
	  
	.right-col
		margin-left 20px

	.main-left .right-col .img {
	  display: flex;
	  justify-content: space-between; }

	.main-left .right-col .img img {
	  margin-right: 0px; }

	.main-left .right-col .img img:last-child {
	  margin-left: 20px; }
	  
	.close 
		max-width none
		width 24px !important
		height 36px !important
		
	@media screen and (max-width: 1800px)

		.main-table--buttons 
			margin-left 150px  !important
			
	@media screen and (max-width: 1800px)
			overflow-x: scroll;
			margin 0 auto
			max-width 100%
			width 100%
		
	.icon 
		display flex
		align-items center
		justify-content center
		width 60px
		height 60px	
		background: linear-gradient(135deg, #53007E 0%, #5B008C 100%);
		box-shadow: -3px 3px 6px rgba(70, 0, 106, 0.2), 3px -3px 6px rgba(70, 0, 106, 0.2), -3px -3px 6px rgba(104, 0, 160, 0.9), 3px 3px 8px rgba(70, 0, 106, 0.9);
		border-radius: 20px;
		img
			transition .4s ease all 
			filter: grayscale(1)
		&:hover 
			img 
				filter: grayscale(.1)
				
		&:active
			box-shadow none
				
	.active 
		img
			filter: grayscale(.1)

	.main-left .right-col p {
	  height 36px
	  margin 0
	  color: #AFAFAF;
	  font-weight: bold;
	  font-size: 20px; }

	.main-left .right-col p a 
		height 36px !important
		color: #AFAFAF;
		font-weight: bold;
		font-size: 20px;
		text-decoration: none; 
		transition .4s ease all
		&:hover 
			color: #52007F;

	.main-left .right-col .active {
	  color: #570085; }

	.main-table {
	  height: 700px;
	  overflow-y: auto;
	  position: relative;
	  padding-right: 94px; }

	.main-table-scroll {
	  position: absolute;
	  top: -5px;
	  bottom: -5px;
	  right: 8px;
	  width: 2px;
	  height: calc(100% + 10px);
	  background: #570085;
	  border-radius: 2px; }

	.main-table__top {
	  display: flex;
	  font-size: 20px;
	  color: #570085; }

	.main-table__top p:nth-child(1) {
	  margin-right: 214px; }

	.main-table__top p:nth-child(2) {
	  margin-right: 106px; }

	.main-table__top p:nth-child(3) {
	  margin-right: 163px; }

	.main-table__top p:nth-child(4) {
	  margin-right: 41px; }

	.main-table__top p:nth-child(5) {
	  margin-right: 151px; }

	.main-table--buttons {
	  display: flex;
	  justify-content: space-between;
	  margin-top: 18px;
	  margin-left 350px }

	.main-table--buttons a {
	  padding: 33px  50px;
	  background: #570085;
	  box-shadow: -8px 8px 16px rgba(224, 227, 233, 0.2), 8px -8px 16px rgba(224, 227, 233, 0.2), -8px -8px 16px rgba(248, 251, 255, 0.9), 8px 8px 20px rgba(224, 227, 233, 0.9), inset 1px 1px 2px rgba(248, 251, 255, 0.3), inset -1px -1px 2px rgba(224, 227, 233, 0.5);
	  border-radius: 50px;
	  border: none;
	  color: #fff;
	  font-weight: 600;
	  font-size: 20px; }

	.main-table table {
	  text-align: center;
	  color: #52007F;
	  font-size: 16px;
	  font-weight: 600;
	  font-family: 'Montserrat', sans-serif;
	  border-spacing: 15px;
	  border-collapse: collapse; }
	  
	.main-table table tr td {
	  padding: 18px 25px;
	  position: relative;
	  background-color #fff }
	  
	.main-table table tr td:after {
	  content: '';
	  width: 1px;
	  position: absolute;
	  top: 0;
	  right: 0;
	  bottom: 0px;
	  background: rgba(87, 0, 133, 0.2); }

	.main-table table tr td:first-child {
	  border-radius: 50px 0 0 50px; }

	.main-table table tr td:last-child {
		border-radius: 0 50px 50px  0; }

	.main-table table tr td:last-child:after {
	  display: none; }
	  
	.close 
		width 24px !important
		height 24px !important
	.application__row {
	  display: flex;
	  background: #FFFFFF;
	  box-shadow: -8px 8px 16px rgba(224, 227, 233, 0.2), 8px -8px 16px rgba(224, 227, 233, 0.2), -8px -8px 16px rgba(248, 251, 255, 0.9), 8px 8px 20px rgba(224, 227, 233, 0.9);
	  border-radius: 20px; }

	.application__column {
	  width: 100%;
	  max-width: 454px;
	  padding: 30px 21px;
	  border-right: 1px solid rgba(87, 0, 133, 0.2); }

	.application__column:last-child {
	  border: none; }

	.application .column__header {
	  display: flex;
	  align-items: center;
	  justify-content: space-between;
	  margin-bottom: 34px; }

	.application .column__header .title {
	  font-family: 'Montserrat';
	  font-style: normal;
	  font-weight: normal;
	  font-size: 20px;
	  line-height: 24px;
	  display: flex;
	  align-items: center;
	  color: #570085; }

	.application .column__header .add {
	  border: 0;
	  padding: 0;
	  background: transparent;
	  display: flex;
	  align-items: center; }

	.application .column__header .add span {
	  margin-left: 15px; }

	.application .column__items {
	  margin-bottom: 20px; }

	.application .column__items:last-child {
	  margin-bottom: 0; }

	.application .column__items .item {
	  background: #AFFFB3;
	  border-radius: 20px;
	  padding: 16px 17px; }

	.application .column__items .item__title {
	  font-family: 'Montserrat';
	  font-style: normal;
	  font-weight: 300;
	  font-size: 14px;
	  line-height: 17px;
	  color: #555555;
	  padding-bottom: 5px;
	  border-bottom: 1px solid rgba(0, 0, 0, 0.2);
	  margin-bottom: 14px; }

	.application .column__items .item__card {
	  background: #D9FFDA;
	  border-radius: 10px;
	  padding: 5px 10px;
	  display: flex;
	  align-items: center;
	  margin-bottom: 10px; }

	.application .column__items .item__card:last-child {
	  margin-bottom: 0; }

	.application .column__items .item__card .card__image {
	  width: 34px;
	  height: 34px;
	  background: #CECECE;
	  border-radius: 10px;
	  margin-right: 10px; }

	.application .column__items .item__card .card__text {
	  font-family: 'Montserrat';
	  font-style: normal;
	  font-weight: 300;
	  font-size: 14px;
	  line-height: 17px;
	  color: #555555; }
	  
	.profile 
		margin-right 12px
		
	.vector 
		width: 12px;
		height: 12px;
		
	

.list 
	
	&__items 
		padding-top 30px
		margin-left 350px

	&__item 
		display flex
		align-items center
		background: #FFFFFF;
		box-shadow: -8px 8px 16px rgba(224, 227, 233, 0.2), 8px -8px 16px rgba(224, 227, 233, 0.2), -8px -8px 16px rgba(248, 251, 255, 0.9), 8px 8px 20px rgba(224, 227, 233, 0.9), inset 1px 1px 2px rgba(248, 251, 255, 0.3), inset -1px -1px 2px rgba(224, 227, 233, 0.5);
		border-radius: 50px;
		border 2px solid #DADADA
		margin-bottom 15px
		position relative
		
		&-first 
			display flex
			margin-bottom 36px
			
	&__text 
		font-family: Montserrat;
		font-weight: normal;
		font-size: 20px;
		line-height: 24px;
		color: #570085;
		&--1,
		&--2,
		&--3,
		&--4,
		&--5,
		&--6 
			position absolute
			top 0px

		&--1
			left 28%
		&--2
			left 40%
		&--3
			left 52%
		&--4
			left 59.5%
		&--5
			left 68.5%
		&--6
			left 85%	

	&__num,
	&__name,
	&__tel,
	&__well
	&__group,
	&__info 
		display flex
		justify-content center
		padding 20px 25px
		font-family: Montserrat;
		font-weight: 600;
		font-size: 16px;
		line-height: 20px;
		color: #52007F;
		border-right 1px solid rgba(87, 0, 133, 0.2);
		
	&__pay 
		margin 0 auto
		
	@media screen and (max-width: 1800px)
		&__items 
			width 1400px
			margin-left 150px
			
		.main-table--buttons 
			margin-left 150px  !important
			
		&__text 
			&--1
				left 17%
			&--2
				left 29%
			&--3
				left 41%
			&--4
				left 48.5%
			&--5
				left 57.5%
			&--6
				left 75%	