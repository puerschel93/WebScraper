.section-explore
	display none
	background-color white
	margin 50px 0px 0px 0px
	padding 0px
	
	.wrapper-filter-a, .wrapper-filter-b
		margin 0px
		padding 0px
		height 37px
		display block
		width 100%
		
		ul
			height 100%
			margin 0px
			align-items center
			padding 0px
			display flex
			align-content center
			justify-content space-around
			list-style none
			
			li
				background-color #E0E0E0
				height 23px
				width 85px
				border-radius 15px
				
				
				a
					text-decoration none
					width 100%
					height 100%
					display flex
					align-items center
					justify-content center
					span
						margin-right 4px
						
					p, span
						height 10px
						text-align center
						line-height 10px
						position relative
						top -1px
			li:hover
				background-color white
						
	.wrapper-filter-b
		display none					
	.substage-explore
		.wrapper-categories-btn
			display block
			background-color white
			margin 0px
			padding 0px
			position relative
			top 85px
			height 100%
			position fixed
			z-index 5
			width 100%
	
				
			ul
				display flex
				flex-wrap wrap
				width 100%
				margin 15px auto 0px auto
				padding 0px
				list-style none
				
				li
					width 100%
					margin 3px 0px
					padding 0px
					
					a
						text-decoration none
						display flex
						align-content flex-start
						align-items center
						justify-content flex-start
						height 35px
						cursor pointer
						div
							width 40px
							margin 0px 0px 0px 30px
							img
								display block
								height 20px
								
							
						p
							text-align center
							margin 0px
							
						span
							margin-top 3px
							margin-left 5%
							display none
							
				li:hover
					background-color #EFEFEF
					a span
						display block

.wrapper-thumbnails
	display flex
	flex-direction column
	justify-content center
	align-content center
	margin 120px auto 0px auto
	width auto
	.thumbnail
		cursor pointer
		display block
		width 100%
		height 260px
		overflow hidden
		.thumb-image
			position relative
			z-index 1
			display block
			width 100%
			height 100%
			margin 0px
			padding 0px
			overflow hidden
			-webkit-filter: blur(18px);
			-moz-filter: blur(18px);
			-o-filter: blur(18px);
			-ms-filter: blur(18px);
			filter blur(18px)
			
			img
				margin 0px
				padding 0px
				display block
				width 100%
				
			
		.mask
			position relative
			z-index 2
			display block
			margin 0px
			padding 0px
			top -260px
			
			.transparent-mask-content
				display block
				position relative
				z-index 3	
				height 260px
				width 100%
				margin 0
				overflow hidden
				
				.wrapper-price
					line-height 30px
					background-color #00aeb5
					opacity 0.7
					position relative
					top 13px
					left 0px
					height 45px
					width 120px
					display flex
					flex-wrap wrap
					align-items center
					align-content center
					padding 0px 7px
					
					.price
						margin 0px
						padding 0px
						text-align left
						height 26px
					p
						margin 0px
						padding 0px
						height 13px
						line-height 13px
						text-align right
					.price, p
						width 100%
					
				.background-ico-play
					background-color white
					opacity 0.3
					width 50px
					height 50px
					border-radius 50%
					margin 60px auto 0px auto
					padding 0px
					
			
				.ico-play
					margin -50px auto 0px auto
					display block
					width 50px
					height 50px
					line-height 50px
					text-align center
					font-size 2.5em
					border-radius 50%
					color white
											
				h3, h4
					display block
					position relative
					top 55px
					margin 0px 4% 0px 0px
					padding 0px
					text-align right
					
				h3
					text-transform uppercase
					
				h4
					text-transform capitalize

				

			.blur-mask-content
				display none
				position relative
				z-index 2
				height 100%
				margin 0px
				padding 0px
				width auto
					
				
				.wrapper-blur-mask-btn
					display flex
					justify-content space-between
					margin 13px 0px 13px 0px
					padding 0px 25px 10px 25px
					
				h1, .thumb-text
					margin 0px
					padding 0px 25px
					
				.thumb-description
					margin 6px 0px
					height 60px

				
				.wrapper-social-interaction-data
					list-style none
					margin 0px 0px 0px 0px
					padding 0px
					padding 0px 30px
					li
						display inline-block
						
						a, p
							display inline-block
						a
							text-decoration none
							.icos
								position relative
								top 2px
								
							.ico-heart-active
								display none
							.icon-heart-outline, .ico-heart-active
								position relative
								top 4px
								
							 
					.floating-wrapper-data
						float right
						
					.comments-info
						margin-left 5px

.transparent-btn
	background-color rgb(201, 201, 201, 0.1)
	margin 0px
	padding 0px 10px
	height 25px
	width auto
	border-radius 20px
	line-height 25px
	text-decoration none
	cursor pointer
	display flex
	align-items center
	align-content center
	justify-content space-around
	
	p
		margin 0px 0px 0px 7px
		
	span 
		color white
		



		
.wrapper-filter-a
	position fixed
	top 50px
	z-index 4
	background-color #EFEFEF

.wrapper-filter-b
	position fixed
	top 85px
	z-index 4
	background-color white
	
.btn-filter
	background-color #D8D8D8
	border-radius 10px
	width 80px
	height 20px
	margin 0px 3px
	padding 0px
	overflow hidden
	a
		cursor pointer
		position relative
		top -3px
		display block
		width 100%
		height 100%
		text-decoration none
		p
			display block
			width 100%
			font-size 0.6em
			font-weight lighter
			color #585858
			font-family "Oxygen"
			text-align center
			margin 6px auto 0px auto
			
.movil-element-hidden
	display none

@media screen and (min-width: 767px)
		
	body .container .app-stage .section-explore .substage-explore .wrapper-thumbnails
		display flex
		flex-direction row
		flex-wrap wrap
		width 712px
		margin 130px auto 0px auto
		justify-content flex-start
		.thumbnail
			width 346px
			margin 0px 5px 10px 5px

@media screen and (min-width: 1000px)
	.background-ico-play, .wrapper-thumbnails .thumbnail .mask .transparent-mask-content .ico-play
		display none
	.wrapper-thumbnails .thumbnail .mask .transparent-mask-content 
		h3, h4
			top 160px
	.section-explore
		margin 50px 0px 10px 0px 
	.wrapper-thumbnails
		width auto
		margin 0px auto
		
	body .container .app-stage .section-explore .substage-explore .wrapper-thumbnails
		justify-content flex-start
		align-content flex-start
		margin 0px auto 0px auto
		padding 20px 15px 0px 15px
		border-top solid 1px white
		border-left solid 1px white
		
		.thumbnail
			margin-top 0px
			margin-bottom 10px
			border-radius 10px
			
			

	.section-explore .wrapper-filter-a 
		position relative
		top 0px
		height 37px
		background none
		background-color #F2F2F2
		ul
			max-width 300px
			margin 0px auto
			
			
						
		
	.section-explore .wrapper-filter-b
		position relative
		top 0px
		height 37px
		background-color #FBFAFA
		ul
			max-width 430px
			margin 0px auto
			
			li
				background-color #F2F2F2
		
	.element-hidden
		display none
		
	.movil-element-hidden
		display block

	body .container .app-stage .section-explore .substage-explore .wrapper-categories-btn
		display block
		width 170px
		padding 0px
		margin 5px 0px 0px 0px
		position relative
		top 0px
			
		ul
			list-style none
			margin 10px auto 0px auto
			padding 0
			display flex
			align-content center
			align-items center
			justify-content center
			width 160px
			height 100%
			
			
			li
				width 90%
				cursor pointer
				height 30px
				margin-bottom 5px 
				
				a
					width 100%
					height 100%
					display flex
					align-items center
					justify-content flex-start
					text-decoration none
					
					div	
						margin 0px
						width 40px
						img, p
							margin 0px
						img 
							display block
							margin 0px auto
	
					
				
						
	
	body .container .app-stage .section-explore .substage-explore
		display flex
		align-content flex-start
		
@media screen and (min-width: 1278px)
	
	body .container .app-stage .section-explore .substage-explore .wrapper-thumbnails
		width 1110px
