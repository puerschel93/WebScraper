.out-side-confirm {
  background-color: #f4f4f4;
  min-height: 100vh;
  padding-bottom: 2rem;
}

.out-side-confirm .gold-auto {
  display: inline-block;
  width: .95rem;
  height: .6rem;
  overflow: hidden;
  float: right;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu3.png') no-repeat 0 -5.6rem / 1rem 7rem;
  margin-top: .35rem;
}

.out-side-confirm .gold-auto.this-over {
  background: url('https://api.duishangbao.cn/upload/image/icon_zu3.png') no-repeat 0 -6.3rem / 1rem 7rem;
}

.out-side-confirm .confirm-header-null {
  height: .88rem;
}

.out-side-confirm .order-address {
  overflow: hidden;
  background-color: #fff;
}

.out-side-confirm .order-address .line {
  display: block;
  height: .2rem;
  background: url('https://api.duishangbao.cn/upload/image/line-order-header.png') repeat-x center / .84rem .04rem;
  border-bottom: .5px solid #e5e5e5;
}

.out-side-confirm .order-addr-mess {
  width: 7.1rem;
  margin: 0 auto;
  position: relative;
}

.out-side-confirm .order-addr-mess .name {
  display: inline-block;
  font-size: .28rem;
  color: #1a1a1a;
  line-height: .32rem;
  float: left;
  margin-top: .2rem;
}

.out-side-confirm .order-addr-mess .tel {
  display: inline-block;
  font-size: .28rem;
  color: #1a1a1a;
  line-height: .32rem;
  float: right;
  margin: .2rem .63rem 0 0;
}

.out-side-confirm .order-addr-mess .addr-main {
  display: block;
  clear: both;
  line-height: .32rem;
  padding: .15rem .56rem .4em 0;
}

.out-side-confirm .order-addr-mess .icon-star {
  display: inline-block;
  width: .5rem;
  height: .5rem;
  position: absolute;
  right: 0;
  top: .4rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat .3rem -1.93rem/0.7rem 10rem;
  z-index: 1;
}

.out-side-confirm .order-addr-mess .add-new-btn {
  display: block;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu4.png') no-repeat 0 -1.15rem/0.5rem 10rem;
  padding: .38rem 0 .3rem .58rem;
}

.out-side-confirm .order-goods-list {
  background-color: #fff;
  padding: .2rem;
  overflow: hidden;
  margin-top: .2rem;
}

.out-side-confirm .order-goods-list .pub-name {
  height: .7rem;
}

.out-side-confirm .order-goods-list .order-goods-bar {
  height: 2.15rem;
  position: relative;
}

.out-side-confirm .order-goods-img {
  width: 1.68rem;
  height: 1.68rem;
  position: absolute;
  left: 0;
  top: 0;
}

.out-side-confirm .order-goods-img img {
  width: 1.68rem;
  height: 1.68rem;
}

.out-side-confirm .order-goods-bar .order-goods-name {
  line-height: .36rem;
  padding: 0 .8rem .05rem 1.88rem;
}

.out-side-confirm .order-goods-bar .order-goods-sku {
  line-height: .32rem;
  padding: 0 .8rem 0 1.88rem;
}

.out-side-confirm .order-goods-bar .order-goods-price {
  width: 5rem;
  position: absolute;
  top: 1.22rem;
  left: 1.88rem;
}

.out-side-confirm .order-goods-bar .order-goods-price .price-num {
  display: inline-block;
  line-height: .4rem;
  float: left;
  margin: .01rem 0 0 0;
}

.out-side-confirm .order-goods-bar .order-goods-price .return-num {
  display: inline-block;
  color: #fc6c15;
  border: .5px solid #fc6c15;
  padding: .02rem .06rem .03rem;
  border-radius: .05rem;
  position: relative;
  float: left;
  margin: 0 0 0 .2rem;
}

.out-side-confirm .order-goods-bar .order-goods-num {
  display: inline-block;
  position: absolute;
  right: 0;
  top: 1.24rem;
}

.out-side-confirm .order-goods-message {
  height: .8rem;
  border-top: 0.5px solid #f4f4f4;
  border-bottom: .5px solid #f4f4f4;
}

.out-side-confirm .order-goods-message .title {
  display: inline-block;
  width: 1.8rem;
  height: .38rem;
  line-height: .38rem;
  float: left;
  margin: .2rem 0 0 0;
}

.out-side-confirm .order-goods-message input {
  width: 5.29rem;
  height: .38rem;
  line-height: .38rem;
  border: 0;
  float: left;
  margin: .2rem 0 0 0;
}

.out-side-confirm input::-webkit-input-placeholder {
  color: #737373
}

.out-side-confirm .order-goods-delivery {
  height: .8rem;
}

.out-side-confirm .order-goods-delivery .title {
  display: inline-block;
  line-height: .8rem;
  float: left;
}

.out-side-confirm .order-goods-delivery .delivery {
  display: inline-block;
  line-height: .8rem;
  float: right;
}

.out-side-confirm .order-goods-subtotal {
  overflow: hidden;
  padding: .25rem 0 0;
  line-height: .5rem;
  text-align: right;
}

.out-side-confirm .order-goods-p-count {
  overflow: hidden;
  background-color: #fff;
  padding: .2rem;
  margin-top: .2rem;
}

.out-side-confirm .order-goods-p-count li {
  line-height: .55rem;
}

.out-side-confirm .order-goods-p-count li .price {
  display: inline-block;
  float: right;
}

.out-side-confirm .go-pay {
  height: 1rem;
  width: 100vw;
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: 0;
  border-top: .5px solid #f4f4f4;
}

.out-side-confirm .go-pay span {
  display: inline-block;
  width: 5.06rem;
  line-height: 1rem;
  float: left;
  text-align: right;
}

.out-side-confirm .go-pay span b,
.out-side-confirm .go-pay span i {
  display: inline-block;
  padding-left: .05rem;
  line-height: 1rem;
  float: right;
}

.out-side-confirm .go-pay a {
  display: inline-block;
  width: 2.12rem;
  line-height: 1rem;
  float: right;
  background-color: #e62343;
  text-align: center;
}

.out-side-confirm .golden-yh {
  width: 100vw;
  height: .82rem;
  background-color: #fff;
  position: fixed;
  left: 0;
  bottom: .98rem;
  border-top: .5px solid #f4f4f4;
}

.out-side-confirm .golden-yh .title {
  display: inline-block;
  line-height:.82rem;
  padding-left: .2rem;
}

.out-side-confirm .golden-yh .title i {
  color: #fe7600;
}

.out-side-confirm .golden-yh .gold-auto {
  width: .48rem;
  height: .48rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -1rem/0.7rem 10rem;
  float: right;
  margin: .16rem;
}

.out-side-confirm .golden-yh .gold-auto.this-over {
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 0/0.7rem 10rem;
}