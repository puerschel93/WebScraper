$path        = "http://r.syyx.com/events/p026/"
$bg          = $path"bg.jpg"
$main        = $path"main.jpg"
$btn         = $path"btn.jpg"
$cyl         = $path"cyl.jpg"
$gdj         = $path'gdj.jpg'
$jws         = $path'jws.jpg'
$qxs         = $path'qxs.jpg'
$ynz         = $path'ynz.jpg'
$reg_scroll  = $path'reg_scroll.jpg'
$reg_bg      = $path'reg_bg.jpg'
$reg_btn     = $path'reg_btn.jpg'
$loading     = $path'loading.gif'
$right_wrong = $path'right_wrong.png'
