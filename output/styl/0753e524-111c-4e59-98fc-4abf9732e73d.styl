body
    margin 0
    font 12px/2em "宋体"
    color #5A506E
    background #000
a
    text-decoration none
    color #D8AF6F
    outline none
    &:hover
        color #fff
.container    
    position relative
    background url($bg) no-repeat center top 
    min-width 980px
.wrap
    background url($main) no-repeat center 600px 
    position relative
    min-width 980px
.main
    width 980px
    margin 0 auto
    height 1600px
    position relative
.nav
    padding-left 700px
    width 270px
    color #D8AF6F
    a
        line-height 30px
.logo
    position absolute
    width 187px
    height 86px
    left 37px
    top 20px
    text-indent -9999cm
#flash, #flash_but, #flash_btn
    width 186px
    height 105px
    position absolute
    top 329px
    left 73px
#flash_btn
    background url($btn) no-repeat 999cm
    cursor pointer
.nycs
    width 125px
    height 39px
    position absolute
    background url($btn) no-repeat 999cm
    left 645px
    top 402px
