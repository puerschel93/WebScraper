        background-position -623px -17px
.mini
    left 427px
    &:hover
        background-position -751px -17px
#job
    position absolute
    top 610px
    left 0
    width 679px
    height 272px
    color #FAC995
    #j_tab
        height 30px
        position absolute
        width 400px
        left 121px
        top 5px
        span
            display inline-block
            margin-right 1px
            width 99px
            float left
            background url($btn) no-repeat 9999cm
            height 100%
            cursor pointer
        .check_0
            background-position -99px -23px
        .check_1
            background-position -199px -23px
        .check_2
            background-position -299px -23px
        .check_3
            background-position -399px -23px
    #j_con
        height 273px
        width 100%
        top 0px
        position absolute
        overflow hidden
        line-height 25px
        .con
            width 100%
            height 100%
            position absolute
            top 0
            display none
        .jws
            background url($jws) no-repeat right top
        .qxs
            background url($qxs) no-repeat right top
        .ynz
            background url($ynz) no-repeat right top
        .gdj
            background url($gdj) no-repeat right top
        .feat
            width 220px
            position absolute
            top 84px
            left 300px
            line-height 22px
        .video
            width 238px
            height 134px
            position absolute
            top 100px
            left 27px
            background #000
            overflow hidden
    #to_reg_3
        width 99px
        height 24px
        background url($btn) no-repeat 0 -5px
        position absolute
        top 210px
        right 40px
        cursor pointer
        &:hover
            background-position 0 -29px
#img_focus
    width 972px
    height 428px
    position absolute
    top 897px
    left 4px
    background url($loading) center 200px no-repeat
#img_focus_tab
    position absolute
    top 366px
    height 58px
    a
        width 238px
        display inline-block
        float left
        height 58px
        cursor pointer
        background url($btn) no-repeat 999px 999px
        margin-right 6px
    .check_0
        background-position 0 -53px
    .check_1
        background-position -244px -53px
    .check_2
        background-position -488px -53px
    .check_3
        background-position -732px -53px
#img_focus_img
    height 366px
    img
        display none
        position absolute
.copy_right
    width 980px
    text-align center
    position absolute
    top 1355px
    line-height 18px
    .feet_txt
        text-align left
        margin 10px 0 0 380px
    .feet_logo
        position absolute
        width 140px
        height 60px
        left 225px
        top 30px
.pop_video_detail
    background: #54200A;
    border 1px solid #873410
#pop_flash
    border 1px solid #873410
    background #000
    line-height 0
    height 350px
.ui-dialog .ui-dialog-titlebar-close
    background url($close) no-repeat
object
    outline none
#reg_scroll
    width 70px
    height 383px
    position absolute
    top 100px
    right 0
    #reg_but
        width 70px
        height 291px
        background url($reg_scroll) no-repeat 0 0
        position absolute
        &:hover
            background-position -72px 0
    #reg_box
        width 284px
        height 455px
        display none
        background url($reg_bg) no-repeat #221334
        position relative
        overflow hidden
    .reg_panel
        width 283px
        padding-top 80px
        color #c998ff
        overflow hidden
        height 390px
        .reg
            margin-left 40px
        a
            color #A696C7
            text-decoration underline
            &:hover
                color #fff
        table
            height 162px
            td
                height 27px
                width 20px
            input
                background #776595
                border 1px solid #683ec8
                height 18px
                color #221334
                line-height 18px
                width 145px
            .focus_in_inut
                background #af99d4
        .agreement
            height 21px
        .reg_form_item_name
            width 60px
        .reg_form_item_input
            width 150px
        .validate_num_input
            height 30px
            vertical-align top
            padding-top 3px
        #txtValidate
            width 50px
        #validateimg
            width 90px
            margin-left 5px
            height 27px
            vertical-align top
            cursor pointer
        #reg_step2,#reg_step3,#reg_step4,#reg_step5
            width 220px
            margin 0 auto
            padding-top 30px
            line-height 25px
            display none
            height 120px
            text-align center
            input
                background #776595
                border 1px solid #683ec8
                height 18px
                color #221334
                line-height 18px
                width 145px
        #reg_step2
            height 211px            
            .active
                height 200px
                line-height 30px
        #reg_step4 .active_info
                margin-top 40px
        #reg_step5
            height 390px
            width 100%
            p
                padding 0 40px
        #active_link
            display block
            margin 20px auto
            width 120px
            height 30px
            background url($reg_btn) no-repeat -2px -61px
            &:hover
                background-position -2px -93px
        .download
            height 66px
            padding 0 10px
            position relative
            margin 0 auto
            background url($download) no-repeat
            .mini_download
                height 30px
                width 125px
                display block
                position relative
                top 80px
                float left
                background url($reg_btn) no-repeat -13px -125px
                &:hover
                    background-position:-13px -157px
            .full_download
                height 30px
                width 125px
                display block
                position relative
                top 80px
                float right
                background url($reg_btn) no-repeat -140px -125px
                &:hover
                    background-position:-140px -157px
        .download_tip
            height 40px
            line-height 30px
            margin-top 80px
            text-align center
            font-size 14px
            padding 0 40px
            text-align left
        .reg_submit
            height 55px
            position relative
        #reg_button
            width 120px
            height 30px
            position absolute
            left 51px
            top 11px
            background url($reg_btn) no-repeat -124px -61px
            &:hover
                background-position -124px -93px
        .success
            background url($success) no-repeat
            padding 10px 40px
            display inline
            color #FAAB10
        .fail
            background url($fail) no-repeat
            padding 10px 40px
            display inline
            color #FAAB10
        .back_bg
            position absolute
            top 360px
            height 20px
            width 100%
            background url($download) no-repeat
            left 0
        #back
            width 120px
            height 30px
            position absolute
            top 40px
            left 81px
            background url($reg_btn) no-repeat -267px -125px
            &:hover
                background-position -267px -157px
        .check_result_wrong
            background url($right_wrong) no-repeat 0 -32px
            height 100%
            width 16px
        .check_result_right 
            background: url($right_wrong) no-repeat 0 4px
            height: 25px;
            width: 16px;

        .check_result_loading 
            background url($loader) no-repeat 0 4px
            height 25px
            width 16px
    #close
        width 22px
        height 22px
        cursor pointer
        position absolute
        top 4px
        right 4px
.disnone
    display none