
.out-side-businesshome {
  background-color: #f4f4f4;
  width: 100vw;
  min-height: 100vh;
  padding-top: .88rem;
  overflow: hidden;
}

.out-side-businesshome .busine-title {
  background-color: #fff;
  font-weight: 900;
  padding: .15rem .2rem;
  line-height: .5rem;
  text-align: left;
}

.out-side-businesshome .open-time {
  background-color: #fff;
  padding: 0 .2rem .2rem;
}

.out-side-businesshome .banner {
  height: 3.75rem;
}

.out-side-businesshome .banner img {
  width: 100vw;
  height: 3.75rem;
}

.out-side-businesshome .addr {
  background-color: #fff;
  overflow: hidden;
  padding: .2rem;
}

.out-side-businesshome .addr .l-side {
  width: 5.15rem;
  overflow: hidden;
  float: left;
}

.out-side-businesshome .addr .l-side span {
  display: block;
}

.out-side-businesshome .addr .border-line-shu-e6e6e6 {
  height: .46rem;
  float: left;
  margin: .1rem 0 0 .15rem;
}

.out-side-businesshome .addr .tel {
  display: inline-block;
  width: 1.7rem;
  height: .56rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu6.png') no-repeat .7rem -3rem / .6rem 12rem;
  margin-top: .02rem
}

.out-side-businesshome .voucher-bar {
  overflow: hidden;
  background-color: #fff;
  padding: .2rem;
  margin-top: .16rem;
}

.out-side-businesshome .voucher-bar .title {
  height: .68rem;
  line-height:.5rem;
}

.out-side-businesshome .voucher-ul {
  overflow: hidden;
  display: block;
}

.out-side-businesshome .voucher-ul li {
  display: block;
  width: 7.1rem;
  height: 2.04rem;
  background: url('https://api.duishangbao.cn/upload/image/juan_jiao3.png') no-repeat right / 10rem 2.04rem;
  float: left;
  margin-bottom: .4rem;
}

.out-side-businesshome .voucher-ul li .l {
  width: 2.6rem;
  height: 2.04rem;
  background: url('https://api.duishangbao.cn/upload/image/juan_jiao1.png') no-repeat 0 0 / 3rem 2.04rem;
  float: left;
}

.out-side-businesshome .voucher-ul li .l .price {
  display: block;
  text-align: center;
  padding: .5rem 0 0 .08rem;
}

.out-side-businesshome .voucher-ul li .l .use-time {
  display: block;
  text-align: center;
  padding-top: .2rem;
}

.out-side-businesshome .voucher-ul li .c {
  display: inline-block;
  width: .5rem;
  height: 2.04rem;
  background: url('https://api.duishangbao.cn/upload/image/juan_jiao2.png') no-repeat 0 0 / .5rem 2.04rem;
  float: left;
}

.out-side-businesshome .voucher-ul li .r {
  height: 2.04rem;
  padding-left: 3.3rem;
  position: relative;
}

.out-side-businesshome .voucher-ul li .r .price {
  padding: .2rem 0 0 0;
  overflow: hidden;
}

.out-side-businesshome .voucher-ul li .r .price span {
  display: inline-block;
  float: left;
  line-height: .5rem;
}

.out-side-businesshome .voucher-ul li .r .price .return {
  line-height: .26rem;
  padding: .05rem .1rem;
  border: .5px solid #fc6c15;
  border-radius: 2px;
  margin: .06rem 0 0 .1rem;
}

.out-side-businesshome .voucher-ul li .r .gold {
  height: .6rem;
  line-height: .36rem;
}

.out-side-businesshome .voucher-ul li .r .buy-btn {
  padding: .15rem .6rem;
  border-radius: 1rem;
  background-color: #e62343;
  margin-top: .1rem
}

.out-side-businesshome .voucher-ul li .r .icon-gold {
  display: inline-block;
  width: 1.1rem;
  height: 1.1rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu7.png') no-repeat -.32rem -3.06rem / 1.5rem 6rem;
  position: absolute;
  top: 0;
  right: 0;
}

.out-side-businesshome .voucher-bar .more {
  text-align: center;
  overflow: hidden;
  padding: .2rem 0 ;
}

.out-side-businesshome .package-bar {
  overflow: hidden;
  padding: .2rem 0 .4rem;
  background-color: #fff;
  margin-top: .16rem;
}

.out-side-businesshome .package-bar .header {
  height: .65rem;
  padding: 0 .2rem;
}

.out-side-businesshome .package-bar .header .more {
  display: inline-block;
  padding-right: .23rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat .5rem -1.98rem/0.7rem 10rem;
  float: right;
  margin-top: .05rem;
}

.out-side-businesshome .package-outside {
  width: 100vw;
  height: 3.52rem;
  overflow: hidden;
}

.out-side-businesshome .package-outside .auto-k {
  width: 100vw;
  height: 3.8rem;
  overflow-x: auto;
}

.out-side-businesshome .package-outside ul {
  display: block;
  white-space: nowrap;
}

.out-side-businesshome .package-outside ul li {
  display: inline-block;
  width: 5.8rem;
  height: 3.5rem;
  margin-left: .2rem;
}

.out-side-businesshome .package-outside ul li .img,
.out-side-businesshome .package-outside ul li .img img {
  width: 5.8rem;
  height: 2.28rem;
  border-top-left-radius: .1rem;
  border-top-right-radius: .1rem;
}

.out-side-businesshome .package-outside ul li .main {
  display: block;
  height: 1.22rem;
  border-left: .5px solid #e6e6e6;
  border-right: .5px solid #e6e6e6;
  border-bottom: .5px solid #e6e6e6;
  border-bottom-left-radius: .1rem;
  border-bottom-right-radius: .1rem;
  position: relative;
}

.out-side-businesshome .package-outside ul li .main .name {
  display: inline-block;
  position: absolute;
  width: 3.55rem;
  left: .15rem;
  top: .2rem;
}

.out-side-businesshome .package-outside ul li .main .price {
  display: inline-block;
  position: absolute;
  width: 2rem;
  text-align: right;
  right: .15rem;
  top: .2rem;
}

.out-side-businesshome .package-outside ul li .main .sale {
  display: inline-block;
  position: absolute;
  width: 3.55rem;
  left: .15rem;
  top: .68rem;
}

.out-side-businesshome .package-outside ul li .main .price-old {
  display: inline-block;
  position: absolute;
  width: 2rem;
  text-align: right;
  right: .15rem;
  top: .68rem;
}