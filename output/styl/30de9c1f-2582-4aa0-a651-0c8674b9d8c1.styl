.p-servicios
	h1
		display none
	ol.breadcrumb
		padding-left 0
		// @media $small-medium
		// 	margin-top 100px
		font-size 10px
		width 100%
		@media $small
			li
				padding-left 1px !important
				padding-right 1px !important
				&:before
					padding-left 1px !important
					padding-right 1px !important
		// 	margin-top 120px
		.active a
			color #d81a33
	&.p-detail
		h1
			@media $small-medium
				margin-top 0px
				font-size 1.2rem !important
			display block !important
			color color2
		@media $small-medium
			article.card-panel
				margin-top 0px !important
		.cotizar
			display none
		.fotos > .row
			display flex
			flex-direction column-reverse
			.train
				width 100%
				// vertical-align top
				height 133px
				// overflow-x auto
				display flex
				justify-content space-between
				// padding-bottom 1px
				> div
					border 1px solid #ccc
					// max-width 19%
					width auto
					max-width 33%
					// margin-left .5%
					// margin-right 
					// display inline-block
					float left
					height 100%
					overflow hidden !important
					// vertical-align middle
					&:hover
						border-color navy
					a
						// overflow hidden !important
						height inherit
						display flex
						align-items center
						justify-content center
						// height auto
						padding 1px
						// margin 0px
						// display block !impotant
						img
							height auto
							max-height 100%
							margin 0 !important
							// width auto
					// a 
					// 	height auto%
					// 	padding 1px
					// 	display block !important
					// 	img
					// 		height 100%
					@media $small-medium
						height 80px
			.foto
				width 100%
				margin auto
				// text-align center
				// float none !important
				// display block !important
				// height auto
		.main
			overflow hidden
			
					

	figure
		object-fit contain
		width 100%
		img.mfp-img
			max-height 100% !important
			// object-fit contain




	&.p-grid
		#menu_left
			.collection-item 
				padding 10px
				a
				.collapsible-header
					font-weight normal
					font-size 15px
					line-height 1.5rem
					min-height: 1.5rem;
					padding: .9em 1em .9em 1em
					// background pink
					&.active,&.active-fixed
						background color3 !important
				.collapsible-body ul li 
					> a
						font-size 13px				
						font-weight normal
						padding: .3em 1em .3em 1em
					&.active 
						position relative
						padding-right 14px
						// > a
						// 	icomoon('e654')
						// 	&:before
						// 		float right
						// 		color red
						// 		font-size 43px
						// 		margin-right -45px
						&:before
							position absolute
							right 8px
							top 0
							content ""
							background-color red
							width 5px
							height 28px
							border-left 2px solid color2
						&:after
							position absolute
							right 0
							top 0
							content ""
							border-left: 8px solid red;
							border-top: 14px solid transparent;
							border-bottom: @border-top;
							
			ul >
				li.active
					> a
						background lighten(grey,45)
						color black			
			@media $small-medium
				display none
			ul.submenu
				padding-left 1em
				li.active
					> a
						background grey
						color white
		.line_grupos
			.collection-item
				border-bottom 2px solid #aaa
				@media $small
					padding-top 2em
					h2
						padding-bottom 1em
					
				
		// .productos_itemm
		.productos_item
			.responsive
				padding-bottom 1px
			.card
				overflow hidden
				margin-bottom .5em
				.card-image
					// height 200px
					overflow hidden
				.card-content h4
					text-align center
					font-size 1.1em
				.card-title
					// display none
					height 60px
					overflow hidden
					text-align center
					@media $small
						font-size .8em
				.card-action
					height 50px
					text-align center
					.subname
						display block
						text-align center
						color color2
						font-weight bold
						&:after
							content "%"
							
		.cabecera
			@media $small-medium
				margin-bottom 0
			h2
				text-transform initial
				font-size 1.7rem
				line-height 110%
				margin 2.1rem 0 1.68rem 0
				@media $small-medium
					// border-bottom 1px solid #0077bd
					font-weight normal
					margin 0
					margin-top 30px
			.cent
				padding-left 45px
				@media $small-medium
					padding-left 0
		.menu_post
			.collection-item
				> a
					font-weight normal
					text-transform uppercase
					font-family Roboto
					color color_white !important
					background color2
					font-size 13px
					// padding 0 0 0 1em !important
					// height 1.5em
					// line-height 2.2em
					// padding 10px 0 10px 1em !important
				a 
					color color_black
					display block
					padding .4em 0 .4em 1em
				&.active
					background #fff
					> a
						// font-weight bold
						background #da251b
						color color_white !important
				> .submenu,> .collapsible-body
					> li
						padding-left 1.5em
						&.active
							a
								// color color1
								font-weight bold
								icomoon('e64f')
								&:before
									color color1
									margin-left -1em
				.collapsible-body
					padding 1rem 0rem !important
		.line_grupos
			background-color initial !important
			margin-top -25px
			@media $medium-small
				.collection-item
					padding-top 0em

		// &.p-2
		// 	.collection-item a
		// 		padding-top .3em !important
		// 		padding-bottom .3em !important

	
	.post.project
		@media $large
			img.responsive-img, video.responsive-video
				margin -1px
				max-width 100.5%			
		.row
			position relative
			// &:before
			// 	content ""
			// 	position absolute
			// 	width 1px
			// 	background-color #0077bd
			// 	left 50%
			// 	top 3em
			// 	height 98%
			// 	@media $small-medium
			// 		width 0
			&.cabecera:before
				width 0
			@media $large
				> div > article
					margin-top 0px
					margin-bottom 0px
		.more
			line-height 21px 
			& + a
				color grey
				&:hover
					color #0077bd
			

	.cabecera
		display flex
		@media $small
			display block
			text-align center
			// margin-bottom -3em
		h1
			font-size 1.7rem
		// 	text-align center
		@media $small
			h1
				font-size 1.7em
				// margin-bottom 0
		.cparts
			flex-grow 1
			flex-shrink 1
			flex-basis auto
			@media $small
				display inline-block
				// background-color red
		.contacts
			display inline-block
			color #0076bb
			padding 1em 0 0 0
			text-align right
			@media $small
				padding-top 0
			span
				font-size 1em
				margin 0.5em
				@media $small
					margin 0.5em 0.5em 0.5em 1em
					// display block
				
				&.whatsapp a
					&:before
						color #333
						margin-right 0.5em
					icomoon('e900')
					// text-align left
				&.phone
					background url(imgs+'phone.jpg') no-repeat 0px 2px
					padding-left 18px
					font-size 1.3em
					@media $small
						font-size .8em
						background-position 0px -2px
						display none
					// text-align left
				&.email
					font-size 1.3em
					background url(imgs+'email.jpg') no-repeat 0px 4px	
					padding-left 27px
				&.cotizar
					background url(imgs+'pencil.jpg') no-repeat 0px 2px
					padding-left 20px
			
	article.card-panel
		// min-height 500px
		// min-height 345px
		min-height 370px
		margin-bottom 20px !important
		@media $small-medium
			min-height 0

	#cotizar
		// p
		// 	display none
		h2
			padding-bottom 0
	.actions
		display none
		position fixed
		bottom 0px
		z-index 2
		width 100%
		// display inline-block
		ul
			margin 0 auto
			padding 0
			display flex
			width 100%
			// margin 0 auto
			text-align center
			li
				flex-grow 1
				flex-shrink 1
				flex-basis auto
				a
					display block
					box-shadow inset 0px 0px 50px 0px #09598a
					text-shadow: 3px 1px 5px #09598a
					height 55px
					line-height @height
					// min-width 300px
					width 100%
					// margin-right 1%
					// padding-left 5px
					display inline-block
					background-color #0d9aee
					color color_white
					border-radius 25px 25px 0 0
					text-align center
					// position relative
					cursor pointer
					font-size 50px
					@media $small
						font-size 30px
					&:hover
						text-decoration none
					&:before
						content ""
						width 44px
						height 38px
						top 10px
						left -7px
						position relative
						// top 20px
						// left 50px
						display inline-block
						// margin-left 10px
						background-repeat no-repeat
						@media $small
							background-size @width @height
					&.email:before
						background-image url(imgs+'proy-email.png')
					&.call:before
						background-image url(imgs+'proy-llamar.png')
	@media $small
		.actions
			display block
		// footer
		// 	visibility hidden
			
	
	// .menu_post
	// 	.collapsible-body
	// 		display block !important
			

	.card-title h2
		padding 1.3em 0 .4em
		font-size 1.7em

	article.card-panel
		// box-shadow none
		.parent-container > li
			@media $large
				min-height 220px
			.card
				transition border-radius .5s, box-shadow .5s, background-color .5s, margin .2s
				box-shadow 0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12)
				background-color darken(white,5)
				border-radius 15px
				&:hover
					box-shadow 0 12px 15px 0 rgba(0,0,0,0.24),0 17px 50px 0 rgba(0,0,0,0.19)
					background-color white
					border-radius 0
					@media $large
						margin -1px
				.card-content
					padding-top 0
					padding-bottom 0
					font-weight bold
					text-transform uppercase
	#cotizar
		.card-title
			h2
				margin-top 0
				margin-bottom 0
				padding-top 0
		form
			.btn
				margin-bottom 1em
			// .input-field
			// 	margin-top .1rem
				
.p-descuentos,.p-importaciones
	&.p-grid
		.breadcrumb
			display none
		h1
			text-align center
			display block
		.cabecera
			margin-bottom 0
