//formularios
#inscripcion,
#f-viaje
	background #5a740b
	border 2px solid #7ca70c
	border-radius 0 100px 0 100px
	box-shadow 0 0 12px #4e6310 inset
	box-sizing border-box
	margin 0 auto
	margin-bottom 30px
	overflow hidden
	padding 20px 
	text-align center
	width 340px
.input
	background white
	border 3px solid #7ca70c
	border-radius 8px
	box-shadow 0 0 6px rgba(#5d7c00,.25)
	font-size 16px
	padding 0 15px
	margin 10px 0
	height 33px 
	outline 0 
	line-height 33px 
	transition .3s
	width 260px
	&:focus	
		border-color amarillo

.submit 
	background #eac805
	border 1px solid #5d7a00
	border-radius 20px 0 20px 0
	box-shadow 0 0 6px rgba(#5d7c00,.25) 
	color #9b7900
	cursor pointer
	font-size 15px 
	font-weight bold 
	display block
	height 40px !important
	line-height 40px 
	margin 0 auto
	margin-top 15px
	text-align center
	text-transform uppercase
	transition .3s
	width 135px 
.submit:hover 
	background #fabe09