fondoPresentacion = white
fuenteGoogle = 'Raleway', sans-serif
fondoMenu = #227FFF
fondoLetraMenu = white
fondoContacto = #2D4454

body,html
	margin 0
	font-size 16px
	font-family fuenteGoogle
	background-color fondoContacto
	overflow-x hidden


.Inicio
	background-image url('../img/fondoPadre.jpg')
	background-repeat no-repeat
	background-size cover
	position relative
	width 100%
	height 100vh;
	text-align center

	&-contenedor
		position relative
		display inline-block
		margin-top 2em
		font-size 2rem
		width 100%

	&-imagen
		background-image url('../img/yo.jpg')
		background-repeat no-repeat
		background-size cover
		width 7em
		height 7em
		display inline-block
		border-radius 50%
		box-shadow 1px 1px 20px black

	&-nombre
		display block
		vertical-align top
		color white
		margin-top 1em
		padding 1.5em 0
		background-color rgba(53, 53, 53, .5)

	&-contactos
		margin-right .5em
		cursor pointer
		&:last-child
			margin-right 0
		&:hover
			opacity .7


.Menu
	background-color fondoMenu;
	-webkit-box-shadow 1px 1px 20px #000
	//border-bottom solid
	//border-bottom-width thin
	//border-bottom-color black
	color white
	//#3288FF
	z-index 10


	&-lista
		margin 0
		padding 0
		text-align center

	&-elemento
		display inline-block
		padding 1.5em 1em

		& a
			text-decoration none
			color fondoLetraMenu

		&:hover a
			border-bottom solid


.titulos
	margin 0
	text-decoration none
	font-weight normal
	background-color rgb(226, 226, 226)
	padding 2em 0 1em 1em
	z-index 0
	text-align left


.Presentacion
	//margin-top margen1
	position relative
	width 100%
	//background-color fondoPresentacion
	background-image url('../img/fondoPadre2.jpg')
	background-size cover
	background-repeat no-repeat
	text-align center

	&-texto
		max-width 200px
		margin 0
		padding 5em 1em
		color white
		display inline-block
		text-align left
		margin-right 15%
		vertical-align top
		font-size 1.1em

	&-tecnologias
		display inline-block
		background-color rgba(255,255,255,.5)
		vertical-align top
		height 300px
		margin-top 2em
		padding 1em 0

	&-backend
		min-width 400px
		margin-top 1em
		padding 1em

		&-ronda1
			text-align center

			position relative
			height 300px
			width 370px
			display inline-block

			&-python
				background-image url('../img/python.png')
				background-size cover
				background-repeat no-repeat
				width 80px
				height 80px
				position absolute
				top 0
				left 0

			&-node
				background-image url('../img/node.png')
				background-size cover
				background-repeat no-repeat
				width 260px
				height 70px
				position absolute
				top 25px
				left 110px

			&-java
				background-image url('../img/java.png')
				background-size cover
				background-repeat no-repeat
				width 50px
				height 65px
				position absolute
				top 120px
				left 40px

			&-django
				background-image url('../img/django.png')
				background-size cover
				background-repeat no-repeat
				width 100px
				height 40px
				position absolute
				top 130px
				left 110px

			&-ruby
				background-image url('../img/ruby.png')
				background-size cover
				background-repeat no-repeat
				width 110px
				height 100px
				position absolute
				top 140px
				left 220px


		&-ronda2
			text-align center

			position relative
			height 300px
			width 370px
			display inline-block

			&-html5
				background-image url('../img/html5.png')
				background-size cover
				background-repeat no-repeat
				width 70px
				height 70px
				position absolute
				top 15px
				left 20px

			&-stylus
				background-image url('../img/stylus.png')
				background-size cover
				background-repeat no-repeat
				width 110px
				height 105px
				position absolute
				top 0px
				left 100px

			&-css3
				background-image url('../img/css3.png')
				background-size cover
				background-repeat no-repeat
				width 80px
				height 110px
				position absolute
				top 0px
				left 250px

			&-javascript
				background-image url('../img/javascript.png')
				background-size cover
				background-repeat no-repeat
				width 90px
				height 120px
				position absolute
				top 110px
				left 0px

			&-jquery
				background-image url('../img/jquery.png')
				background-size cover
				background-repeat no-repeat
				width 130px
				height 30px
				position absolute
				top 140px
				left 240px

			&-bootstrap
				background-image url('../img/bootstrap.png')
				background-size cover
				background-repeat no-repeat
				width 80px
				height 80px
				position absolute
				top 120px
				left 150px

	/*&-imagen
		width 100%
		height 20%
		//margin-bottom 4em
			*/


.Trabajo
	width 100%
	position relative

	&-elemento
		border-bottom solid
		border-bottom-color fondoMenu
		border-bottom-width thin
		cursor pointer
		position relative
		background-color white
		-webkit-transition -webkit-transform .1s ease-out
		-moz-transition -moz-transform .1s ease-out
		-o-transition -o-transform .1s ease-out
		-ms-transition -ms-transform .1s ease-out
		transition transform .1s ease-out

	&-elemental
		border-bottom solid
		border-bottom-color fondoMenu
		border-bottom-width thin
		cursor pointer
		position relative
		background-color white
		-webkit-transition -webkit-transform .1s ease-out
		-moz-transition -moz-transform .1s ease-out
		-o-transition -o-transform .1s ease-out
		-ms-transition -ms-transform .1s ease-out
		transition transform .1s ease-out

	&-contentImg
		position relative
		padding 1em 0 1em 1em

	&-imagen
		width 20%

	&-nombre
		display inline-block
		vertical-align top
		margin-top 1em
		margin-left 1em
		font-size 1.25em
		font-style italic

	&-descripcion
		display inline-block
		padding 1em
		margin 0
		max-width 30em


.Contacto
	background-color fondoContacto
	color white

	&-caja
		max-width 100%
		min-width 100%

	&-titulo
		margin 0
		font-weight normal
		padding 2em 0 1em 1em

	&-noDudes
		padding 1em
		margin 0
		text-align left
		width 80%
		display inline-block

	&-contenedor1,
	&-contenedor2
		text-align center

	&-formulario
		text-align left
		display inline-block
		width 80%

	&-nombre,
	&-email,
	&-texto,
	&-enviar
		border-radius 4px
		display inline-block
		border none
		margin-bottom .5em

	&-nombre,
	&-email,
	&-texto
		padding 1em 2%
		width 96%

	&-texto
		max-width 96%
		min-width 96%

	&-enviar
		padding .5em
		width 100%
		background-color #03A400
		color white
		font-size 1.5em
		cursor pointer

		&:hover
			//opacity .7
			background-color #027800
			color #DFDFDF

	&-ademas,
	&-red
		width 80%
		display inline-block
		text-align left

	&-red
		margin 0 0 .5em 0

		&:last-child
			margin-bottom 2em

	&-logoRed
		margin-right .3em



.invisible
	display none

#Strongfit: hover ~ #Strongfit1
	display none

.grande
	border none
	webkit-box-shadow 1px 1px 1px rgba(0,0,0,.2)
	box-shadow 0px 0px 20px rgba(0,0,0,.4)
	z-index 1
	-moz-transform scale(1.1)
	-webkit-transform scale(1.1)
	-o-transform scale(1.1)
	-ms-transform scale(1.1)
	transform scale(1.1)


.revelarTrabajo
	display flex
	justify-content center
	flex-direction column

.flotar
	position fixed
	top 0
	left 0
	right 0

.agrandar
	width 90%

.nuevoContent
	width 20%

.nuevoImg
	width 100%

.flexible
	display flex
	justify-content space-between
	margin 0

//=============media querys=======================
@media (min-width: 750px)
	.Contacto

		&-caja
			display flex

		&-texto
			height 11em

		&-enviar
			margin-bottom 5em

	.Trabajo
		background-color #8D9890

		&-contenedor
			text-align center

		&-elemento,
		&-elemental
			display inline-block
			width 15em
			height 15em
			vertical-align top
			border none
			margin 1em 1em 1em 0
			border-radius 50%

		&-elemental
			margin 0

		&-click
			background-color rgba(158, 158, 158, .7)
			color white
			border-radius 50%
			height 100%
			width 100%
			position absolute
			z-index 2
			font-size 1.2em

			& p
				margin 0

		&-descripcion
			width 25%;


		&-contentImg
			height 100%
			padding 0

		&-imagen
			position absolute
			width 70%
			height 70%
			right 0
			bottom 0
			border-radius 0 50% 50% 50%

		&-nombre
			display block
			margin 0
			padding 1.5em

@media (max-width:400px)
	.Menu
		&-elemento
			padding 1.5em .2em


@media (max-height:300px)
	.Inicio
		&-imagen
			width 3em
			height 3em
			position absolute
			top 1.5em
			left 1em

		&-nombre
			margin-top 0

			& p
				margin .3em 0
				padding-left 3em

		&-contenedor
			margin-top .7em
			font-size 1.5em


@media (min-height:300px) and (max-height:330px)
	.Inicio
		&-imagen
			width 4em
			height 4em
			top 2.3em
			left 3em

		&-contenedor
			margin-top 1em
			font-size 1.5em

		&-nombre
			& p
				margin .3em 0


@media (min-height:330px) and (max-height:450px)
	.Inicio
		&-imagen
			width 5em
			height 5em
			top 2.3em
			left 3em


		&-contenedor
			margin-top 1em
			font-size 1.5em

		&-nombre
			& p
				margin .3em 0

@media (min-height:450px) and (max-height:561px)
	.Inicio
		&-imagen
			width 7em
			height 7em
			top 2.3em
			left 3em


		&-contenedor
			margin-top 1em
			font-size 1.5em

		&-nombre
			& p
				margin .5em 0


@media (max-width: 788px)
	.Presentacion
		&-texto
			max-width none
			margin-right 0

		&-tecnologias
			display block
			margin-top 0
