.Cart
    width 100%
    height 100%
    background #eeeeee
    .head
        text-align center
        color #ffffff
        font-size 18px
        line-height 50px
        font-weight bold
    .empty-container
        text-align: center;
        position: absolute;
        left: 0;
        right: 0;
        top: 50%;
        -webkit-transform: translateY(-50%);
        transform: translateY(-50%);
        .img-container
            width: 88px;
            height: 114px;
            // background: url('https://ss0.bdstatic.com/70cFuHSh_Q1YnxGkpoWK1HF6hhy/it/u=1955250436,3083941858&fm=26&gp=0.jpg') no-repeat top;
            background-size: 88px auto;
            margin: 0 auto;
            img 
                width 100%
                height 100%
                background-repeat no-repeat
        h3  
            color: rgba(0,0,0,.15);
            margin: 25px 0 9px;
            font-size: 1.5rem;
        p   
            font-size: 1rem;
            text-align: center;
            color: rgba(0,0,0,.15);
        .login
            box-shadow: inset 0 -1px 1px rgba(0,0,0,.2), 0 0 1px rgba(0,0,0,.1), 0 1px 3px rgba(0,0,0,.2);
            background: #5e8bdd;
            background: linear-gradient(#6f94e8,#5178df);
            border-radius: 6px;
            text-shadow: 0 -1px 0 rgba(0,0,0,.15);
            color: #fff;
            display: inline-block;
            width: 142px;
            height: 40px;
            line-height: 40px;
            font-size: .6rem;
            font-weight: 700;
            // color: rgba(0,0,0,.6);
            margin: 16px 1px 0;
        .choose
            display: inline-block;
            width: 142px;
            height: 40px;
            line-height: 40px;
            font-size: .6rem;
            font-weight: 700;
            color: rgba(0,0,0,.6);
            margin: 16px 1px 0;
            // background: linear-gradient(rgba(0,0,0,.01),hsla(0,0%,100%,.01));
            box-shadow: inset 0 2px 4px hsla(0,0%,100%,.3);
            background-color #eee
            border-radius: 6px
    .cart-item
        display: flex;
        height: 114px;
        align-items: center
        
        margin-bottom 10px
        background-color #fff
        .chexbox
            width: 22px;
            height 22px
            text-align: center;
            line-height: 0;
            background-color #ededed
            margin 0 10px
            border-radius 50%
        .item
            display: flex;
            flex: 1;
            padding: 0 15px 0 15px;
            overflow: hidden;
            align-items: center;
            .item-img
                width: 90px;
                height: 90px;
                flex: 0 0 90px;
                position: relative;
                img 
                    width 100%
                    height 100%
        .item-info
            flex: 1;
            margin-left: 13px;
            padding: 0 5px 0 0;
            position: relative;
            .title
                font-size: .6rem;
                color: #000;
            .attr
                font-size: .5rem;
                color: #7f7f7f;
                overflow: hidden;
                margin: 7px 0;
            .price
                display: flex;
                align-items: center;
                justify-content: space-between;
                width 150px
                .newPrice
                    display flex
                    color: #d44d44;
                    font-size: .6rem;
                    font-weight: 700;
                    .newPrice-price
                        color: #d44d44;
                        font-size: .6rem;
                        font-weight: 700;
                    .newPrice-num
                        color: #999;
                        position: relative;
                        padding-left: 14px;
                        display: inline-block;
                        margin-left: 6px;
                .oldPrice
                    font-size: .6rem;
                    color: #7f7f7f;
                    position: relative;
                    margin-left: 20px;
                    text-decoration line-through
    .cash
        bottom: 55px;
        position: fixed;
        height: 54px;
        padding: 0 12px;
        left: 0;
        right: 0;
        z-index: 90;
        background: #fafafa;
        box-shadow: 0 -0.2rem 1rem rgba(0,0,0,.1);
        font-size: 12px;
        color: #666;
        display: flex;
        align-items: center;
        justify-content: space-between;
        box-sizing: border-box;
        .select
            height: fit-content;
            display: flex;
            align-items: center;
            .chexbox
                width: 22px;
                height 22px
                text-align: center;
                line-height: 0;
                background-color #ededed
                margin 0 10px
                border-radius 50%
            .chexbuy
                color: #999;
                margin-left: 8px;
                font-size 14px
        .sum-info
            margin-right: 120px;
            .desc
                font-size: 12px;
                color: #666;
                
                .p
                    display flex
                    margin-top 12px
                    .total
                        color: #d44d44;
                        font-size: 12px;
                .desc
                    margin-top -12px
                    
            .btn
                position: absolute;
                width: 110px;
                right: 11px;
                top: 50%;
                font-size: 14px
                -webkit-transform: translateY(-50%);
                transform: translateY(-50%);
                font-weight: 700;
                height: 40px        
                line-height: 40px
                border-radius: 6px;
                text-align: center;
                text-shadow: 0 -1px 0 rgba(0,0,0,.15);
                color: #fff;
                // background-color red

