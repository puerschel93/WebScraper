*
	margin: 0
	padding: 0
	outline: 0
	box-sizing border-box
a
	&:hover
	&:focus
		text-decoration: none
		outline: 0
		color: initial
iframe 
	border: 0
section
	padding: 100px 0

h1
	font-size: 30px;
	font-family: 'OswaldMedium', arial;
	color: #fff
	line-height: 1.2
	text-shadow: 0px 1px 30.96px rgba(0, 0, 0, 0.75);
	width auto
	position relative
	z-index: 9
	margin: 20px 0
h3
	font-size: 40px;
	font-family: 'OswaldMedium', arial;
	color: #fff
	line-height: 1
	text-align: center;
	letter-spacing: 1.1px;
	&:after
		content: ''
		display: block
		width: 68px
		height: 4px
		margin: 15px auto 70px
		background-color: #288ba0
#table_section .flip-clock-divider .flip-clock-label
	font-size: 18px;
a.button
	font-family: 'OswaldMedium', arial;
	min-width: 300px
	width: 0
	margin: 0 auto
	display block
	padding-top: 5px
	margin: 70px auto 0
	&:hover
	&:focus
		color: #fff
.video_wrapper + a,
.author_wrapper a
	pointer-events: none
#feedback
	.col-md-6
		margin-bottom: 25px
		&:last-child
			background-color rgba(#fff,.9)
@import 'parts/first_screen'
@import 'parts/mail_section'
@import 'parts/risons'
@import 'parts/plan'
@import 'parts/you_reach'
@import 'parts/author'
@import 'parts/table_section'
@import 'parts/videos'
@import 'parts/testimonials'
@import 'parts/footer'
@import 'parts/hw'
/* Medias */
@media screen and (max-width: 1199px)
	.contacts .fa
		margin 0 5px 0 40px
	.aut_n_date p
		float: none
		&first-child
			margin-right: 0
	#lidia_main
		right 0
		bottom: 0
	.plan_wrapper
	#moon
		width: 100%
	.author_wrapper img
		bottom: -18px;
		margin-bottom: 18px;
	#licenses
		a>img
			width: 90%
			margin: 0 auto;
			display: block;
	#you_reach .item
		width: 800px
@media screen and (max-width: 991px)
	#logo
		display block
		margin: 0 auto 10px
		float: none
	.contacts
		float: none
		text-align: center
		a:nth-child(2)
			margin: 0 10px
		.fa
			margin: 0 5px;
	#lidia_main
		right 40px
	.video_wrapper
		margin: 35px 0
		width: 50%
	#mail_section .form_newsletter
		width: 60%
		margin: 0 auto
	#mail_section .col-md-3
		display block
		height: auto
	#mail_section .col-md-3 h3
		text-align: center
	#mail_section .col-md-3 p
		text-align: center
		margin: 20px 0
	#mail_section .col-md-3 .infront_submit, #mail_section .col-md-3 .span_phone
		font-size: 18px;
		text-align: center;
		margin: 20px 0;
	.email-block input, .phone-block input
		margin-bottom: 20px
	.plan_wrapper
		height auto
		padding: 20px
	.plan_caption
		padding-left: 10px
	.author_wrapper
		margin-bottom: 20px
		img
			bottom: 0;
			margin-bottom: 0;
			width: 40%;
			left: 25px;
	.education .lovely_dir li
		padding-left: 5px;
	#licenses
		.col-md-4
			width: 50%
			float left
			.col-md-6
				width: 50%
				float left
	#video_slider
		.video_wrapper
			width: 70%
	#hw .video_wrapper
		width: 80%
	#you_reach .item
		width: 100%
	#table_section .flip-clock-wrapper
		width: 100%
	#pay_table .buttons td .button
		font-size: 18px
	.aut_n_date p:first-child
		margin: 0
	#videos .days .video_wrapper
		width: 500px
@media screen and (max-width: 768px)
	#risons_slider .crsl-item .item
		height: 340px
	.plan_wrapper
		display block
	.img_wrapper
		display block		
		margin: 0 auto
	.plan_caption h4
		text-align: center
	.lovely_dir
		display block
		margin-left: 40px
		li
			margin-bottom: 5px
	#author h5
		margin-bottom: 5px
	#video_slider .slidernav
		width: 580px
	#pay_table
		display: none
	#mob_pay_table
		display block
	#course_first_screen_section .aut_n_date, #rec_first_screen_section .aut_n_date
		width: 100%
		text-align: center
	#course_first_screen_section .aut_n_date p, #rec_first_screen_section .aut_n_date p
		display: block

@media screen and (max-width: 660px)
	.contacts a
		display block
	#first_screen_section .container
		text-align: center
	h1
		width: 100%
	#bonus .aut_n_date p span
		display block
	.aut_n_date p:first-child
		margin-right: 0
	#lidia_main
		display: none
	.video_wrapper
		margin 35px auto
		width: 100%
@media screen and (max-width: 629px)
	#risons_slider .crsl-item .item
		height: 240px
	#video_slider .slidernav
	#mob_pay_table
		width: 100%
@media screen and (max-width: 560px)
	#risons_slider .crsl-item .item
		height: 300px
	#hw .aut_n_date
		width: 100%
		text-align: center
		span
			display: block
	#videos .days .video_wrapper
	.hw_pic img
		width: 100%
@media screen and (max-width: 480px)
	.contacts .fa + span
		font-size: 15px
		line-height: 15px
	#first_screen_section
		padding: 0 0 20px
	#pre
		font-size: 24px
	h1
		font-size: 28px
		line-height: 1.1
	.aut_n_date p
		font-size: 20px
	.video_wrapper
		margin-bottom: 0
	#mail_section .form_newsletter
		width: 100%
	#you_reach h3:after
		margin-bottom: 60px
	section
		padding: 40px 0
	h3
		font-size: 32px
		&:after
			margin-bottom: 40px
	a.button
		margin-top: 40px
	#risons_slider .crsl-item .item h5
		font-size: 19px
	.img_wrapper img
		display block
		width: 60%
		margin: 0 auto
	#licenses .col-md-4
		width: 100%
		float: none
		margin-bottom: 20px
	#video_testimonials
		margin-bottom: 70px
	#video_slider .slidernav
		width: 162px
	#v-testm-btns
		position: static
	#video_slider .video_wrapper
		width: 100%
	#text_slider .crsl-item p
		height: 300px
		padding: 0 10px
		overflow-y: scroll
		overflow-x: hidden
	#videos .days h4
		font-size: 26px
	#you_reach
	#videos
		background-attachment: scroll
@media screen and (max-width: 420px)
	.author_wrapper img
		width: 100%
		position: static
		margin-bottom: 25px
	.lovely_dir
		margin-left: 20px;
	h1
		margin-bottom: 40px;
	#risons_slider .crsl-item .item
		height: 330px
	#table_section .flip-clock-divider
		width: auto
	#table_section .flip-clock-divider .flip-clock-label
	#table_section .flip-clock-divider.minutes .flip-clock-label
	#table_section .flip-clock-divider.seconds .flip-clock-label
		left 0
		right 0
		margin: 0 auto
		width: 100px
	#table_section .flip-clock-wrapper ul
		height: 65px
	#table_section .flip-clock-wrapper ul li
		line-height: 66px
	#table_section .flip-clock-wrapper ul li a div.up::after
		top 31px
	#table_section .flip-clock-wrapper ul li a div div.inn
		font-size: 50px
@media screen and (max-width: 360px)
	#header
		padding-bottom: 0
	h1
		font-size: 26px
	#hw #first_screen_section h2
		font-size: 22px
	#mail_section .col-md-3 h3
	#feedback h3
		font-size: 24px
	#risons_slider .crsl-item .item
		height 365px
	.img_wrapper
		width: auto
	a.button
		min-width: 100%
	.author_caption
		padding: 15px 15px;
	#author h5
		line-height: 1
		margin 15px 0
	.plan_caption h4
		line-height: 1.1
	.alert-danger
		font-size: 28px
