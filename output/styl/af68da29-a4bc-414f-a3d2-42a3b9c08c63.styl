.myBank_view
  .part1
    width 100%
    position relative
    padding-right 15px
    padding-left 15px
    box-sizing border-box
    .bankContainer
      width 100%
      height 145px
      margin-top 20px
      position relative
      box-sizing border-box
      .bank_item
        width 100%
        height 100%
        background-size 20% 20%
        background #ccc
        border-radius 8px
        .bank_title
          padding 20px 50px
          box-sizing border-box
          width 100%
          height 85px
          position relative
          .icon_p
            position absolute
            width 49px
            height 49px
            top 20px
            left 20px
            background-color #FFF
            border-radius 50%
            text-align center
            box-sizing border-box
            padding 2px 1.2px 1px 1px
            vertical-align middle
            i
              display inline-block
              width 45px
              height 45px
              vertical-align middle
          .bank_name
            float left
            font-family PingFang-SC-Bold
            font-size 22px
            height 45px
            line-height 45px
            color #FFFFFF
            padding-left 25px
          .bank_type
            float right
            font-family PingFang-SC-Bold
            font-size 16px
            color #FFFFFF
            height 45px
            line-height 45px
        .bank_num
          width 100%
          padding 5px 30px 20px
          box-sizing border-box
          height 60px
          span
            display inline-block
            width 22%
            font-family PingFang-SC-Bold
            font-size 22px
            color #FFFFFF
            height 30px
            line-height 30px
            text-align center
      .bank_remove
        position absolute
        right 0
        top 0
        height 100%
        width 30px
        border-top-right-radius 8px
        border-bottom-right-radius 8px
        background-color #C1BFC2
        p
          height 100%
          text-align center
          position relative
          b
            position absolute
            font-size 15px
            width 20px
            height 20px
            text-align center
            line-height 20px
            background-color #FFF
            top 50%
            left 50%
            border-radius 50%
            transform translate(-50%, -50%)
      /* 绑定银行卡的颜色 */
      .bank_item.ICBC // 工商
        background -moz-linear-gradient(-3deg, #FF0404 0%, #FF3F4E 100%)
        background -webkit-linear-gradient(-3deg, #FF0404 0%, #FF3F4E 100%)
        background -ms-linear-gradient(-3deg, #FF0404 0%, #FF3F4E 100%)
        background linear-gradient(-3deg, #FF0404 0%, #FF3F4E 100%)
      .bank_item.ABC // 农业银行
        background -moz-linear-gradient(0deg, #019779 0%, #449780 100%)
        background -webkit-linear-gradient(0deg, #019779 0%, #449780 100%)
        background -ms-linear-gradient(0deg, #019779 0%, #449780 100%)
        background linear-gradient(0deg, #019779 0%, #449780 100%)
      .bank_item.BOC // 中国银行
        background -moz-linear-gradient(0deg, #D9241B 0%, #d95249 100%)
        background -webkit-linear-gradient(0deg, #D9241B 0%, #d95249 100%)
        background -ms-linear-gradient(0deg, #D9241B 0%, #d95249 100%)
        background linear-gradient(0deg, #D9241B 0%, #d95249 100%)
      .bank_item.CCB // 中国建设银行
        background -moz-linear-gradient(0deg, #0E3B96 0%, #153496 100%)
        background -webkit-linear-gradient(0deg, #0E3B96 0%, #153496 100%)
        background -ms-linear-gradient(0deg, #0E3B96 0%, #153496 100%)
        background linear-gradient(0deg, #0E3B96 0%, #153496 100%)
      .bank_item.CMB // 招商银行
        background -moz-linear-gradient(0deg, #CA0304 0%, #CA2728 100%)
        background -webkit-linear-gradient(0deg, #CA0304 0%, #CA2728 100%)
        background -ms-linear-gradient(0deg, #CA0304 0%, #CA2728 100%)
        background linear-gradient(0deg, #CA0304 0%, #CA2728 100%)
