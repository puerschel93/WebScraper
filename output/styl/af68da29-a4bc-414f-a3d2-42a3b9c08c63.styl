      .bank_item.SPABANK // 平安银行
        background -moz-linear-gradient(0deg, #D8681F 0%, #D87E37 100%)
        background -webkit-linear-gradient(0deg, #D8681F 0%, #D87E37 100%)
        background -ms-linear-gradient(0deg, #D8681F 0%, #D87E37 100%)
        background linear-gradient(0deg, #D8681F 0%, #D87E37 100%)
      .bank_item.CITIC // 中信银行
        background -moz-linear-gradient(0deg, #E00723 0%, #E04053 100%)
        background -webkit-linear-gradient(0deg, #E00723 0%, #E04053 100%)
        background -ms-linear-gradient(0deg, #E00723 0%, #E04053 100%)
        background linear-gradient(0deg, #E00723 0%, #E04053 100%)
      .bank_item.HXB // 华夏银行
        background -moz-linear-gradient(0deg, #D70C17 0%, #D7353F 100%)
        background -webkit-linear-gradient(0deg, #D70C17 0%, #D7353F 100%)
        background -ms-linear-gradient(0deg, #D70C17 0%, #D7353F 100%)
        background linear-gradient(0deg, #D70C17 0%, #D7353F 100%)
      .bank_item.SPDB // 广东发展银行
        background -moz-linear-gradient(0deg, #063C7F 2%, #264E7F 98%)
        background -webkit-linear-gradient(0deg, #063C7F 2%, #264E7F 98%)
        background -ms-linear-gradient(0deg, #063C7F 2%, #264E7F 98%)
        background linear-gradient(0deg, #063C7F 2%, #264E7F 98%)
