        background -webkit-linear-gradient(0deg, #183883 0%, #243983 100%)
        background -ms-linear-gradient(0deg, #183883 0%, #243983 100%)
        background linear-gradient(0deg, #183883 0%, #243983 100%)
      .bank_item.CEB // 中国光大银行
        background -moz-linear-gradient(0deg, #E4A509 0%, #E4B135 100%)
        background -webkit-linear-gradient(0deg, #E4A509 0%, #E4B135 100%)
        background -ms-linear-gradient(0deg, #E4A509 0%, #E4B135 100%)
        background linear-gradient(0deg, #E4A509 0%, #E4B135 100%)
      .bank_item.CMBC // 中国民生银行
        background -moz-linear-gradient(0deg, #0B9C57 0%, #0cda78 100%)
        background -webkit-linear-gradient(0deg, #0B9C57 0%, #0cda78 100%)
