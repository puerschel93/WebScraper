html
  width: 100%
  height: 100%
  -webkit-text-size-adjust: 100%
  -ms-text-size-adjust: 100%
body
  margin: 0
  width: 100%
  height: 100%
  font-size: 12px
  line-height: 1
  color: #333
  font-family: Helvetica,Arial,PingFangSC-Light,"Hiragina Sans GB","WenQuanYi Micro Hei",'microsoft yahei ui','microsoft yahei',sans-serif
  -webkit-tap-highlight-color: rgba(0,0,0,0)
  /*-webkit-font-smoothing: antialiased*/
  -moz-osx-font-smoothing: grayscale
*
  -webkit-box-sizing: border-box
  -moz-box-sizing: border-box
  box-sizing: border-box
ul,ol,h1,h2,h3,h4,h5,h6,p,dl
  margin: 0
  padding: 0
ul,ol
  list-style: none
dd
  margin-left: 0
a
  text-decoration: none
  color: #333
textarea
  vertical-align: top
  overflow: auto
  background-image: none
.form-control
  display: block
  width: 100%
  height: 30px
  padding: 6px 12px
  font-size: 12px
  line-height: 1.428571429
  color: #555
  vertical-align: middle
  border: 1px solid #ccc
  border-radius: 2px
  -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,0.075)
  box-shadow: inset 0 1px 1px rgba(0,0,0,0.075)
  -webkit-transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s
  transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s
  background-color: #fff
  &:focus
    border-color: #00c1e0
    outline: 0
    box-shadow: inset 0 1px 1px rgba(0,193,224,.075)
textarea.form-control
  height: 80px
.clearfix
  &::after
    display: table
    content: ''
    clear: both
.btn-default, .btn-primary
  display: inline-block
  margin-bottom: 0
  padding: 0 12px
  height: 30px
  border: 1px solid transparent
  border-radius: 2px
  font-size: 12px
  font-weight: normal
  line-height: 30px
  text-align: center
  white-space: nowrap
  vertical-align: middle
  cursor: pointer
  -webkit-user-select: none
  -moz-user-select: none
  -ms-user-select: none
  -o-user-select: none
  user-select: none
.btn-default
  border-color: #ddd
  color: #333
  background: #fff
.btn-primary
  border-color: #2babbc
  color: #fff
  background: #00c1e0
  &[disabled]
    border-color: #46b8da
    background: #5bc0de
    cursor: not-allowed
    opacity 0.7
  &:hover
    border-color: #269abc
    background: #2babbc
.dialog-body-wrapper
  padding: 10px 20px
.dialog-btn-wrapper
  padding: 10px 0
  text-align: center
  background: #e1eef0
  button
    display: inline-block
    margin-right: 10px
.title
  padding-left: 15px
  height: 50px
  font-size: 14px
  line-height: 50px
  border-bottom: 1px solid #e6e6e6
.alert-info
  margin: 15px 0;
  padding: 0 10px
  border: 1px solid #00c1e0
  border-radius: 3px
  line-height: 30px
  font-size: 12px
  color: #333
  background: #b2ecf6
.content-menu
  padding: 15px
.form-group
  margin-bottom: 15px
  &::after
    clear both
    content ''
    display block
  .form-label
    float: left
    display: block
    height: 30px
    line-height: 30px
  .form-statics-text
    height: 30px
    line-height: 30px
  .form-error
    .error
      line-height: 28px
      color: #a94442
 .ng-dirty.ng-invalid:not(form)
    border: 1px solid #a94442
.red
