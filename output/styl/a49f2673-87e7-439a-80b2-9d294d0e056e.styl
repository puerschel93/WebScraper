/* 重置日期区间组件 */
.daterangepicker .drp-buttons .btn
  padding: 0

/*通用组*/
.btn-group
  &:after
    display block
    content ''
    clear both
  button
    width: 95px;
    height: 30px;
    color: #333;
    background: none;
    border: 1px solid #ccc;
    outline none
    margin-right 0
    border-right 0
    margin-left -1px
    position relative
    cursor pointer
    font-size 12px
    float left
    border-radius 0
    &:first-child
      margin-left 0
    &:last-child
      border-right 1px solid #ccc
    &.active
      border: 1px solid #00c1e0;
      background: rgba(188, 237, 245, .38);
      color: #00c1e0;
      outline none
      z-index 2
      transition all ease-in-out .3s
    &:hover
      background #eee
button:focus
  outline none
.cursor-pointer
  cursor: pointer
.icon-chart
  font-weight: bold
/*tool-tip提示框*/
.tooltip
    position: absolute;
    left: 0;
    z-index: 1070;
    padding: 5px 0;
    /*transition: all ease-in-out .3s;*/
    opacity: 0.9;
    display: none;
    .tooltip-arrow
        position: absolute;
        width: 0;
        height: 0;
        border-color: transparent;
        border-style: solid;
        bottom: 0;
        left: 50%;
        margin-left: -5px;
        border-width: 5px 5px 0;
        border-top-color: #000;
    .tooltip-inner
        padding: 0 8px;
        color: #fff;
        text-align: center;
        background-color: #000;
        border-radius: 4px;
        font-size: 12px;
        white-space: nowrap;
        line-height 30px
    &.left
      bottom: 0;
      left: inherit;
      padding 0
      height 30px
      .tooltip-arrow
        top: 50%;
        right: 0;
        margin-top: -5px;
        border-width: 5px 0 5px 5px;
        border-left-color: #000;
        border-top-color: transparent;
        left: 100%;
        margin-left 0
    &.right
      bottom: 0;
      right: inherit;
      padding 0
      height 30px
      .tooltip-arrow
        top: 50%;
        left: 0;
        margin-top: -5px;
        margin-left: -5px;
        border-width: 5px 5px 5px 0;
        border-right-color: #000;
        border-top-color: transparent;
        right: 100%;
