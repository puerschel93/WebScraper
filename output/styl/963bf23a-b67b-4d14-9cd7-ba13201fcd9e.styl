h()
  columns = 0 1 2 3 4
  for col in columns
    .h-{col}
      width (col/columns[-1]*100) + '%'
      flex-basis auto
      box-sizing border-box
      display block
      padding-left 5px
      padding-right 5px
  for col in columns
    .h-offset-{col}
      margin-left: (col/columns[-1]*100) + '%'

t()
  columns = 0 1 2 3 4
  for col in columns
    .t-{col}
      width (col/columns[-1]*100) + '%'
      flex-basis auto
      box-sizing border-box
      display block
      padding-left 11px
      padding-right 11px
  for col in columns
    .t-offset-{col}
      margin-left: (col/columns[-1]*100) + '%'

d()
  columns = 0 1 2 3 4
  for col in columns
    .d-{col}
      width (col/columns[-1]*100) + '%'
      flex-basis auto
      box-sizing border-box
      display block
      padding-left 20px
      padding-right 20px
  for col in columns
    .d-offset-{col}
      margin-left: (col/columns[-1]*100) + '%'

d-screen = 1248px
t-screen = 768px
h-screen = 312px

.cnt
  box-sizing border-box
  padding 0 15px
  width: 100%
  @media(min-width: t-screen)
    padding 0 39px
    width 100%
  @media(min-width: d-screen)
    padding 0 84px
    margin 0 auto

.row
  flex-basis 100%
  margin 0 auto
  flex-grow 1
  display flex
  display -webkit-flex
  flex-flow column
  justify-content flex-start
  align-content flex-start
  > *
    box-sizing border-box
  @media(min-width 768px)
    max-width 680px
    flex-flow row wrap
  @media(min-width 1280px)
    max-width 1112px
    flex-flow row wrap

.button-blue
  width 280px
  height 62px
  border-radius 100px
  background-color #4ab9ff
  background-image linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.16) 100%)
  color #ffffff
  font-family "DINPro"
  font-size 14px
  font-weight 500
  line-height 18px
  border none
  transition all 0.3s ease-out
  outline none
  cursor pointer
  z-index 5
  &:hover
    background-color #4ab9ff
    background-image linear-gradient(to top, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.16) 100%)

  &:active
    border-radius 100px
    background-color #4ab9ff
    background-image linear-gradient(180deg, rgba(0, 0, 0, 0.16) 0%, rgba(0, 0, 0, 0.16) 100%)

.button-white
  width 240px
  height 62px
  border-radius 100px
  background-color transparent
  border 2px solid #4ab9ff
  color #2d2d2d
  font-family "DINPro"
  font-size 14px
  font-weight 500
  line-height 18px
  transition all 0.3s ease-out
  outline none
  text-align center
  margin 0 auto
  cursor pointer
  &:hover
    background-color #4ab9ff
    background-image linear-gradient(180deg, rgba(0, 0, 0, 0) 0%, rgba(0, 0, 0, 0.16) 100%)
    color #FFFFFF

  &:active
    background-color #4ab9ff
    background-image linear-gradient(180deg, rgba(0, 0, 0, 0.16) 0%, rgba(0, 0, 0, 0.16) 100%)


.markers
  width 150px
  height auto
  display flex
  flex-direction row
  justify-content center
  align-items center

  li
    width 10px
    height 10px
    border 2px solid #4ab9ff
    border-radius 100%
    background-color transparent
    margin 0 8px
    &:hover
      background-color #4ab9ff
    &:active, &.slick-active
      border 2px solid #3e9ad5
      background-color #3d9ad5
    button
      display none


.marker-active
  width 14px
  height 14px
  border 2px solid #4ab9ff
  background-color #4ab9ff

.services-min
