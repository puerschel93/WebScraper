.activity_tate{
    display: flex;
    // flex-direction: column;
    // overflow: hidden;
    position: absolute;
    width: 24%;
    height: 95%;
    margin-left: 5%;
    box-sizing: border-box;
    align-items: flex-end;
    .contontBox{
        display: flex;
        flex-direction: column;
        width: 100%;
        height: 76%;
        .rateBox{
            width: 100%;
            display: flex;
            flex: 1;
            .ratestyleBox{
                display: flex;
                flex: 1;
                align-items: center;
                width: 100%;
                height: 100%;
                position: relative;
                .textSize{
                    position: absolute;
                    top: 50%;
                    left: 50%;
                    line-height: 28px;
                    color: #FFAD81;
                    font-size: 23px;
                    transform: translate(-50%, -50%);
                }
                .ratestyle{
                    display: flex;
                    width: 100%;
                    height: 100%;
                }
            }
            .font_rate{
                color:#fff;
                display: flex;
                flex: 1;
                align-items: flex-end;
                ul{
                    display: flex;
                    flex-direction: column;
                    height: 80%;
                    li{
                        display: flex;
                        height: 20%;
                        font-size: 24px;
                        align-items: center;
                        span{
                            display: flex;
                            white-space: nowrap;
                            align-items: center;
                            margin-left: 10px;
                        }
                        span:first-child{
                            width: 15px;
                            height: 15px;
                            background: #fff;
                        }
                    }
                }
            }
            
        }
    }
    
}
.user_development{
    position: absolute;
    margin-left:31.8%;
    top: 0;
    width: 36%;
    height 95%;
    display: flex;
    box-sizing: border-box;
    align-items: flex-end;
    // overflow: hidden;
    .userTrendBox{
        display: flex;
        flex-direction: column;
        width: 100%;
        height: 84%;
        .userTrend{
            display: flex;
            width: 100%;
            flex: 1.1;
            flex-direction: column;
            .userTitle{
                display: flex;
                flex-basis: 11%;
                align-items: flex-end;;
                span{
                    display: flex;
                    width: 50%;
                    height: 70%;
                    background: url("./../img/dik.png") no-repeat;
                    background-size: 100% 100%;
                    margin-left: 34%;
                    a{
                        display: flex;
                        flex: 1;
                        align-items: center;
                        justify-content: center;
                        box-sizing: border-box;
                        color:#fff;
                        font-size: 24px;
                        white-space: nowrap;
                    }
                    .active{
                        background: url("./../img/xuanz.png") no-repeat;
                        background-size: 100% 100%;
                        color: #33cdda;   
                    } 
                }
                
            }
            .rokenLine{
                padding-top: 2%;
                box-sizing: border-box;
                display: flex;
                flex: 1;
            }     
        }
        .userTypeNum{
            display: flex;
            flex: 1.3;
            align-items: flex-end;
            .userTypeNumBox{
                display: flex;
                width: 100%;
                height: 81%;
                flex-direction: column;
                .userTitle{
                    display: flex;
                    flex: 1;
                    align-items: flex-end;
                    span{
                        display: flex;
                        width: 40%;
                        height: 90%;
                        background: url("./../img/dik.png") no-repeat;
                        background-size: 100% 100%;
                        margin-left: 46%;
                        a{
                            display: flex;
                            flex: 1;
                            align-items: center;
                            justify-content: center;
                            color:#fff;
                            font-size: 24px;
                            // padding-left: 15px;
                            // padding-top: 5px;
                            // padding-right: 15px;
                            // padding-bottom: 5px;
                        }
                        .active{
                            background: url("./../img/xuanz.png") no-repeat;
                            background-size: 100% 100%;
                            color: #33cdda;   
                        } 
                    }
                }
                .barChart{
                    box-sizing: border-box;
                    padding-top: 2%;
                    display: flex;
                    flex: 9;
                }
            }
            
        }
    }
    
    

}
.ageOf_account{
    position: absolute;
    right: 4%;
    top: 0;
    width: 25%;
    height: 100%;
    display: flex;
    // padding-top: 8%;
    // padding-bottom: 3%;
    align-items: flex-end;
    box-sizing: border-box;
    .ageAcountBox{
        display: flex;
        width: 100%;
        flex-direction: column;
        height: 81%;
        .ageOpeningBox{
            display: flex;
            // flex: 0.75;
            height: 40%;
            .ageOpening{
                display: flex;
                width: 100%;
                height: 95%;
            }
        }
        .broadbandRatio{
            display: flex;
            // flex: 1;
            height:60%;
            
            box-sizing: border-box;
            align-items: flex-end;
            .broadbandBox{
                display: flex;
                flex-direction: column;
                width: 100%;
                height: 75%;
                .ageTitle{
                    color: #fff;
                    display: flex;
                    height: 10%;
                    align-items: flex-end;
                    span{
                        display: flex;
                        color: #fff;
                        width: 70%;
                        height: 85%;
                        margin-left: 5%;
                        background: url("./../img/dik.png") no-repeat;
                        background-size: 100% 100%;
                        a{
                            color:#fff;
                            font-size: 24px;
                            display: flex;
                            flex: 1;
                            justify-content: center;
                            align-items: center;
                        }
                        .active{
                            background: url("./../img/xuanz.png") no-repeat;
                            background-size: 100% 100%;
                            color: #33cdda;   
                        }
                    }
                    
                }
                .broadBox{
                    display: flex;
                    height: 77%;
                    // flex: 6;
                    box-sizing: border-box;
                    position: relative; 
                    align-items: center;
                    justify-content: center;
                    .broadbandBg{
                        max-width: 100%;
                        max-height: 100%;
                    }
                    .broadband{
                        width: 100%;
                        height: 100%;
                        position: absolute;
                        left: 50%;
                        top: 50%;
                        z-index: 111;
                        transform: translate(-50%, -51%);
                        display: flex;    
                    }
                    .penetration{
                        position: absolute;
                        width: 47%;
                        height: 96%;
                        color: #fff;
                        display: flex;
                        flex-direction: column;
                        justify-content: center;
                        align-items: center;
                        transform: scale(1.2);
                        span:first-child{
                            font-size: 22px;
                        }
                        span:last-child{
                            font-size: 24px;
                            margin-top: 3%;
                            color: #1ae1f8;
                        }
                    }
                }
                
            }
            
        }
    }
    
    
}
