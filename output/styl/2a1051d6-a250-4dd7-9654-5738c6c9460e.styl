// Font Editors
@font-face
  font-family: $fi-font-name;
  src: url($fi-font-path + 'fontisto/fontisto-editors.eot)
  src: url($fi-font-path + 'fontisto/fontisto-editors.eot) format("embedded-opentype"), 
  url($fi-font-path + 'fontisto/fontisto-editors.ttf) format("truetype"), 
  url($fi-font-path + 'fontisto/fontisto-editors.woff) format("woff"), 
  url($fi-font-path + 'fontisto/fontisto-editors.svg) format("svg")
  
.{$fi-css-prefix}
  display: inline-block
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  text-rendering: auto
  line-height: $fi-font-line-height
  font-family: $fi-font-name
  font-size: $fi-font-size
  font-size: inherit
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale
