// 1. Colors
// ==========================================================================
// $primary = #5c00de//color("materialize-red", "lighten-2")
// $primary-light = lighten($primary, 15%) 
// $primary-dark = darken($primary, 15%)
// $secondary = #ff7214//color("teal", "lighten-1")
// $secondary-light = lighten($secondary, 30%)//color("teal", "lighten-1")
$success-color = #00fc9e//color("green", "base")
$error-color = #ff148f//color("red", "base")
$link-color = var(--link-color)//#14ffff //color("light-blue", "darken-1")
// 2. Badges
// ==========================================================================
$badge-bg-color = var(--secondary)
$badge-height = 22px
// 3. Buttons
// ==========================================================================
// Shared styles
$button-border = none
$button-background-focus = lighten($secondary, 4%)
$button-font-size = 14px
$button-icon-font-size = 1.3rem
$button-height = 36px
$button-padding = 0 16px
$button-radius = 2px
// Disabled styles
$button-disabled-background = #dfdfdf
$button-disabled-color = #9f9f9f
// Raised buttons
$button-raised-background = $secondary
$button-raised-background-hover = lighten($button-raised-background, 5%)
$button-raised-color = white
// Large buttons
$button-large-font-size = 15px
$button-large-icon-font-size = 1.6rem
$button-large-height = $button-height * 1.5
$button-floating-large-size = 56px
// Small buttons
$button-small-font-size = 13px
$button-small-icon-font-size = 1.2rem
$button-small-height = $button-height * 0.9
$button-floating-small-size = $button-height * 0.9
// Flat buttons
$button-flat-color = #343434
$button-flat-disabled-color = lighten(#999999, 10%)
// Floating buttons
$button-floating-background = var(--secondary)
$button-floating-background-hover = $button-floating-background
$button-floating-color = white
$button-floating-size = 40px
$button-floating-radius = 50%
// 4. Cards
// ==========================================================================
$card-padding = 24px
$card-bg-color = white
$card-link-color = #ff7214 //color("orange", "accent-2")
$card-link-color-light = lighten($card-link-color, 20%)
// 5. Carousel
// ==========================================================================
$carousel-height = 400px
$carousel-item-height = $carousel-height / 2
$carousel-item-width = $carousel-item-height
// 6. Collapsible
// ==========================================================================
$collapsible-height = 3rem
$collapsible-line-height = $collapsible-height
$collapsible-header-color = white
$collapsible-border-color = #dddddd
// 7. Chips
// ==========================================================================
$chip-bg-color = #e4e4e4
$chip-border-color = #9e9e9e
$chip-selected-color = #26a69a
$chip-margin = 5px
// 8. Date + Time Picker
// ==========================================================================
$datepicker-display-font-size = 2.8rem
$datepicker-calendar-header-color = #999999
$datepicker-weekday-color = rgba(0, 0, 0, 0.87)
$datepicker-date-bg = var(--secondary)
$datepicker-year = rgba(255, 255, 255, 0.7)
$datepicker-focus = rgba(0, 0, 0, 0.05)
$datepicker-selected = var(--secondary)
$datepicker-disabled-day-color = rgba(0, 0, 0, 0.3)
$timepicker-clock-color = rgba(0, 0, 0, 0.87)
$timepicker-clock-plate-bg = #eeeeee
// 9. Dropdown
// ==========================================================================
$dropdown-bg-color = white
$dropdown-hover-bg-color = #eeeeee
$dropdown-color = var(--secondary)
$dropdown-item-height = 40px
// 10. Forms
// ==========================================================================
// Text Inputs + Textarea
$input-height = 3rem
$input-border-color = #ababab//color("grey", "base")
$input-border = 1px solid $input-border-color
$input-background = white
$input-error-color = $error-color
$input-success-color = $success-color
$input-focus-color = var(--secondary)
$input-font-size = 16px
$input-margin-bottom = 8px
$input-margin = 0 0 $input-margin-bottom 0
$input-padding = 0
$label-font-size = 0.8rem
$input-disabled-color = rgba(0, 0, 0, 0.42)
$input-disabled-solid-color = #949494
$input-disabled-border = 1px dotted $input-disabled-color
$input-invalid-border = 1px solid $input-error-color
$input-icon-size = 2rem
$placeholder-text-color = lighten($input-border-color, 20%)
// Radio Buttons
$radio-fill-color = var(--secondary)
$radio-empty-color = #5a5a5a
$radio-border = 2px solid $radio-fill-color
// Range
$range-height = 14px
$range-width = 14px
$track-height = 3px
// Select
$select-border = 1px solid #f2f2f2
$select-background = rgba(255, 255, 255, 0.9)
$select-focus = 1px solid lighten($secondary, 47%)
$select-option-hover = rgba(0, 0, 0, 0.08)
$select-option-focus = rgba(0, 0, 0, 0.08)
$select-option-selected = rgba(0, 0, 0, 0.03)
$select-padding = 5px
$select-radius = 2px
$select-disabled-color = rgba(0, 0, 0, 0.3)
// Switches
$switch-bg-color = var(--secondary)
// $switch-checked-lever-bg = desaturate(lighten($switch-bg-color, 25%), 25%)
$switch-unchecked-bg = #f1f1f1
$switch-unchecked-lever-bg = rgba(0, 0, 0, 0.38)
$switch-radius = 15px
// 11. Global
// ==========================================================================
// Media Query Ranges
$small-screen-up = 601px
$medium-screen-up = 993px
$large-screen-up = 1201px
$small-screen = 600px
$medium-screen = 992px
$large-screen = 1200px
$medium-and-up = "only screen and (min-width : #{$small-screen-up})"
$large-and-up = "only screen and (min-width : #{$medium-screen-up})"
$extra-large-and-up = "only screen and (min-width : #{$large-screen-up})"
$small-and-down = "only screen and (max-width : #{$small-screen})"
$medium-and-down = "only screen and (max-width : #{$medium-screen})"
$medium-only = "only screen and (min-width : #{$small-screen-up}) and (max-width : #{$medium-screen})"
// 12. Grid
// ==========================================================================
$num-cols = 12
$gutter-width = 1.5rem
$element-top-margin = $gutter-width / 3
$element-bottom-margin = $gutter-width * 2 / 3
// 13. Navigation Bar
// ==========================================================================
$navbar-height = 64px
$navbar-line-height = $navbar-height
$navbar-height-mobile = 56px
$navbar-line-height-mobile = $navbar-height-mobile
$navbar-font-size = 1rem
$navbar-font-color = white
$navbar-brand-font-size = 2.1rem
// 14. Side Navigation
// ==========================================================================
$sidenav-width = 300px
$sidenav-font-size = 14px
$sidenav-font-color = rgba(0, 0, 0, 0.87)
$sidenav-bg-color = white
$sidenav-padding = 16px
$sidenav-item-height = 48px
$sidenav-line-height = $sidenav-item-height
// 15. Photo Slider
// ==========================================================================
$slider-bg-color = #ababab //color("grey", "base")
$slider-bg-color-light = #cdcdcd //color("grey", "lighten-2")
$slider-indicator-color = #00fc9e//color("green", "base")
// 16. Spinners | Loaders
// ==========================================================================
$spinner-default-color = var(--secondary)
// 17. Tabs
// ==========================================================================
$tabs-underline-color = var(--primary)-light
$tabs-text-color = var(--primary)
$tabs-bg-color = white
// 18. Tables
// ==========================================================================
$table-border-color = rgba(0, 0, 0, 0.12)
$table-striped-color = rgba(242, 242, 242, 0.5)
// 19. Toasts
// ==========================================================================
$toast-height = 48px
$toast-color = #323232
$toast-text-color = white
$toast-action-color = #eeff41
// 20. Typography
// ==========================================================================
$font-stack = -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Oxygen-Sans, Ubuntu, Cantarell, "Helvetica Neue", sans-serif
$off-black = rgba(0, 0, 0, 0.87)
// Header Styles
$h1-fontsize = 4.2rem
$h2-fontsize = 3.56rem
$h3-fontsize = 2.92rem
$h4-fontsize = 2.28rem
$h5-fontsize = 1.64rem
$h6-fontsize = 1.15rem
// 21. Footer
// ==========================================================================
$footer-font-color = white
$footer-bg-color = var(--primary)
$footer-copyright-font-color = rgba(255, 255, 255, 0.8)
$footer-copyright-bg-color = rgba(51, 51, 51, 0.08)
// 22. Flow Text
// ==========================================================================
$range = $large-screen - $small-screen
$intervals = 20
$interval-size = $range / $intervals
// 23. Collections
// ==========================================================================
$collection-border-color = #e0e0e0
$collection-bg-color = white
$collection-active-bg-color = var(--secondary)
$collection-active-color = var(--secondary-light)
$collection-hover-bg-color = #dddddd
$collection-link-color = var(--secondary)
$collection-line-height = 1.5rem
// 24. Progress Bar
// ==========================================================================
$progress-bar-color = var(--secondary)
$progress-bar-color-light = var(--secondary-light)