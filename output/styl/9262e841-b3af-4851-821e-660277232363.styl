body
  padding 0
  margin 0
  font-family Roboto
  letter-spacing 2px

#list
  padding 10px
  letter-spacing 0px

#list tr
  display flex
  flex-direction row
  justify-content center
  align-content center
  border none
  border-radius 5px
  margin-bottom 10px
  background #FFFFFF

#list h3
  background #FE7674
  color white
  padding 5px 10px
  border-radius 5px

#list td:nth-child(1)
  width 75%
  border none
  padding 5px 10px

#list td:nth-child(2)
  width 25%
  align-self center
  border none
  padding 5px 10px

#list .button
  display flex

#list button
  flex 1 1 auto

#list .listTbody:nth-child(2)
  text-align right

#list .count
  display flex
  flex-direction column
  text-align center
  line-height 1.5rem

#list .count span
  color #FE7674
  font-weight bold

#list .count span:nth-child(3)
  font-size 2rem
  line-height 2rem

#list .count span span
  font-size 1rem
  color gray

.icon
  width 1.7em
  height 1.7em

#app
  width 100vw
  height 100vh
  background #F9EAD8
  overflow hidden

#app a,h5
  color #FE7674
  margin 0

#app .navbar-brand
  display flex
  justify-content center
  align-items center

#app .navbar
  height 10vh

#app .content
  height 90vh

#left
  height 100vh
  background #FE7674
  color white

.colorPink
  color #FE7674

.borderPink
  border-bottom 1px solid #FE7674

.bg-none
  background rgba(0,0,0,0)

.bg-pink
  background #FE7674

.listIcon
  background rgba(0,0,0,0)
  border none

#home .col-2
  display flex
  align-items center

#home .col
  text-align right

#home .date
  display flex
  align-items flex-end
  justify-content flex-end
  margin-bottom -5px
  font-weight bolder

#home .time
  font-size 35px
  font-weight bolder

#home .countDownCircle
  position absolute
  left calc(50% - 15vw)
  top calc(50% - 18vw)
  width 30vw
  height 30vw
  background white
  border-radius 50%

#home .countDownCircle::before
  content ''
  background #FE7674
  border-radius 50%
  position absolute
  top 1.5vw
  left 1.5vw
  width 27vw
  height 27vw

#home .content
  position absolute
  top 50%
  left 50%
  transform translate(-50%,-50%)
  display flex
  flex-direction column
  align-items center
  justify-content center

#home .content:nth-child(1)
  font-size calc(1rem + 7vw)

#home .content:nth-child(2)
  font-size calc(1rem + 1vw)
  top 110%

#home .start
  top 70%
  width 20px

.navbarText
  font-size calc(1rem + 1vw)

.iconsize1
  font-size calc(1rem + 1vw)

.iconsize2
  font-size calc(1rem + 2vw)

.iconsize3
  font-size calc(1rem + 0.5vw)

#home .started
  top 125%
  flex-direction row

#settings .container
  display flex
  flex-direction column
  justify-content center
  align-content center

#settings .txt
  background #FE7674
  font-size calc(1rem + 1vw)
  font-weight bolder
  color white
  padding 0.1vw 2vw
  border-radius 5px

#settings .iconPink
  color #FE7674

#settings .iconGray
  color gray

#settings td
  font-size calc(1rem + 0.5vh)
  color gray
  border none
  vertical-align middle

#settings td:nth-child(1)
  width 10px

#analystics
  padding 10px

#analystics .title
  background #FE7674
  color white
  font-size calc(1px + 2vw)
  font-weight bolder
  padding 1%
  border-radius 5px

#analystics .number
  color #FE7674
  font-size calc(1px + 8vw)
  font-weight bolder

#analystics .tomato
  color gray

#analystics .col-8
  height 35vh