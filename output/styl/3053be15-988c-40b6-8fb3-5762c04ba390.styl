a, audio, b, body, canvas, dd, div, dl, dt, footer, form, h1,
h2, h3, h4, h5, h6, header, html, i, img, input,label, li,nav, ol,
output, p,span, table, tbody, td, tfoot, th, thead, tr, tt, u, ul, video
  box-sizing border-box
html,body,#app
  width: 100%
  background-color: #f5f5f5
button, input
  outline: none
  border: none
  font-size: inherit
  font-family: inherit
  background-color: transparent
img
  width: 100%
  height: 100%
  display: block
.nowrap
  flex: 1
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  vertical-align: middle;
.fixed-top
  background-image: linear-gradient(90deg,#0af,#0085ff);
.padding-fixed
  padding-top: 50px
.goback-icon
  width: 10px;
  height: 10px;
  display: inline-block;
  border-bottom: 2px solid #fff;
  border-left: 2px solid #fff;
  transform: rotate(45deg);
.more-rihgt-icon
  width: 10px;
  height: 10px;
  display: inline-block;
  border-top: 2px solid #fff;
  border-right: 2px solid #fff;
  transform: rotate(45deg);
.sanjiao-right
  width:0;
  height:0;
  border-width:6px 0 6px 6px;
  border-style:solid;
  border-color:transparent transparent transparent #333;/*透明 透明 透明 灰*/
.sanjiao-bottom
  width:0;
  height:0;
  border-width:4px 4px 0 4px;
  border-style:solid;
  border-color: #999 transparent transparent transparent;/*透明 透明 透明 灰*/
.fixed-top-search
  position: fixed
  top: 0
  left: 0
  width: 100%
  display flex
  align-items center
  padding: 10px 0
  background-color: #fff
  z-index: 10
  .goback
    padding: 5px 20px 5px 5px
    .goback-icon-svg
      width: 18px
      height: 28px
  .input-w
    flex 1
    position: relative
    .icon-fangdajing
      position: absolute
      left: 10px
      top: 50%
      transform translateY(-50%)
      width: 12px
    .input
      width: 100%
      padding: 10px 10px 10px 33px
      border-radius 4px
      background-color: #f8f8f8
      font-size: 12px
  .btn-search
    padding: 10px
    font-size: 16px
    font-weight: bold
    color #000
//  排序过滤列表
.filter-uls
  display flex
  align-items center
  height: 40px
  background-color: #fff
  &.fixed-filrter
    position: absolute !important
    left: 0
    width: 100%
    z-index: 1000
  .filter-list
    flex 1
    height: 100%
    font-size: 14px
    color $hei
    display flex
    align-items center
    justify-content center
    &.current-filter
      font-weight: bold;
      color #000
    .dropdown-icon
      width: 6px
      margin-left: 5px
    .filter-nav-more__icon
      width: 12px
      height: 10px
      margin-left: 5px
//  列表-商家列表
flex-center()
  display: flex
  align-items center
.shopping-uls
  padding:0 10px
  .shopping-list
    display flex
    padding: 16px 0
    .goods-logo
      width: 80px
      height: 80px
      border: 1px solid #ddd
      margin-right: 10px
    .goods-info
      flex 1
      overflow: hidden
      .top-title
        flex-center()
        .goods-title
          flex 1
          .title-text
            font-size: 16px
            color #000
            font-weight: bold
          .brand
            background-image: linear-gradient(-139deg,#fff100,#ffe339);
            padding: 4px
            font-size: 12px
            font-weight: bold
            border-radius 2px
            color #000
            margin-right: 4px
        .right
          flex-center()
          justify-content center
          font-size: 12px
          .icon-item
            font-size: 10px
            border-radius 2px
            border: 1px solid #ddd
            color #999
            margin-left: 4px
          .add
            padding: 0 5px
            color #666
          .solt
            vertical-align middle
            padding: 0 5px
            margin-left: 5px
            margin-top: -5px
            flex-center()
            justify-content center
      .star-w
        flex-center()
        justify-content space-between
        margin: 12px 0 8px
        font-size: 12px
        .star-l
          color #666
          flex-center()
        .star
          font-size: 16px
          color #fff100
        .grade-num
          margin: 0 6px
        .hummingbird
          color #fff
          background-image: linear-gradient(45deg,#0085ff,#0af)
          border: 1px solid #f5f5f5
          padding: 2px
      .money
        flex-center()
        justify-content space-between
        font-size: 12px
        color #666
        .price
          flex-center()
          .price-border
            padding: 0 5px
            border-left: 1px solid #f5f5f5
            margin-left: 8px
            white-space nowrap
      .tag-uls
        flex-center()
        margin: 10px 0 18px
        color #666
        .tag-list
          padding: 1px 2px
          margin-right: 4px
          border: 1px solid #f5f5f5
          font-size: 12px
          border-radius 2px
      .activity-w
        display: flex
        .activity-uls
          flex 1
          .activity-list
            flex-center()
            font-size: 12px
            color #666
            margin-bottom: 5px
            padding-right: 10px
            .tag-first
              font-size: 12px
              color #fff
              background-color: rgb(112, 188, 70)
              padding: 1px 2px
              border-radius 2px
              margin-right: 5px
        .activity-total
          height: 14px
          flex-center()
          font-size: 12px
          color #666
          .dropdown-icon
            width: 6px
            margin-left: 6px
