section.sf-container-msj-proyectos
    background: $color-white
    margin: 30px 0
    div.sf-container-flex-msj-proyectos
        display: flex
        flex-direction: row
        justify-content: center
        +media-m()
            flex-direction: column
        +media-s()
            flex-direction: column
        div.sf-msj-proyectos
            font-family : $font-family-ubuntu
            text-align : center
            font-size: 22px
            color: $color-primary
            align-self: center
            +media-m()
                font-size: 22px
                align-self: center
            +media-s()
                font-size: 22px
            label 
                font-weight: bold
        div.sf-msj-button
            margin: 0 0 0 30px
            +media-m()
                margin: 10px 0 0 0
                align-self: center
            +media-s()
                text-align: center
                margin: 20px 0 0 0


//botones
ul.list-categories
    & li
        & a
            &.active
                color : $color-primary
                -webkit-box-shadow: -6px 0px 0px 0px $color-primary
                box-shadow: -6px 0px 0px 0px $color-primary
                outline: 0 
                +media-m()
                    -webkit-box-shadow: 0px -6px 0px 0px $color-primary
                    -moz-box-shadow: 0px -6px 0px 0px $color-primary
                    box-shadow: 0px -6px 0px 0px $color-primary
                +media-s()
                    -webkit-box-shadow: 0px -6px 0px 0px $color-primary
                    -moz-box-shadow: 0px -6px 0px 0px $color-primary
                    box-shadow: 0px -6px 0px 0px $color-primary

ul.list-categories
    list-style: none
    padding: 0
    margin: 0 0 0 10px
    width: 112px
    +media-m()
        margin: 0
        display: flex;
        flex-direction: row
        width: 1200px
    +media-s()
        margin: 0
        display: flex;
        flex-direction: row
        width: 1200px
    & li
        display: flex;
        flex-direction: column
        justify-content: center
        text-align: center
        border-left: solid 6px $color-gray-600
        background: $color-white    
        +media-m()
            width: 144px 
            border-top: solid 6px #757575
            border-left: 0
        +media-s()
            width: 144px 
            border-top: solid 6px #757575
            border-left: 0
        & a
            color : $color-gray-600
            padding: 15px 0
            display: block
            border-top: solid 1px $color-gray-350
            border-right: solid 1px $color-gray-350
            +media-m()
                border-top: solid 1px $color-gray-350
                border-right: solid 1px $color-gray-350
            +media-s()
                border-top: solid 1px $color-gray-350
                border-right: solid 1px $color-gray-350
            &:hover , &:focus  
                color : $color-primary
                -webkit-box-shadow: -6px 0px 0px 0px $color-primary
                box-shadow: -6px 0px 0px 0px $color-primary
                outline: 0 
                +media-m()
                    -webkit-box-shadow: 0px -6px 0px 0px $color-primary
                    -moz-box-shadow: 0px -6px 0px 0px $color-primary
                    box-shadow: 0px -6px 0px 0px $color-primary
                +media-s()
                    -webkit-box-shadow: 0px -6px 0px 0px $color-primary
                    -moz-box-shadow: 0px -6px 0px 0px $color-primary
                    box-shadow: 0px -6px 0px 0px $color-primary
            &:hover div, &:focus div 
                color : $color-primary
        & i
            align-self: center
            font-size: 24px
            margin-bottom: 8px
        & div
            font-family: $font-family-ubuntu-regular
            font-size: 10px
            color: $color-gray-550
        &:last-child
            border-bottom: solid 1px $color-gray-350
            +media-m()
                border-bottom: 0
                border-right: solid 1px $color-gray-350
            +media-s()
                border-bottom: 0
                border-right: solid 1px $color-gray-350

//nueva seccion
section.sf-container-proyectos-new
    background: $color-gray
    height: 750px
    +media-xl()
        height: auto
    +media-m()
        height: auto
    +media-s()
        height: auto
    & .content
        width: $width-slim-fit
        +media-xl()
            width: auto
        +media-m()
            width: 100%
            margin: 0
        +media-s()
            margin: 0
            width: 100%
        & div.sf-container-flex-proyectos
            display: flex
            flex-direction: column
            margin: 40px 0
            +media-s()
                margin: 40px 0 30px 0
            & div.sf-content-titles-proyectos
                color: $color-gray-500
                text-align: center
                margin : 0 0 25px 0
                & div.sf-title-proyectos
                    font-family: $font-family-ubuntu
                    font-size: 44px 
                    & label
                        font-family: $font-family-ubuntu
                    +media-m()
                        font-size: 35px
                    +media-s()
                        font-size: 35px
                & div.sf-subtitle-proyectos h3
                    font-family: $font-family-ubuntu
                    font-size: 26px 
                    & label
                        font-family: $font-family-ubuntu
                    +media-m()
                        font-size: 22px
                    +media-s()
                        font-size: 22px
            & div.sf-container-pictures-proyectos
                display: flex
                +media-m()
                    flex-direction: column
                +media-s()
                    flex-direction: column
                & div.sf-container-pictures-tiny
                    display: flex
                    flex-direction: row-reverse
                    flex-grow: 1
                    +media-m()
                        justify-content: flex-start
                        flex-direction: row
                        overflow-x: auto
                    +media-s()
                        justify-content: flex-start
                        flex-direction: row
                        overflow-x: auto
                & div.sf-container-picture-big
                    flex-grow: 2
                    width: 100%
                    cursor: pointer
                    overflow: hidden
                    position relative
                    +media-s()
                        height : auto
                    .load
                        background-image: url(../img/spinner_1.png)
                        background-repeat: no-repeat
                        background-position: center
                        width: 100%
                        height: 560px
                        z-index: 2
                        display none
                        position: absolute
                        background-color: #F5F5F5
                        +media-s()
                            height : 280px
                    & .txt_izq
                        position: absolute
                        left : 0
                        background: url(../img/bg_popup_dark.png)
                        color : #fff
                        z-index : 2
                        padding: 10px 20px
                        +media-s()
                            padding: 6px 10px
                            font-size: 12px
                    & .txt_der
                        position: absolute
                        background: url(../img/bg_popup_dark.png)
                        color : #fff
                        right : 0
                        z-index : 2
                        padding: 10px 20px
                        +media-s()
                            padding: 6px 10px
                            font-size: 12px
                    & img
                        +media-m()
                            height : auto
                            flex-grow: 1
                        +media-s()
                            height : auto
                            flex-grow: 1
                    &:hover .owl-nav
                        display: block
                        +media-m()
                            display: none
                        +media-s()
                            display: none

.owl-nav
    width: 100%
    position: absolute
    top: 0
    margin-top: 25% !important
    overflow: hidden
    display: none      
    & button.owl-prev 
        float: left
    & button.owl-next
        float: right
    & button.owl-prev, 
    & button.owl-next
        border-radius: 0
        background: $color-white !important
        border: none
        width: 50px
        height: 50px
        outline: 0
        &:hover
            background: $color-primary !important
            & span 
                color: $color-white
        & span
            font-size: 40px
            color: $color-primary
.owl-dots
    margin-top: 15px
    & button
        outline: 0
    & button.active span
        background: $color-primary !important
        outline: 0








