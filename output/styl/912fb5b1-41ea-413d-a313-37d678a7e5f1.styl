#page-doctor div[data-role='footer'],
#map-page div[data-role='footer']
	background-color #fff
	padding 0 1em
	text-align center

.to-form-doctor-btn
	display inline-block
	position relative
	overflow hidden
	border 3px solid #e9d9d3
	border-radius 28px

.to-form-doctor-btn > a
	text-decoration none
	text-transform uppercase
	border 2px solid #cd4005
	background-color #fc5512
	display block
	font-size 15px
	line-height 15px
	position relative
	padding 13px 50px 13px 17px
	letter-spacing 1px
	border-radius 26px

.to-form-doctor-btn > a,
.to-form-doctor-btn > a:hover,
.to-form-doctor-btn > a:active,
.to-form-doctor-btn > a:visited
	color #fff
	text-shadow none
	text-decoration none

.to-form-doctor-btn > a > i
	display block
	position absolute
	right 15px
	top 14px
	sprite($s-envelope)

.call-doctor-btn
	float right
	border 3px solid #ccebc3
	overflow hidden
	border-radius 100%

.call-doctor-btn > a
	border-radius 100%
	border 2px solid #fff
	background-color #36b00e
	display block
	width 44px
	height 44px
	position relative

.call-doctor-btn > a > i
	display block
	position absolute
	left 50%
	top 50%
	margin-left -9.5px
	margin-top -9.5px
	sprite($s-call-icon)

.doctor-info-block .left-block
	float left
	margin-right 1em

.doctor-info-block .left-block .avatar-wrap
	clear both
	overflow hidden
	border 2px solid #257b7e
	background-color #257b7e
	border-radius 5px
	margin-bottom 8px
	width 65px
	height 65px
	text-align center

.doctor-info-block .left-block .avatar-wrap img
	display inline-block
	vertical-align top

.doctor-info-block .left-block .rating-wrap
	overflow hidden
	clear both
	margin-bottom 3px

.doctor-info-block .left-block .rating-wrap strong:first-child
	float left
	color #fc5412
	font-size 12px
	line-height 12px
	font-weight 400

.doctor-info-block .left-block .rating-wrap ul
	float left
	clear none
	padding-left 5px
	padding-top 1px

.doctor-info-block .left-block .rating-wrap ul li
	float left
	margin 0 0 0 1px
	text-indent -9999em

.doctor-info-block .left-block .rating-wrap ul li.full
	sprite($s-star-full)

.doctor-info-block .left-block .rating-wrap ul li.empty
	sprite($s-star-empty)

.doctor-info-block .left-block .rating-wrap ul li:first-child
	margin-left 0

.doctor-info-block .left-block .qty-reviews
	display block
	clear both
	overflow hidden
	text-align center
	text-transform uppercase
	color #9a9a9a
	font-size 9px
	letter-spacing 2px

.doctor-info-block .right-block
	overflow hidden
	position relative
	padding-right 30px

.doctor-info-block .right-block a.link-to-map
	display block
	text-indent -9999em
	position absolute
	top -5px
	right 0
	text-decoration none
	background transparent url('../img/pics/location_btn.png') no-repeat 100% 50%
	width 30px
	height 30px

.doctor-info-block .right-block h3.name
	margin 0 0 10px 0
	padding 0
	color #343131
	font-size 16px
	font-weight 400
	line-height 20px

.doctor-info-block .right-block .prof
	display block
	clear both
	color #18b1ac
	text-transform uppercase
	font-weight 400
	font-size 11px
	letter-spacing 1px
	margin-bottom 3px

.doctor-info-block .right-block .skills-wrap
	clear both
	overflow hidden
	margin-bottom 10px

.doctor-info-block .right-block .skills-wrap > span
	float left
	margin-left 15px
	color #9a9a9a
	font-size 11px

.doctor-info-block .right-block .skills-wrap > span:first-child
	margin-left 0

.doctor-info-block .right-block .skills-wrap > span.left b
	color #343131

.doctor-info-block .right-block .skills-wrap > span.right b
	color #fe5822

.doctor-info-block .right-block .skills-wrap > span.right b span
	font-family 'ALSRublRegular'
	font-weight 400

.doctor-info-block .address-wrap
	margin-bottom 4px

.metro-wrap
	overflow hidden
	clear both
	position relative
	color #9a9a9a
	font-size 11px
	padding-left 15px
	height 13px
	line-height 13px

.doctor-info-block .metro-wrap > i:first-child
	display block
	position absolute
	top 2px
	left 0
	sprite($s-metro-icon)

.doctor-info-content p.main-info
	color #000
	font-size 12px
	line-height 15px
	margin-bottom 0

.doctor-info-content .doctor-info-item
	position relative
	overflow hidden
	clear both
	border-top 1px solid #dbdbdb
	padding 1em 0

.doctor-info-content .doctor-info-item:first-of-type
	border none

strong.w-img
	display block
	clear both
	overflow hidden
	position relative
	color #18b1ac
	font-size 13px
	font-family 'Circe-bold'
	font-weight 400
	margin-bottom 10px
	margin-left 5px
	padding-left 25px

strong.w-img i
	display block
	position absolute
	left 0
	top 0

strong.img-1 i
	sprite($s-img-1)

strong.img-2 i
	sprite($s-img-2)

strong.img-3 i
	sprite($s-img-3)
	top 3px

strong.img-4 i
	sprite($s-img-4)

strong.img-4
	padding-top 4px
	margin-bottom 12px

.doctor-info-content ul.skills-listing
	margin-left 15px

.doctor-info-content ul.skills-listing li
	font-size 11px
	line-height 15px
	padding-left 15px
	position relative
	overflow hidden
	clear both
	background transparent url('../img/pics/bullet.png') no-repeat 0 6px

.send-review-block
	position relative
	overflow hidden
	clear both
	padding 1em 0

.send-review-block > a,
.send-review-block > a:hover,
.send-review-block > a:active,
.send-review-block > a:visited
	color #fc5412
	float left
	border-bottom 2px dotted #fc5412
	font-size 14px
	font-weight 400
	font-family 'Circe-bold'
	text-decoration none

.send-review-block > span
	float right
	text-transform uppercase
	font-size 12px
	line-height 14px

.total-review-block
	position relative
	overflow hidden
	clear both

.total-review-block strong:first-child
	display block
	clear both
	overflow hidden
	font-size 12px
	font-family 'Circe-bold'
	font-weight 400
	margin-bottom 12px

.total-review-block > span.total,
.total-review-block > .item
	float left 
	width 25%

.total-review-block > span.total
	font-size 13px
	font-weight 400
	font-family 'Circe-bold'
	color #18b1ac

.total-review-block > .item > ul
	position relative
	overflow hidden
	clear both
	margin-bottom 8px
	padding-top 2px

.total-review-block > .item > ul > li
	float left
	margin-left 4px

.total-review-block > .item > ul > li:first-child
	margin-left 0

.total-review-block > .item > ul > li.full
	sprite($s-star-full)

.total-review-block > .item > ul > li.empty
	sprite($s-star-empty)

.total-review-block > .item > span
	display block
	clear both
	text-transform uppercase
	color #767676
	font-size 7px

.list-of-reviews > strong:first-child
	display block
	clear both
	overflow hidden
	font-size 12px
	font-weight 400
	font-family 'Circe-bold'
	margin-bottom 12px

.list-of-reviews > ul > li
	position relative
	overflow hidden
	clear both
	padding-top 15px

.list-of-reviews > ul > li:first-child
	padding-top 0

.list-of-reviews > ul > li .review-text-wrapper
	float left
	width 75%
	padding-right 8px
	overflow hidden
	position relative

.list-of-reviews > ul > li .review-text-wrapper > i
	display block
	position absolute
	top 0
	right 0
	sprite($s-frame)

.list-of-reviews > ul > li .review-text-wrapper .review-text-block
	overflow hidden
	background-color #f3f3f3
	border-radius 20px
	padding 12px 15px

.list-of-reviews > ul > li .review-text-wrapper .review-text-block p
	margin 0
	font-size 11px
	line-height 15px

.list-of-reviews > ul > li .review-author-block
	float left
	width 25%
	padding-left 7px

.list-of-reviews > ul > li .review-author-block strong,
.list-of-reviews > ul > li .review-author-block span,
.list-of-reviews > ul > li .review-author-block em
	display block
	clear both
	font-style normal
	font-weight 400

.list-of-reviews > ul > li .review-author-block strong
	color #18b1ac
	font-size 13px
	font-family 'Circe-bold'
	margin-bottom 8px

.list-of-reviews > ul > li .review-author-block span
	font-size 11px
	margin-bottom 2px

.list-of-reviews > ul > li .review-author-block em
	font-size 9px
	color #9a9a9a

.list-of-reviews > ul > li .items-wrapper
	position relative
	overflow hidden
	clear both
	padding-top 8px
	padding-left 15px

.list-of-reviews > ul > li .items-wrapper .item
	float left
	width 33.333%

.list-of-reviews > ul > li .items-wrapper .item ul
	position relative
	overflow hidden
	clear both
	margin-bottom 8px

.list-of-reviews > ul > li .items-wrapper .item ul > li
	float left
	margin-left 4px

.list-of-reviews > ul > li .items-wrapper .item ul > li:first-child
	margin-left 0

.list-of-reviews > ul > li .items-wrapper .item ul > li.full
	sprite($s-star-full)

.list-of-reviews > ul > li .items-wrapper .item ul > li.empty
	sprite($s-star-empty)

.list-of-reviews > ul > li .items-wrapper .item > span
	display block
	clear both
	text-transform uppercase
	color #767676
	font-size 7px