$font-family1 = 'Josefin Sans',Arial,Helvetica,'Nimbus Sans L',sans-serif;
$font-family2 = 'Quattrocento',Arial,Helvetica,'Nimbus Sans L',sans-serif;
$font-family0 = Arial, Helvetica, sans-serif;
html, body {
  font-family: $font-family1;
}

h1, h2, h3, h4, h5, h6, [class='title$'] {
  font-family: $font-family1!important;
}

div, p, span, a, h1, h2, h3, h4, h5, h6,img, li, label, strong {
  &::selection {
    background: var(--accent-color-a15);
    color: var(--accent-color);
  }
  font-family: $font-family2;
}

// code, span {
//   &::selection {
//     background: var(--accent-color-a15);
//     color: var(--accent-color);
//   }
// }

code, span[class*='token'] {
  font-family: $font-family0;
  &::selection {
    background: var(--accent-color-a15)
  }
}

.dropdown-subtitle span {
  font-size: 1rem!important;
  font-weight: 600;
  cursor: default;
}

a:hover {
  transition: all 0.5s;
  color: var(--accent-color)!important;
}

[role=navigation] {
  &:hover {
    transition: all 1s;
  }
}

.search-box input {
  &:hover {
    // border: 1px solid #3eaf7c;
    border: 1px solid var(--accent-color);
    transition: all 0.5s;
  }
}


.blog-hero {
  // box-shadow: 0 1px 3px rgb(18 18 18 / 10%);
  h1 {
    font-size: 1.5rem!important;
    font-family: $font-family1;
  }
  .description {
    font-size: 2.2rem!important;
    font-family: $font-family1;
  }
}

button {
  outline: none;
}

article {
  .title {
    // display: inline-block;
    // width: 100%;
  }
}

.footer-wrapper .footer {
  display: flex;
  align-items: center;
  width: 100%;
  h2 {
    border: none!important;
  }
}

// .navbar {
//   background: transparent!important;
// }

span.token, code {
  font-size: 1em!important;
}

// awesome img
.awesome-img {
  margin: 20px 0;
  box-shadow: 0 0 10px 0 var(--accent-color-a15);
}

.awesome-img:hover {
  transform: translateY(-6px);
  transition: all ease 1s;
}
