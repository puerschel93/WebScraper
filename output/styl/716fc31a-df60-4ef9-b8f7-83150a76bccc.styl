*
  margin: 0
  padding: 0
  list-style: none

html,
body
  height: 100%

body
  display: flex
  justify-content: center
  align-items: center

.row-1
  display: flex

.app
  display: flex

.home
  width: 1100px

.todo
  width: 1100px

.ringtone
  width: 1100px

.analytics
  width: 1100px  

.container
  display: flex
  justify-content: center
/* left menu*/
.col-13
  top: 0px
  left: 0px
  width: 180px
  height: 800px
  background: linear-gradient(180deg, #FF6F61 0%, #FF4040 100%)
  box-shadow: 0px 0px 20px #44444480
  opacity: 1

.col-13-b
  top: 0px
  left: 0px
  width: 180px
  height: 800px
  background: linear-gradient(180deg, #0AB6A2 0%, #009987 100%)
  box-shadow: 0px 0px 20px #44444480
  opacity: 1

.col-14
  top: 0px
  left: 0px
  width: 1100px
  height: 800px
  background: linear-gradient(180deg, #FF6F61 0%, #FF4040 100%)
  opacity: 0.8

.col-14-b
  top: 0px
  left: 0px
  width: 1100px
  height: 800px
  background: linear-gradient(180deg, #0AB6A2 0%, #009987 100%)
  opacity: 0.8

.font
  top: 100px
  left: 65px
  width: 50px
  height: 200px
  text-align: center
  writing-mode: vertical-lr
  text-orientation: mixed
  font: normal normal bold 30px/60px Roboto
  color: #FFFFFF
  transform:scale(-1)

p
  margin: 0px

.left_menu
  display: flex
  flex-direction: column
  justify-content: space-around
  align-items: center
  height: 800px

.icon
  display: flex
  flex-direction: column
  justify-content: space-around
  align-items: center
  height: 300px

/* main css */
.time
  display: flex
  flex-direction: column
  justify-content: center
  align-items: center
  height: 450px
  font: normal normal bold 70px/92px Roboto
  color: #FFFFFF

.time_up
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center
  width: 800px
  height: 100px 

.time_up a
  width: 60px

.time_num
  display: flex
  flex-direction: row
  justify-content: center
  align-items: center
  width: 300px

.time_name
  display: flex
  flex-direction: row
  justify-content: center
  align-items: center
  width: 540px

.time_down
  display: flex
  flex-direction: column
  justify-content: start
  align-items: center

progress
  border-radius: 1px
  width: 800px
  height: 20px

progress::-webkit-progress-bar
  border: 2px solid #fff
  background-color: transparent
  border-radius: 1px

progress::-webkit-progress-value
  background-color: #fff
  border-radius: 1px

.home_todo
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center
  height: 350px
  font: normal normal normal 20px/26px Roboto
  color: #FFFFFF

.home_todo_up
  padding: 0px
  width: 800px
  height: 40px
  display: flex
  justify-content: space-between
  align-items: center

.home_todo_down
  height: auto
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center

.main_text
  width: 740px
  height: 40px
  box-sizing: border-box
  border: 3px solid #fff
  background-color: transparent
  font: normal normal normal 20px/26px Roboto
  padding: 15px 22px
  margin: 10px 5px
  color: #FFFFFF
  opacity: 0.8

.main_text::placeholder
  color: #FFFFFF
  opacity: 0.5

.home_todo_list
  width: 800px
  display: flex
  flex-direction: row
  justify-content: space-between
  align-items: center
  padding-top: 10px
  padding-bottom: 10px
  border-bottom: 3px solid rgba(255,255, 255, 0.5)

.home_todo_list p
  width: 85%

.list_playicon
  opacity: 0.5

/* right_list */
.col-15
  top: 0px
  left: 0px
  width: 1100px
  height: 800px
  background: #fff
  opacity: 0.8

.pomodo_title
  width: 1100px
  height: 125px
  display: flex
  justify-content: center
  align-items: center

.pomodo_title_group
  width: 800px
  height: 100px
  display: flex
  justify-content: space-between
  align-items: center

.pomodo_title_group p
  width: 200px
  font: normal normal bold 30px/39px Roboto
  color: #FF4040

.pomodo_content
  height: 675px
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center

.right_label_title
  width: 800px
  height: 40px
  display: flex
  flex-direction: row
  justify-content: space-between
  align-items: center
  background: #FF4040

.right_label_title p
  padding-left: 25px
  font: normal normal normal 20px/26px Roboto
  color: #fff

.right_todo
  height: 284px
  overflow: auto

.right_todo::-webkit-scrollbar-track
  -webkit-box-shadow: inset 0 0 6px rgba(0, 0, 0, 0.3)
  background-color: #ffc0cb

.right_todo::-webkit-scrollbar
  width: 6px;
  background-color: black;

.right_todo::-webkit-scrollbar-thumb 
  background-color: #bb6573;

/* todo css */
.todo_list_text
  width: 500px
  height: 40px
  box-sizing: border-box
  border: 3px solid #998477
  background-color: transparent
  font: normal normal normal 20px/26px Roboto
  padding: 15px 22px
  margin: 10px 5px
  color: #998477
  opacity: 0.8

.todo_list_text::placeholder
  color: #998477
  opacity: 0.5

.todo_collapse_todo
  top: 180px
  height: auto
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center

.todo_collapse_todo_list
  width: 800px
  display: flex
  flex-direction: row
  justify-content: space-between
  align-items: center
  padding-top: 10px
  padding-bottom: 10px
  border-bottom: 3px solid rgba(153,132, 119, 0.5)

.todo_collapse_todo_list p
  width: 85%
  color: #998477
  font: normal normal normal 20px/26px Roboto

.todo_list_playicon
  opacity: 0.5

.todo_collapse_done
  height: 375px
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center

.todo_collapse_done_list
  width: 800px
  display: flex
  flex-direction: row
  justify-content: space-between
  align-items: center
  padding-top: 10px
  padding-bottom: 10px
  border-bottom: 3px solid rgba(153,132, 119, 0.5)

.todo_collapse_done del
  width: 80%
  color: #998477
  padding-left: 10px
  font: normal normal normal 20px/26px Roboto

.todo_uppand_img
  padding-right: 10px

.circle_group
  display: flex
  flex-direction: row-reverse

.circle
  padding-left: 15px

/* ringtons css */
.ringtones_work
  height: 340px
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center

.ringtones_break
  height: 340px
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center

.radio_list
  height: 290px
  width: 800px
  padding-top: 30px
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: flex-start

.radio_group
  width: 800px
  display: flex
  flex-direction: row
  flex-wrap: wrap
  justify-content: space-between
  align-items: flex-start

.radios
  width: 160px
  height: 60px
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center

/* analytics css */
.analytics_focustime
  height: 260px
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center

.analytics_focustime_content
  display: flex
  flex-direction: column
  justify-content: flex-start
  align-items: center

.analytics_focustime_content_title
  width: 800px
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center

.today,
.week
  padding-top: 45px
  width: 400px
  text-align: left
  font: normal normal normal 20px/26px Roboto
  color: #998477
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center

.analytics_focustime_content_data
  width: 800px
  height: 150px
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center

.today_data
  width: 400px
  height: 150px
  text-align: left
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center

.week_data
  width: 400px
  height: 150px
  text-align: left
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center

.tomato_num
  text-align: left
  height: 150px
  font: normal normal normal 80px/26px Roboto
  color: #FF6F61
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center

.tomato_name
  text-align: left
  padding-top: 30px
  height: 120px
  font: normal normal normal 20px/26px Roboto
  color: #707070
  display: flex
  flex-direction: row
  justify-content: flex-start
  align-items: center

.analytics_chart
  height: 435px

.apexcharts-menu-icon
  display: none

.right_chart
  display: flex
  flex-direction: row
  justify-content: center
  align-items: center

.apexcharts-xaxis
  display: none

.apexcharts-series path
  fill: #998477

.apexcharts-datalabels
  display: none

.apexcharts-text
  fill: #998477

.year
  width: 800px
  display: flex
  justify-content: center
  align-items: flex-start

.year p
  color: #998477
  font: normal normal medium 14px/19px Roboto