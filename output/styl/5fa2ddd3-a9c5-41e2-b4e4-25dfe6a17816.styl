plomo = #CCCCCC
azul = #156FEA
gris= #666666
gris_azulino = #3E4E5F
gris_oscuro = #444444
verde = #A8DA00

body
    padding: 5px
    background-color: #B5D31C
    font-size: 16px
    font-family: 'PT Sans Narrow', sans-serif
    background url(fondo.jpg) no-repeat center center fixed

.titulo-cuadro
    background-color: #157CF5
    border: 1px solid gris_azulino
    box-shadow 0 2px 2px gris_azulino
    color: #FFFFFF !important
    font-size: 20px
    font-weight: normal
    height: 25px
    line-height: 25px
    margin-bottom: 3px
    margin-top: 10px
    padding-bottom: 5px
    padding-top: 5px 
    position relative
    width 100%

    &:before
        content "\e606"
        font-size 1.5rem
        font-family "icomoon"
        margin 0 5px 0 7px
        vertical-align bottom

.contenido
    background-color: #FFFFFF
    border: 1px solid gris_azulino
    box-shadow 0 2px 2px gris_azulino
    font-size: 16px
    width 100%
    margin-bottom 20px


    .titulo1
        background-color: #666666
        color: white
        font-size: 18px
        padding-bottom: 10px
        padding-left: 5px
        padding-top: 5px
        text-align: left
        vertical-align: middle

        &:before
            content "\e60a"
            display inline-block
            font-size 25px
            color white
            font-family "icomoon"
            margin 0 0 0 5px
            vertical-align bottom

    .indicadores
        padding 5px 5px 5px 5px
        overflow hidden

        .ind_viajes
            background-color #F0F0F0
            width 100%
            margin-bottom 5px

            .datos_viajes
                width 66%
                margin 0
                padding 0
                background #F0F0F0
                display inline-block
                    
                .cant_viajes
                    color #545454
                    font-size 40px
                    vertical-align top
                    display inline-block
                    margin 5px 5px 0 15px
                    padding 0
                //  background-color green

                .viajes
                    color azul
                    //font-weight bold
                    display inline-block
                    margin 25px 0 0 5px 
                    padding 0
                    vertical-align middle
                //  background-color black

            .flecha
                width 30%
                display inline-block
                margin 15px 0 0 0
                padding 0
                vertical-align top
                text-align right
                
                &:before
                    color azul
                    content "\e602"
                    font-size: 20px;
                    font-family: "icomoon";

            .percent
                background-color #F0F0F0
                font-size 20px
                color azul
                display block
                margin 0 18px 0 0
                text-align right
                padding 0 5px 5px 0

            .tabla_carga
                height 5px
                margin 0
                padding 0
                border none
                width 100%
                position relative

                .tabla_incompleto
                    background-color plomo
                    border none
                    display inline-block
                    float left
                    height 5px
                    margin 0
                    padding 0
                    width calc(100% - 38.7%)

                .tabla_completo
                    background-color azul
                    border none
                    display inline-block
                    float left
                    height 5px
                    margin 0
                    padding 0
                    width calc(38.7%)
        .ind
            width 32.55%
            display inline-block
            background #F0F0F0
            margin 0    
            color azul

            .nombre_tipo
                background #F0F0F0
                margin 5px 0 5px 8px

            .cant
                text-align center
                font-size 30px
                width 50%
                display inline-block

            .porc_tipo
                width 40%
                display inline-block
                text-align right
                font-size 1em
                margin-bottom 10px

            .tabla_carga
                height 5px
                margin 0
                padding 0
                border none
                width 100%
                position relative

                .tabla_incompleto
                    background-color plomo
                    border none
                    display inline-block
                    float left
                    height 5px
                    margin 0
                    padding 0
                    width calc(100% - 38.7%)

                .tabla_completo_terminados
                    background-color verde
                    border none
                    display inline-block
                    float left
                    height 5px
                    margin 0
                    padding 0
                    width calc(38.7%)                

                .tabla_completo_enreparto
                    background-color azul
                    border none
                    display inline-block
                    float left
                    height 5px
                    margin 0
                    padding 0
                    width calc(38.7%) 
                
                .tabla_completo_porRealizar
                    background-color azul
                    border none
                    display inline-block
                    float left
                    height 5px
                    margin 0
                    padding 0
                    width calc(38.7%) 

