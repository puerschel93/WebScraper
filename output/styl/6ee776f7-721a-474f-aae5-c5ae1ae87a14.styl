@import 'common.styl'

.wrapper
    /*padding-top 1em;*/
    margin 0 auto
    width 44em
ul
    list-style-type none
    padding-left 0

li
    padding 1em
    margin-bottom 0.5em
    /*background-color #aeaeae;*/
    box-shadow .1em -.1em 0 .1em rgba(0, 0, 0, .2), inset 0 0 0 .15em rgba(0, 0, 0, .4), -.1em .1em 0 .1em rgba(0, 0, 0, .2)

    border-radius 0.5em
    text-align center
a
    text-decoration none

.gap
    padding 0
    box-shadow none
.gap__inner
    height 5px
    background-color #aeaeae
    border-radius 0.1em
