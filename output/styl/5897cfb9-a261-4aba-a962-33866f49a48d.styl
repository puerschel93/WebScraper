	line-height 19px
	display inline-flex
	align-items center
	justify-content center
	border-radius 30px
	background-color #e3e3e3
	cursor pointer
	transition .3s
#vp
	font-family $SemiBold
	color $white
	font-size 1.5rem
	overflow hidden
	border none
	position relative
	line-height 19px
	display inline-flex
	align-items center
	justify-content center
	border-radius 30px
	background-color $green
	cursor pointer
	transition .3s
	&:hover
		box-shadow inset 0 0 15px #00ddb5
		color $white
		background-color $green
.rd
	border 2px solid $green

.qst
	line-height 32px
	font-size 1.5rem
	font-family $SemiBold
	color $black
	margin-bottom 50px
	display block
.qst2
	margin-bottom 0
.plv:nth-child(2)
	margin-right 3%
.kn-nav
	margin-top 10px
	display flex
	justify-content space-between
.em
	width 100%
	box-shadow 0 0 13.5px 1.5px rgba(0, 190, 156, 0.15)
	padding 12px 20px
	font-family $Regular
	line-height 22px
	margin-right 1%
	border: 1px solid $white
	font-size 1rem
	border 1px solid $green
	margin-bottom 15px
.blocktest
	width 33%
	display inline-flex
	flex-wrap wrap
	padding 30px
	&__title
		line-height 24px
		font-size 1.125rem
		font-family $SemiBold
		color $black
		margin-bottom 10px
		width 100%
	&__text
		background-color $green
		padding 10px
		color $white
		font-family $SemiBold
		line-height 24px
		font-size 1.125rem
		position relative
		&-image
			position absolute
			right -10px
			top -10px
			border-radius 50%
			background-color #e3e3e3
			width 20px
			height 20px
			display flex
			align-items center
			justify-content center
			i
				color $green
				font-size 15px
	&:nth-child(2)
		background-color $white
		border-left 2px solid $green
		margin-left 16.66667%
	&:nth-child(3)
		z-index 2
		background-color $white
		box-shadow 0 0 10px rgba(0,0,0,0.2)
		& label
			width 100%
		& label:nth-child(3) > span:nth-child(2)
			margin-bottom 0
			margin-top 0
		span,
		a
			padding-top 15px
			line-height 17px
			font-size 0.675rem
		a
			display inline-block
			margin 0 3px

.snd
	padding 14px 32px
	font-family $Bold
	color $green
	font-size 1.5rem
	overflow hidden
	width 100%
	border-left none
	border-top none
	border-right none
	position relative
	line-height 19px
	display inline-flex
	align-items center
	justify-content center
	border-radius 30px
	background-color #e3e3e3
	cursor pointer
	transition .3s
	border-bottom 3px solid $green
	&:hover
		box-shadow inset 0 0 15px #00ddb5
		color $white
		background-color $green
	&:after
		content: ""
		position: absolute
		top -50%
		left -50%
		right -50%
		bottom -50%
		background linear-gradient(to bottom,rgba(229,172,142,0),rgba(255,255,255,.7) 50%,rgba(229,172,142,0))
		transform rotateZ(60deg) translate(-5em,7.5em)
		opacity 0
		animation sheens 2.5s infinite
		z-index 100
.stp
	margin-bottom 50px
.radio
	display none
.radio-custom
	width 20px
	height 20px
	border-radius 3px
	border 2px solid #ededed
	background $white
	position relative
	margin 15px 15px 15px 0
	transition .3s
	cursor pointer
.radio-custom:hover
	border-color $green
.radio-custom
	display inline-block
	vertical-align middle
	cursor pointer
.radio:checked + .radio-custom
	border-color $green
	background $white
.radio:checked + .radio-custom::before
	content ""
	display block
	position absolute
	top 0
	right 0
	bottom 0
	left 0
.radio:checked + .radio-custom::after
	content ""
	display flex
	align-items center
	justify-content center
	width 12px
	height 12px
	background-color $green
	position absolute
	left 2px
	top 2px
	border-radius 50%
.radio-custom
	border-radius 50%
@media(max-width: 991px)
	.plv,
	.trt,
	.plv2,
	.em,
	.snd,
	.blocktest
		width 100%
	.em
		margin-bottom 10px
	.blocktest:nth-child(2)
		margin-left 0
	.blocktest:nth-child(3)
		margin-bottom 15px
	#slide1
		border none
