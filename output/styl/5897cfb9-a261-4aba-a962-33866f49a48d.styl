#questions
	width 100%
	height 100%
	background url(/images/agsquare.png) #f0f0f0 100% 100%
.questions
	display flex
	min-height 525px
	&__block:nth-child(1)
		width 80%
		position absolute
		top -62px
		left 10%
		padding 30px
		background-color $white
		border-radius 5px
		display flex
		box-shadow 0 0 10px rgba(0,0,0,.2)
	&__block:nth-child(2)
		flex-basis 100%
		display flex
		align-items center
	&__block
		&-title
			line-height 32px
			font-size 1.75rem
			color $green
			font-family $Regular
			position relative
			flex-basis 50%
			display flex
			align-items center
			p
				padding-left 10px
			&-img
				width 50px
				height 50px
			&:after
				content ''
				position absolute
				bottom calc(50% - 22.5px)
				display inline-block
				right 30px
				background-color $green
				height 45px
				width 2px
				transform rotate(17deg)
		&-desc
			font-size 1.125rem
			line-height 28px
			flex-basis 50%
			p
				color $text
				font-family $Light
				display inline-block
				span
					margin-left 3px
					color $white
					font-family $SemiBold
					background-color $green
					padding 0 5px
		&-test
			margin 50px auto 66px 96px
			&-title
				line-height 48px
				font-size 2.625rem
				font-family $SemiBold
				color $black
				max-width 70%
				margin-bottom 34px
			&-button
				box-shadow 0 0 13.5px 1.5px rgba(0, 190, 156, 0.15)
				padding 8px 20px
				background-color $green
				font-family $Regular
				line-height 26px
				color $white
				border-radius 3px
				cursor pointer
				transition .3s
				width 176px
				text-align center
				display inline-block
				margin-bottom 43px
				&:hover
					background-color $green - 20%
			&-result
				display flex
				align-items center
				position relative
				width 380px
				&-line
					height 3px
					width 100%
					background-color $green
					opacity .3
				&-linedone
					height 3px
					width 0%
					background-color $green
					position absolute
					left 0
				&-numbers
					margin-left 18px
					display inline-block
					span
						font-family $SemiBold
						color #bfbfbf
						font-size 1.125rem
			&-button:nth-child(3)
				margin-left 20px
@media(max-width: 1080px)
	.questions
		&__block
			&-title
				&:after
					display none
@media(max-width: 991px)
	#questions
		border-top 3px dashed rgba(0,190,156,.3)
		border-bottom 3px dashed rgba(0,190,156,.3)
	.questions
		padding-bottom 20px
		flex-wrap wrap
		&__block
			&:nth-child(1)
				flex-basis 100%
				flex-direction column
				width 90%
				left 5%
		&__block
			&:nth-child(2)
				flex-basis 100%
				background none
				padding-top 122px
		&__block
			&-title
				max-width 100%
				p
					display inline-block
					font-size 1.375rem
			&-desc
				max-width 100%
			&-test
				margin 0
				padding 30px 20px
				display flex
				flex-wrap wrap
				justify-content space-between
				&-title
					max-width 100%
					flex-basis 100%
				&-button
					width auto
				&-result
					width auto
					flex-basis 100%
@media(max-width: 414px)
	.questions
		&__block
			&:nth-child(2)
				padding-top 170px
@media(max-width: 350px)
	.questions
		&__block
			&-test
				&-button
					flex-basis 100%
					&:nth-child(2)
						margin-bottom 15px
					&:nth-child(3)
						margin-left 0
.hd
	display none

.ui-widget-content
	border 1px solid #bdc3c7
	background #e1e1e1
	color #222222
	margin-top 4px
.ui-slider .ui-slider-handle
	width 0px
	height 0px
	position absolute
	top -11px
	margin-left -10px
	z-index 200
	border 0
	-background-size: 30px
	pointer-events auto

.ui-slider .ui-slider-handle .glyphicon
	color #FFFFFF
	margin 0 3px
	font-size 11px
	opacity 0.5

.ui-corner-all
	border-radius 20px

.ui-slider-horizontal .ui-slider-handle
	top -1px

.ui-slider-horizontal .ui-slider-handle
	margin-left -0.5em

.ui-slider .ui-slider-handle
	cursor pointer

.ui-state-default
	display none

.ui-slider a,
.ui-slider a:focus
	cursor pointer
	outline none

.ui-slider-range
	border 1px solid #f9f9f9
	background $green !important
	margin-top -0.2px
.ui-widget
	pointer-events none
	font-size .7em !important
.slider
	height 10px
	background-color #f2f2f2
	position relative
	margin-left 0px
	padding 0 10px
	margin-top 5px
	margin-bottom 15px
#skd
	color $green
	margin-left 5px
#ty
	position absolute
	width 100%
	font-family $SemiBold
	color $black
	text-align center
	bottom 0
	& span
		margin-left 3px
		color $green
		font-family $SemiBold
@media(max-width:700px)
	#ty
		bottom 100px
.sld
	width 100%
	font-family $SemiBold
	color #bfbfbf
	font-size 1.125rem
	position relative
.plv
	padding 0px 20px
	background-color $white
	font-family $Regular
	line-height 26px
	color $black
	border-radius 3px
	cursor pointer
	border 2px solid #bfbfbf
	width 47%
	text-align center
	display inline-block
	margin-bottom 10px
	box-shadow 0 0 13.5px 1.5px rgba(0, 190, 156, 0.15)
	&:hover
		border 2px solid $green
	&:hover .radio-custom
		border-color $green
	&:hover .radio-custom::after
		content ""
		display flex
		align-items center
		justify-content center
		width 12px
		height 12px
		background-color $green
		position absolute
		left 2px
		top 2px
		border-radius 50%
.plv2
	padding 0px 20px
	background-color $white
	font-family $Regular
	line-height 26px
	color $black
	border-radius 3px
	cursor pointer
	border 2px solid #bfbfbf
	width calc(25% - 4px)
	text-align center
	display inline-block
	margin-bottom 10px
	box-shadow 0 0 13.5px 1.5px rgba(0, 190, 156, 0.15)
	&:hover
		border 2px solid $green
	&:hover .radio-custom
		border-color $green
	&:hover .radio-custom::after
		content ""
		display flex
		align-items center
		justify-content center
		width 12px
		height 12px
		background-color $green
		position absolute
		left 2px
		top 2px
		border-radius 50%
.trt
	width 32%
	padding 0px 20px
	background-color $white
	font-family $Regular
	line-height 26px
	color $black
	border-radius 3px
	cursor pointer
	text-align center
	display inline-block
	border 2px solid #bfbfbf
	margin-bottom 10px
	font-size: 1.125rem;
	box-shadow 0 0 13.5px 1.5px rgba(0, 190, 156, 0.15)
	&:hover
		border 2px solid $green
	&:hover .radio-custom
		border-color $green
	&:hover .radio-custom::after
		content ""
		display flex
		align-items center
		justify-content center
		width 12px
		height 12px
		background-color $green
		position absolute
		left 2px
		top 2px
		border-radius 50%
.qu_t1 > div:nth-child(2),
.qu_t1 > div:nth-child(3)
	margin-right 1%
.kn
	border 1px solid grey
	border-radius 10px
	height 40px
	width 120px
	align center
	display inline-block
	line-height 40px
	margin-top 10px
	text-align center
	cursor pointer
#bck
	font-family $SemiBold
	color $green
	font-size 1.5rem
