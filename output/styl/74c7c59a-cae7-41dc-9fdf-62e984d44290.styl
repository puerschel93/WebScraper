.app_Pread_page
  position absolute
  top 0
  width 100%
  height 100%
  background #fff
  z-index 1000
  transition all 0.3s ease
  .link-detail
    width 100%
    box-sizing border-box
    padding-left 30px
    padding-right 30px
    position relative
    overflow-y auto
    .link_header
      width 100%
      margin 20px 0
      .acc_h_d1
        width 100%
        p
          width 100%
          height 35px
          line-height 35px
        & .span1
          float left
          font-family PingFang-SC-Medium
          font-size 14px
          color #4A4A4A
        & .mt-radio
          float left
          height 35px
          line-height 35px
          .mint-radiolist-title
            display none
          .mint-cell
            float left
            height 35px
            line-height 35px
            min-height 0
            .mint-cell-wrapper
              background 0
              height 100%
              box-sizing border-box
              padding 0
              .mint-radiolist-label
                font-family PingFang-SC-Medium
                font-size 14px
                color #4A4A4A
          .mint-cell:last-child
            background 0
    .ipt_d2
      width 100%
      .acc_h_d2
        width 100%
        margin-top 5px
        margin-bottom 5px
        position relative
        .mint-cell:last-child
          background 0
          border 1px solid #C1BFC2
          border-radius 5px
          .mint-cell-wrapper
            background 0
        & input.ipt_account
          display inline-block
          width 100%
          height 48px
          outline 0
          box-sizing border-box
          border 1px solid #aeaeae
          border-radius 5px
          font-size 15px
          padding-left 10px
        & input.ipt_account:focus
          border-color #3879d9
          box-shadow inset 0 1px 2px rgba(0, 0, 0, 0.075), 0 0 5px rgba(81, 167, 232, 0.5)
      & .down_arrow
        position absolute
        width 22px
        height 22px
        top 15px
        right 10px
        z-index 1000
      .acc_h_d3
        width 100%
        .mt_box
          height 35px
          line-height 35px
          margin-bottom 5px
        & .span1
          float left
          font-family PingFang-SC-Medium
          font-size 14px
          color #4A4A4A
          height 35px
          line-height 35px
        & .mt-checklist
          width 235px
          float left
          height 35px
          line-height 35px
          .mint-checklist-title
            display none
          .mint-cell
            float left
            height 35px
            line-height 35px
            min-height 0
            .mint-cell-wrapper
              background 0
              height 100%
              box-sizing border-box
              padding 0
              .mint-checkbox-label
                font-family PingFang-SC-Medium
                font-size 14px
                color #4A4A4A
              & .mint-checkbox-core
                border-radius 25%
          .mint-cell:last-child
            background 0
            width 45%
        & .code
          margin-bottom 10px
          position relative
          .code_img
            width 125px
            height 34px
            position absolute
            right 10px
            top 8px
            img
              width 100%
              height 100%
          input
            height 44px
            border none
            outline none
            font-size 14px
            border-bottom 1px solid #eee
      .acc_h_d4
        width 100%
        margin-top 15px
        .mint-cell:last-child
          background 0
          border 1px solid #C1BFC2
          border-radius 5px
          .mint-cell-wrapper
            background 0
    .accurate_content
      width 100%
      margin-top 10px
      .con-min_max
        width 100%
        height 35px
        line-height 35px
        p
          float right
          height 35px
          line-height 35px
          width 150px
          text-align right
          position relative
          & .mt-radio
            width 75px
            float left
            height 35px
            line-height 35px
            .mint-radiolist-title
              display none
            .mint-cell
              float left
              // width 95px
              height 35px
              line-height 35px
              min-height 0
              .mint-cell-wrapper
                background 0
                height 100%
                box-sizing border-box
                padding 0
                .mint-radiolist-label
                  font-family PingFang-SC-Medium
                  font-size 14px
                  color #4A4A4A
            // padding 0
            .mint-cell:last-child
              background 0
      .d1-header
        width 100%
        height 35px
        line-height 35px
        .p1
          font-family PingFang-SC-Medium
          font-size 14px
          color #4A4A4A
          float left
          height 35px
          line-height 35px
        .p2
          float right
          height 35px
          line-height 35px
          .span1
            display inline-block
            width 22px
            height 22px
            line-height 20px
            background #7ED321
            border-radius 50%
            text-align center
            font-size 22px
            font-weight bold
            color #FFF
            font-family PingFang-SC-Medium
            margin-right 8px
          .span2
            display inline-block
            width 22px
            height 22px
            line-height 20px
            background #EA3826
            border-radius 50%
            text-align center
            font-size 22px
            font-weight bold
            color #FFF
            font-family PingFang-SC-Medium
      .mt-range
        width 100%
        .slot-start
          width 35px
          font-family PingFang-SC-Medium
          font-size 14px
          color #4A4A4A
        .mt-range-content
          // width 150px
          // display block
        .slot-end
          width 35px
          text-align right
          margin-left 12px
    .part2
      margin-top 45px
      position relative
      margin-bottom 80px
      .real_ss
        width 100%
        height 100%
        line-height 45px
        display block
        border-radius 25px
        font-size 15px
        background-color #6B89FE
        text-align center
        color #fff
        outline 0
        border none
        touch-action none
        position relative
        overflow hidden
      .real_ss:after
        content ''
        display block
        position absolute
        width 100%
        height 100%
        top 0
        left 0
        border-radius 25px
        pointer-events none
        background-image radial-gradient(circle, #767676 10%, transparent 10.01%)
        background-repeat no-repeat
        background-position 50%
        transform scale(10, 10)
        opacity 0
        transition transform 0.3s, opacity 0.5s
        height 45px
        line-height 45px
        padding-right 15px
      .real_ss:active:after
        transform scale(0, 0)
        opacity 0.3
        border-radius 25px
        transition 0s
    .acc2_content
      .part2
        margin-top 265px
    .pop_con
      width 100%
.appCheckRadios
  .radio_appTuiguang
    margin-left 10px
    .radio
      margin 0px 5px
    label
      line-height 10px
      display inline-block
.appCheckRadios .radio_appTuiguang input[type=radio]:after, .radio_min_max .radio_minRadio input[type=radio]:after
  background #fff
.radio_min_max
  float right
  .radio_minRadio
    margin-left 10px
    .radio
      margin 5px
.mint-checklist-label
  padding 0
