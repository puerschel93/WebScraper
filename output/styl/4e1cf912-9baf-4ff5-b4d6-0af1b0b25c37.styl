	.line_two
		background #fff
		height 623px
		text-align center
		.responsive
			margin-top 20px
			display inline-block
			width 1110px
			@media $medium-small
				width 740px
				@media $small
					width 370px
			.slick-slide
				position relative
				margin 0 35px
				&.ite
					width 300px
					text-align center
					.status
						position absolute
						// margin-left -2px
						width 100%
						height 39px
						// background url(imgs+'status.png') no-repeat
						color black
						text-align left !important
						padding 4px 0 0 5px
						font-size 16px
						font-weight bold
					img
						width 100% !important
					// margin 0 -1px
				h4.name
					color #0077bd !important
					// text-transform uppercase
					position relative
					padding-bottom 1em
					border-bottom 1px solid #0077bd
					font-size 1.4em
					font-weight bold
					@media $small
						font-size 1.7em
				h5.distrito
					// color #959595 !important
					color #000 !important
					text-transform capitalize
					font-size 1.3em
			.slick-slide:focus
				outline 0
			.slick-dots
				display none !important
			.slick-arrow
				position absolute
				top 52%
				width 17px
				height 30px
				z-index 2
				background-color transparent
				color transparent
				border 0
				// opacity .9
				cursor pointer
				&.slick-prev
					left -50px
					background-image url(imgs+'arrow_left.png')
					@media $small
						left 15px
				&.slick-next
					right -50px
					background-image url(imgs+'arrow_right.png')
					@media $small
						right 15px
				// width 50%
				// padding 0 25%





		
	.servicios
		text-align left
		overflow hidden
		&:before
			content ""
			width 100%
			height 950px
			background url('http://empresas.adimra.org.ar/files/productos/RFpWK4YY/DSC_7354.jpg') center center no-repeat;
			opacity 0.2
			// bacgkround-size 50% 50%
			position absolute
		> .container > .row > .col
			padding 0 40px
			@media $large
				min-height 411px
			.card
				transition border-radius .5s, box-shadow .5s, background-color .5s, margin .2s
				box-shadow 0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12)
				background-color darken(white,5)
				border-radius 15px
				@media $large
					.card-image
						height 180px
						overflow hidden
					.card-content
						height 200px
						overflow hidden
				&:hover
					box-shadow 0 12px 15px 0 rgba(0,0,0,0.24),0 17px 50px 0 rgba(0,0,0,0.19)
					background-color white
					border-radius 0
					@media $large
						margin -2px
				@media $small
					box-shadow none
					background-color white
					border-radius 0
					&:hover
						box-shadow none
					
