.roundedOne {
    width: 30px;
    height: 30px;
    position: relative;
    border-radius: 4px;
    border: 0;
    background-color: #fff;
}

    .roundedOne input[type=checkbox] {
        visibility: hidden;
    }

    .roundedOne label:after {
        content: '';
        width: 26px;
        height: 26px;
        position: absolute;
        top: 2px;
        left: 2px;
        background: #00ADEF;
        opacity: 0;
        border-radius: 3px;
    }

    .roundedOne label:hover::after {
        opacity: 0.3;
    }

    .roundedOne input[type=checkbox]:checked + label:after {
        opacity: 1;
    }

.greySelectbox select {
