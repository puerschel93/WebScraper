@import "../css/mixins.styl"

#app
  height 100%
  width 100%
  .header-top
    height 150px
    width 100%
    position fixed
    top 0
    left 0
    z-index 10
    transform-style preserve-3d
    .sticky-wrapper
      height 150px
      width 100%
      transition all .5s
      &.on
        animation rotate .5s
      .container
        width 100%
        height 50px
        box-sizing border-box
        padding 0 100px
        .position
          float left
          line-height 50px
          padding 0 20px
          &hover
            background white
          img
            display inline-block
            line-height 50px
          b
            font-size 16px
            font-weight bold
          span
            font-size 16px
            color #898989
        .navbar-nav
          float right
          height 50px
          li
            padding 0 15px
            float left
            line-height 50px
            &hover
              background white
            &.active
              a
                color red
            a
              color #000
              font-size 16px
              font-weight bold
      .container-bottom
        height 100px
        padding 0 100px
        width 100%
        box-sizing border-box
        background white
        clearFix()
        .navbar-header
          position absolute
          left 100px
          top 50%
          z-index 5
          transform translateY(-50%)
          a
            img
              width 280px
        .search
          position absolute
          top 50%
          left 50%
          transform translate(-50%,-50%)
          width 600px
          height 38px
          line-height 34px
          border 2px solid #e94840
          background-color #fff
          border-radius 4px
          font-size 14px
          .glyphicon
            position relative
            top 1px
            display inline-block
            font-family 'Glyphicons Halflings'
            font-style normal
            font-weight 400
            line-height 1
            -webkit-font-smoothing antialiased
          input
            width 70%
            border none
            height 30px
            outline none
            padding-left 30px
          .searchbtn
            width 20%
            text-align center
            background-color #e94840
            color #fff
            float right
            height 38px
            cursor pointer
  .carousel
    width 100%
    height 350px
    margin-top 150px
    .full_banner
      width 100%
      height 350px
      position relative
      .swiper-container
        width 100%
        height 350px
        .swiper-wrapper
          height 350px
          width 200%
          .swiper-slide
            height 350px
            float left
            img
              width 100%
              height 100%
      .ulleft
        position absolute
        left 200px
        top 50%
        transform translateY(-50%)
        width 360px
        height 342px
        a
          width 180px
          height 34px
          text-align center
          display block
          color white
          font-weight 700
          line-height 34px
          background #757881
          &:hover
            background rgba(0,0,0,.2)
        .ulright
          position absolute
          left 180px
          top 50%
          transform translateY(-50%)
          width 180px
          height 342px
          ul
            width 100%
            li
              width 100%
              height 34px
              text-align center
              a
                background  white
                display block
                color black
                font-weight 700
                height 100%
                line-height 34px
                &:hover
                  background rgba(0,0,0,.6)
  .content
    .box1
      clearFix()
      .fff
        width 1200px
        margin 0 auto
        padding 50px 0 0
        clear both
        .servicebody
          box-sizing border-box
          .navigation
            text-align center
            span
              font-size 22px
              display block
              font-family Arial, Helvetica, sans-serif
              a
                color #3d3838
                margin-left 20px
              &.serviceIndex
                color #3d3838
                font-weight 800
                font-size 28px
                font-family "Microsoft YaHei"
                margin-bottom 20px
          .servicetags
            box-sizing border-box
            .tagsListHeader
              text-align right
              .lookMore
                position relative
                bottom 22px
                a
                  font-size 18px
            .row
              margin-top 10px
              margin-left 0
              margin-right 0
              &:before
                display table
                content " "
              .form-group
                float left
                width 230px
                position relative
                margin-bottom 0
                text-align center
                margin-left 10px
                .member-container
                  float left
                  padding 10px
                  width 230px
                  border 1px solid #eaeaea
                  margin 0 5px
                  background #fff
                  cursor pointer
                  height 345px
                  img
                    vertical-align middle
                  .serviceName
                    font-size 16px
                    padding 10px 0
                    font-weight 800
                    color #000
                    height 28px
                    overflow hidden
                    width 101%
                    white-space nowrap
                    text-overflow ellipsis
                  .description
                    width 200px
                    color #929292
                    padding 5px 0
                    overflow hidden
                    text-overflow ellipsis
                    display -webkit-box
                    -webkit-line-clamp 2
                    -webkit-box-orient vertical
                    word-break break-all
                    font-size 13px
                    line-height 18px
                    height 45px
                  .price
                    font-size 14px
                    padding 5px 5px
                    font-weight 600
                    height 20px
                    line-height 20px
                    color red
                    .btn
                      float right
                      padding 0
                      width 76px
                      height 24px
                      line-height 22px
                      text-align center
                      background #fb474f
                      border-radius 5px
                      color #fff
                      font-size 14px
                    span
                      color red
                      float left
                      font-size 20px
              &:hover
                box-shadow 5px 5px 15px #ccc
  .supply
    padding-top 60px
    width 100%
    text-align center
    background #fff
    .supplyTil
      font-size 30px
      font-family "微软雅黑"
      font-weight 800
      margin 20px 0 10px 0
      span
        color #e94840
        font-weight 800
    .supplyDes
      margin 0
      font-size 22px
      color #3d3838

  .service
    width 100%
    padding-top 20px
    background #fff
    height 66px
    font-family "微软雅黑"
    box-sizing border-box
    .serList
      width 420px
      margin 0 auto
      .serItem
        float left
        width 33%
        img
          vertical-align middle
          margin-right 10px
  .footer
    width 1200px
    height 170px
    box-sizing border-box
    margin 0 auto
    dl
      width 100%
      height 100%
      display flex
      justify-content space-between
      dt
        width 100px
        font-family " 微软雅黑"
        font-size 18px
        font-weight bold
        margin-left 85px
        position relative
        bottom 20px
        text-align left
        background none
        color #1d1515
      dd
        width 1090px
        height 94px
        a
          display inline-block
          width 8%
          height 30px
          font-size 16px
          color #898989
          text-aligncenter
          font-weight200
          cursorpointer
          font-family"微软雅黑"
          text-align center
          line-height 30px


  .beian
    width 100%
    margin 0 auto
    p
      position relative
      left 80px
      bottom 20px
      font-weight bold
      width 1200px
      margin 0 auto
      background none
      color #1d1515
      font-size 18px
    .hezuo-list
      width 1200px
      margin 0 auto
      clearFix()
      li
        float left
        width 10%
        height 60px
        text-align center
        background #fff
        margin-left 1%
        line-height 55px
        cursor pointer
        img
          vertical-align middle
          border none
  .copyRight
    height 40px
    line-height 40px
    background #fbf8f8
    .copyTil
      width 1200px
      margin 0 auto
      font-size 14px
      color #929292
@keyframes rotate {
  0% {
    transform translateY(-100%) rotateX(90deg)
    opacity .4
  }
  100% {
    transform translateY(0) rotateX(0deg)
    opacity 1
  }
}