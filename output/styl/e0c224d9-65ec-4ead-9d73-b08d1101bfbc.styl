.propose_price
    display: flex
    justify-content: center
    align-items: center
    background: #FFF5EB
    font-size: 12px
    color: #333
    padding: 10px 0
    label
      color #999
      margin-right: 10px
    span + span
      margin-left: 30px


  .pro_mes_box
    background: #F9F9FB
    padding: 16px
    font-size: 12px
    color: #333
    position: relative
    .pro_mes+.pro_mes
      margin-top: 10px
    .name
      color: #999999
      margin-right: 20px
      // margin-bottom: 10px
      min-width:50px
      display: inline-block
    .info
      margin-top: 5px
    .tag
      margin-right: 20px

  .pro_server_box
    background: #fff
    border-bottom: 1px solid #f2f3f5
    .pro_mes
      display: flex
      
  .pro_size_box
    border: 0
    .pro_mes
      align-items: center
    .pro_mes+.pro_mes
      margin-top: 15px

.product_radio
  :global
    .ant-radio-wrapper
      border: 1px solid #dadada
      position: relative
      margin-bottom: 10px
      .ant-radio
        display: none
      .ant-radio-inner
        position: absolute
      span.ant-radio + *
        padding: 0
    .ant-radio-wrapper-checked
      border: 1px solid #F9522E
      color: #F9522E
  .item
    padding: 8px 8px
    line-height: 12px
    font-size: 12px

.total_font
  font-size: 16px
.total_font+.total_font
  margin-left: 20px


.bottom_button_box
  display: flex
  align-items: center
  text-align: center
  padding-left: 20px
  margin-top: 20px
  .button_item
    width: 184px
    height 40px
    line-height: 40px
    font-size: 16px
    color: #FFA54B
    border: 1px solid #FFD6AD
    background: #FFF5EB
    cursor:pointer
  .button_item+.button_item
    margin-left: 10px
  .button_item:nth-child(2)
    color: #FF6150
    border: 1px solid #FFABA2
    background: #FEECEA
  .button_item:nth-child(3)
    color: #ffffff
    border: 1px solid #F9522E
    background: #F9522E
  .button_item:nth-child(3).disable_item
    cursor: default
    background: #dedede
    border: 1px solid #dedede

:global
  .ant-calendar-today .ant-calendar-date
    color: #F9522E;
    border-color: #F9522E;
  .ant-calendar-range .ant-calendar-selected-start-date .ant-calendar-date, 
  .ant-calendar-range .ant-calendar-selected-end-date .ant-calendar-date,
  .ant-calendar-range .ant-calendar-selected-start-date .ant-calendar-date:hover, 
  .ant-calendar-range .ant-calendar-selected-end-date .ant-calendar-date:hover,
  .ant-calendar-year-panel-selected-cell .ant-calendar-year-panel-year,
  .ant-calendar-year-panel-selected-cell .ant-calendar-year-panel-year:hover,
  .ant-calendar-month-panel-selected-cell .ant-calendar-month-panel-month,
  .ant-calendar-month-panel-selected-cell .ant-calendar-month-panel-month:hover,
  .ant-calendar-decade-panel-selected-cell .ant-calendar-decade-panel-decade,
  .ant-calendar-decade-panel-selected-cell .ant-calendar-decade-panel-decade:hover
    background: #F9522E
    color: #fff!important
  .ant-calendar-range .ant-calendar-in-range-cell::before,
  .ant-calendar-date:hover,
  .ant-calendar-year-panel-year:hover,
  .ant-calendar-decade-panel-decade:hover,
  .ant-calendar-month-panel-month:hover
    background: #FFECE8 
  .ant-calendar-date:hover,
  .ant-calendar-year-panel-year:hover,
  .ant-calendar-decade-panel-decade:hover,
  .ant-calendar-decade-panel-cell:hover,
  .ant-calendar-month-panel-month:hover,
  .ant-calendar-year-panel-decade-select:hover,
  .ant-calendar-year-panel-decade-select:focus,
  .ant-calendar-year-select:hover,
  .ant-calendar-month-select:hover
     color: #F9522E!important
  // .ant-input:hover, .ant-input:focus,
  .ant-calendar-picker-input:hover,
  .ant-calendar-picker-input:active,
  .ant-calendar-picker-input:visited
    border: 1px solid #F9522E!important
  .ant-calendar-picker:focus .ant-calendar-picker-input:not(.ant-input-disabled)
    border-color: #F9522E;
    box-shadow: 0 0 0 2px #f9522e24
  .ant-btn-primary
    background-color:#F9522E
    border-color: #F9522E 
  .ant-btn:active, .ant-btn:focus, .ant-btn:hover
    border-color:#f9522ecf
    color: #f9522ecf
  .ant-btn-primary:hover, .ant-btn-primary:focus
    background-color:#f9522ecf
    border-color: #f9522ecf 
    color: #fff 
  .ant-input-number-handler-wrap
    display: none
.date_box
  position: relative
  height:300px
.city_box
  display: flex
  :global
    .ant-select
      min-width:100px

.red
  color: #F9522E


.noArea_box
  background: #feecea;
  border: 1px dashed #ffaba2;
  padding: 5px 10px
  color: #999999
  font-size: 12px
  line-height: 20px
  .item
    display: inline
    .title
      margin-right: 5px
      display: inline-block

.disabled
  opacity: 0.4
  :global
    .ant-radio-wrapper-checked
      border-color: #dddddd
      color: #666666
.has_no
  width: 300px
  height: 300px
  position: absolute
  left: 30%
  top: -150px
  z-index: 0
  opacity 0.1