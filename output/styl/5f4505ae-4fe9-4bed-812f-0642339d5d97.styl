@import "nib"
@import "normalize.styl"
@import "iconos.styl"

@font-face {
	font-family: 'montserratbold';
	src: url('../fonts/montserrat-bold-webfont.eot');
	src: url('../fonts/montserrat-bold-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/montserrat-bold-webfont.woff') format('woff'),
		 url('../fonts/montserrat-bold-webfont.ttf') format('truetype'),
		 url('../fonts/montserrat-bold-webfont.svg#montserratbold') format('svg');
	font-weight: normal;
	font-style: normal;

}
@font-face {
	font-family: 'montserratregular';
	src: url('../fonts/montserrat-regular-webfont.eot');
	src: url('../fonts/montserrat-regular-webfont.eot?#iefix') format('embedded-opentype'),
		 url('../fonts/montserrat-regular-webfont.woff') format('woff'),
		 url('../fonts/montserrat-regular-webfont.ttf') format('truetype'),
		 url('../fonts/montserrat-regular-webfont.svg#montserratregular') format('svg');
	font-weight: normal;
	font-style: normal;

}

$verde = #8ac543
$azul = #1175ba

html
	font-family 'Lato', sans-serif
	font-size 100%
body
	background-color #FFF
	height 100%
ul 
	li 
		margin-bottom 10px
footer
	position absolute
	bottom 0
	left 0
	width 100%
	min-height 30em
	background-color #FFF
	text-align: center
	display table

	.services
		
		text-transform uppercase
		font-weight 300
		letter-spacing .1em
		color #737373
		font-size 13px
	#menu_f
		
		ul 
			border-top 1px solid #D8D8D8
			border-bottom 1px solid #D8D8D8
			display inline-block
			padding 2em 0
			margin-top 3em
			margin-bottom 3em


			li
				display inline-block
				margin .5em  
								
				a 
					display block
					font-size 15px
					letter-spacing .2em
					text-decoration none
					color #000
					text-transform uppercase
					
					&:hover
						color $azul


	.social
		
		.empresas
			text-transform uppercase
			font-weight 300
			letter-spacing .1em
			color #737373
			font-size 13px
			margin 0 0 1em
		a 
			text-decoration none
			display inline-block
			margin 0 .2em
			color #000
			font-size 2.5em
			img
				-webkit-filter grayscale(1)
				transition .2s
				opacity .8
				
			&:hover img
				-webkit-filter grayscale(0)
				opacity 1
				-ms-filter none
				filter none
				

	.copyright
		font-size 10px
		line-height 2.3
		color #ccc
		position absolute
		bottom -1em
		left 50%
		width 20em
		margin-left -10em
		text-align center

	

.header
	background-color #FFF
	position relative
	z-index 100
	#logo
		
		display block
		width 140px
		height 78px
		margin-bottom .5em
		float left
	#btn_nav
		position absolute
		right 20px
		top 20px
		font-size 1.5em
		cursor pointer

		.icon-menu
			vertical-align middle
			color $azul
	#menu
		margin-top 90px
		display none
		
		ul 
			margin 0
			padding 0


			li
				display block
				margin 1em
				text-align center
				border 1px solid transparent
				

				a 
					display block
					font-size 1.5em
					letter-spacing .2em
					text-decoration none
					color $azul
					text-transform uppercase
					padding .5em
				&:hover
					border-color $verde
	.social
		position relative
		text-align center
		display none
		

		a 
			text-decoration none
			display inline-block
			margin .4em .2em
			color #000
			font-size 2.5em
			&:hover
				color $azul

#main 
	min-height 22.3em
	padding-right 0
	padding-left 0

.overflow 
	overflow-x hidden
.list-items
	margin 0
	padding 0
	list-style none
	margin-right -2px
	margin-left -2px
	overflow hidden
	background-color #efefef
	
	.item
	li 
		display inline-block
		width 50%
		max-width 480px
		text-align left
		margin-right -4px
		margin-bottom -4px
		position relative
		//height 348px
		img 
			width 100%
			vertical-align middle
		a 
			position: relative;
			top: 0;
			left: 0;
			width: 100%;
			height: 100%;
			//z-index: 3;
			
						
			
		.info
			background-repeat no-repeat
			background-position 90% 85%
			display none//inline-block
			position absolute
			top 0//343px
			left 0
			right 0
			bottom 0
			background-color rgba(0,0,0,.9)
			padding 1em
			transition all 0.5s ease
			h3
				letter-spacing .2em
				text-transform uppercase
				color #fff
				position relative
				margin-top 0
				margin-bottom 15px
			p 
				line-height 1.5em
				color #fff
				font-size 13px
				margin-bottom 25px
				display none
.item_header
	text-align center
	position relative
	.icon-arrow-down
		background-repeat no-repeat
		background-position 50% 55%
		border-radius 100%
		padding 1em
		background-color #222
		position absolute
		bottom -1.3em
		left 50%
		margin-left -1.5em
		z-index 90
		cursor pointer
		color #fff
		text-decoration none
		
		opacity 0
		transition all 0.5s ease-in

.item_text
	background-color #fff
	text-align center
	padding-top 6%
	padding-bottom 6%
	position relative
	&:after
		content ' '
		position absolute
		bottom -9px
		left 50%
		width 0
		height 0
		display block
		margin-left -10px
		border-left 10px solid transparent
		border-right 10px solid transparent
		border-top 10px solid #fff
		z-index 10

	h1
		font-size 2em
		max-width 60% 
		margin 0 auto
		text-transform uppercase
	h2
		font-size 2.5em
		text-transform uppercase
		font-weight 300;
		max-width 90%
		margin 0 auto

	p 
		font-size 18px
		line-height 1.9em
		max-width 60%
		margin 1em auto 0
.item_plain
	background-color #efefef
	text-align center
	padding-top 1%
	padding-bottom 6%
	padding-left 4%
	padding-right 4%
	margin-bottom -4%
.center-ul
	text-align left
	margin 0 auto
	width 350px
	
.readmore
	border 2px solid #FFF 
	text-decoration none
	color #FFF 
	padding .5em
	&:hover
		background $azul
.item
	border-bottom 1px solid #ccc
 
	.item_header
		padding-top 2.4%
		padding-bottom 2.4%
		
		img
			width 50%
			max-width 55.5em
.upload
	.progress
		position relative 
		width 400px 
		border 1px solid #ddd 
		padding 1px 
		border-radius 3px
		top 7px
		left 750px
		display none

		.bar
			background-color $azul 
			width 0% 
			height 20px 
			border-radius 3px

		.percent  
			position absolute 
			display inline-block 
			top 3px 
			left 48% 
			font-size 12px
			color white
	.field-wrapper
		margin 0
		clear both
		position relative
		overflow visible
		margin 0 0 10px 0

		label
			
			font-weight 500
			display block
			-webkit-user-select none
			-moz-user-select none
			-ms-user-select none
			user-select none
			padding-bottom .2em
			//max-width 550px
			color white

		input[type="text"],input[type="password"],input[type="email"],textarea,#uploadFile
			width 96%
			max-width 550px
			position relative
			
			min-height 26px
			background-color #fff
			border 2px solid #c8c8c8
		textarea
			width 94%

		input[disabled]
			cursor not-allowed
			background-color #eeeeee !important
			
		textarea
			height 90px
			background-color #fff
		label.error
			display block
			margin-left 3px
			color red
			margin-top -5px
			font-size 12px
			font-weight 700
			//background #fff
			border-radius 3px
			padding 2px
			

	.button
		font-size 13px
		height 36px
		line-height 36px
		padding: 0 18px;
		cursor: pointer;
		overflow: hidden;
		font-weight: 700;
		//font-family: "Helvetica Neue",Arial,Helvetica,Verdana,sans-serif;
		text-align: center;
		vertical-align: middle;
		text-transform: capitalize;
		letter-spacing: .01em;
		margin-right: 18px;
		border: 0 none;
		border-radius: 3px;
		-webkit-user-select: none;
		-moz-user-select: none;
		-ms-user-select: none;
		user-select: none;
		color: #737373;
		background-color: $verde;

	.p0
		background $fusia
		color #FFF
	.p0:hover
		background rgb(34, 34, 34)

	.fileUpload 
		position relative
		overflow hidden
		margin 0px 5px

		input 
			position absolute
			top 0
			right 0
			margin 0
			padding 0
			font-size 20px
			cursor pointer
			opacity 0
	#uploadFile
		width 200px !important

	.btn 
		display inline-block
		padding 4px 12px
		margin-bottom 0
		font-size 14px
		line-height 20px
		text-align center
		vertical-align middle
		cursor pointer
		color #333333
		text-shadow 0 1px 1px rgba(255, 255, 255, 0.75)
		background-color #f5f5f5
		background-image linear-gradient(to bottom, #ffffff, #e6e6e6)
		background-repeat repeat-x
		border-color #e6e6e6 #e6e6e6 #bfbfbf
		border-color rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25)
		border: 1px solid #cccccc
		border-bottom-color #b3b3b3
		border-radius (4px)
		box-shadow inset 0 1px 0 rgba(255,255,255,.2), 0 1px 2px rgba(0,0,0,.05)
		
	.mensaje
		font-size 14px
		.ok
			color $verde
			background #fff
			border-radius 3px
			padding 5px
			font-weight 700
			
		.error
			color $azul
			background #fff
			border-radius 3px
			padding 5px
			font-weight 700
			
		
		
.bg_fusia
	background radial-gradient(ellipse at center,#c441d1 0,#a511f1 100%)
.bg_amarillo
	background radial-gradient(ellipse at center,#fab948 0,#f7f20a 100%)
.bg_verde
	background radial-gradient(ellipse at center,#67e207 0,#0f9f06 100%)	
.bg_celeste
	background radial-gradient(ellipse at center,#4ae4fb 0,#129cf7 100%)	
.bg_naranja
	background radial-gradient(ellipse at center,#fb9746 0,#f76e0d 100%)	
.bg_rojo
	background radial-gradient(ellipse at center,#e69291 0,#e94140 100%)	
.bg_rosa
	background radial-gradient(ellipse at center,#e59998 0,#e86160 100%)				
.bg_blanco
	background radial-gradient(ellipse at center,#c4c4c4 0,#FFFFFF 90%)	
.map 
	iframe
		width 100%
.advanced_facebook_likebox 
	display inline-block
	text-align center
					
.b2j_contact
	text-align center
	background none
	form 
		width auto//900px 
		margin 0 auto
	.controls
		font-size: 18px;
		line-height: 1.9em;		
		color #000
	h2
		background $azul
	.control-label
		color #000
	.btn-success
		background $verde !important
	
.wrapper
	max-width 87.5em
	position relative
	padding-top .3em
	margin-right auto
	margin-left auto
	overflow hidden

.page 
	min-height 100%
	margin 0 auto
	position relative
	padding-bottom 30em
	background-color #fff


.gallery
	text-align center
	font-size 0
.gallery-item
	display inline-block
	vertical-align top
	font-size 1rem
	width 218px 
	padding .5rem
	box-sizing border-box

.gallery-link
	display block
	background verde

	img 
		width 100%
		transition all .5s ease
		border 1px solid #EFEFF0
		padding .2em
		box-sizing border-box
		border-radius 3px
		
		&:hover
			opacity .6	
.gallery-info
	h3
		margin 0
		font-size 1em
		font-weight normal



@import "responsive.styl"



    