html,body{
  overflow-x: hidden;
}

.help_message_box{
  height: 50px;
  position: absolute;
  top: 10px;
  left: 480px;
  z-index: 1000;
  overflow hidden;
  box-sizing: border-box;
  // width: 516px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.ship_icon_style{
  // width:10px!important;
  height:29px!important;
}
.help_message_box .left_btn , .help_message_box .right_btn{
  width:14px;
  height:50px;
  background:linear-gradient(0deg,rgba(0,46,103,1),rgba(4,51,130,1));
  box-shadow:0px 0px 4px 0px rgba(10,32,79,0.5);
  opacity:0.9;
  border-radius:2px;
  display: flex;
  justify-content: center
  align-items: center;
  transition: visibility 0.5s;
}

.help_message_box .left_btn img,.help_message_box .right_btn img{
  width: 4px;
  height: 7px;
}

.help_message_box .right_btn{
  margin-left: 4px;
}

.help_message_list_box{
  width: 480px;
  height: 50px;
  overflow: hidden;
  position: relative;
  margin-left: 4px;
 }

.help_message_list_box > ul{
  width: 2400px;
  height: 50px;
  position: absolute;
  left: 0px;
  transition: all 0.5s;
}

.help_message_list_box ul li .un_register{
  position: absolute;
  top: 6px;
  right: 8px;
  width: 34px;
  height: 18px;
  background:rgba(8,130,255,0.3);
  color: #fff;
  border-radius:2px;
  border: none;
  display: flex;
  justify-content: center;
  align-items: center;
}


.help_message_list_box ul li{
  position: relative;
  box-sizing: border-box;
  width:240px;
  height:50px;
  background: linear-gradient(0deg, #002e67, #043382);
  box-shadow:0px 0px 4px 0px rgba(10,32,79,0.5);
  display: flex;
  justify-content: space-between;
  align-items: left;
  flex-direction: column;
  border-radius:2px 0px 0px 2px;
  float: left;
  cursor:pointer;
}
.help_message_list_box ul .task_box{
  padding: 7px 10px 10px 10px;
  width:240px;
  height:50px;
  background-color:rgba(0,46,103,0.9);
}
.help_message_list_box ul .click{
    background: rgba(8,130,255,0.3);
    color: #fff;
    border-bottom: 4px solid #0a7aec;
}


.help_message_list_box ul li div{
  border-radius:2px 0px 0px 2px;
  font-size:12px;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
}


/* 右侧信息 */
#helpMsgBox{
  position: absolute;
  right: 5px;
  top: 70px;
  z-index: 1000;
  width: 260px;
}

#helpMsgBox .help_msg_box{
  width: 100%;
  max-height: 500px;
  overflow-y: auto;
  overflow-x: hidden;
  transition: all 0.5s;
}

#helpMsgBox .helpMsg{
  transition: all 0.5s;
  width:240px;
  background:linear-gradient(0deg,rgba(0,46,103,1),rgba(4,51,130,0.9));
  box-shadow:0px 0px 4px 0px rgba(10,32,79,0.5);
  border-radius:4px;
  margin-top: 4px;
  padding: 20px;
  position: relative;
  margin-left: 0px;
  animation: move 0.5s;
  -moz-animation: move 0.5s;	/* Firefox */
  -webkit-animation: move 0.5s;	/* Safari 和 Chrome */
  -o-animation: move 0.5s;	
}

@keyframes move{
  from {margin-left:300px}
  to {margin-left:0px}
}

#helpMsgBox .helpMsg .close{
  width: 8px;
  height: 8px;
  position: absolute;
  top: 10px;
  right: 10px;
  color:#D7D7D7;
  opacity:0.5;
  cursor:pointer;
}

#helpMsgBox .helpMsg .box {
  // margin-top: 20px;
  // margin-left: 20px;
}

#helpMsgBox .helpMsg .box .header{
  font-size:12px;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,112,112,1);
  line-height:21px;
}

#helpMsgBox .helpMsg .box .title{
  marign-top: 11px;
  font-size:12px;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(255,255,255,1);
  line-height:21px;
}

#helpMsgBox .helpMsg .box .number{
  font-size:12px;
  font-family:PingFang SC;
  font-weight:400;
  color:rgba(178,192,211,1);
  line-height:18px;
}

#helpMsgBox .close_all{
  width: 240px;
  height: 34px;
  background:linear-gradient(0deg,rgba(0,46,103,1),rgba(4,51,130,1));
  box-shadow:0px 0px 4px 0px rgba(10,32,79,0.5);
  opacity:0.9;
  border-radius:4px;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  margin-top: 4px;
}


#helpMsgBox .helpMessageBox{
  position: absolute;
  top: 10px;
  left: 50%
}




