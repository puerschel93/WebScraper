// === Subheading 1 ====================================================================================================
Subheading1DefaultProperties = {
  geometry: {
    fontSize: 23px,
    lineHeight: 27px,
    customDeclarations: {}
  },
  decoration: {
    fontWeight: null,
    fontColor: ShadeOfGray__12--W3C_Palette,
    customDeclarations: {}
  }
}


Subheading-Level1(preset = Subheading1DefaultProperties, overridings = {})

  properties = deeplyCloneAndOverrideObject(preset, overridings)

  // { Required for appended/prepended badge alignment
  display flex
  align-items center
  // }


  // --- Geometry ------------------------------------------------------------------------------------------------------
  MultilineTextWithoutExtraSpaceCausedByLineHeight({
    fontSize: properties.geometry.fontSize,
    lineHeight: properties.geometry.lineHeight
  })

  applyIfNotNull(properties.geometry.customDeclarations)


  // --- Decoration ----------------------------------------------------------------------------------------------------
  applyIfNotNull({
    font-weight: properties.decoration.fontWeight,
    color: properties.decoration.fontColor
  })

  applyIfNotNull(properties.decoration.customDeclarations)


