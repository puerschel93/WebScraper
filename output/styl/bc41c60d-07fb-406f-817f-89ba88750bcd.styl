//************** slider dgalli **************//
.owl-carousel
  display none
  position relative
  width 100%
  -ms-touch-action pan-y
  .owl-wrapper
    display none
    position relative
    -webkit-transform translate3d(0px, 0px, 0px)
    &:after
      content "."
      display block
      clear both
      visibility hidden
      line-height 0
      height 0
  .owl-wrapper-outer
    overflow hidden
    position relative
    width 100%
    &.autoHeight
      -webkit-transition height 500ms ease-in-out
      -moz-transition height 500ms ease-in-out
      -ms-transition height 500ms ease-in-out
      -o-transition height 500ms ease-in-out
      transition height 500ms ease-in-out
  .owl-item
    float left

.owl-controls .owl-page,
.owl-controls .owl-buttons div
  cursor pointer

.owl-controls
  -webkit-user-select none
  -khtml-user-select none
  -moz-user-select none
  -ms-user-select none
  user-select none
  -webkit-tap-highlight-color rgba(0, 0, 0, 0)

.grabbing
  cursor url(grabbing.png) 8 8, move

.owl-carousel  .owl-wrapper,
.owl-carousel  .owl-item
  -webkit-backface-visibility hidden
  -moz-backface-visibility hidden
  -ms-backface-visibility hidden
  -webkit-transform translate3d(0,0,0)
  -moz-transform translate3d(0,0,0)
  -ms-transform translate3d(0,0,0)

#owl-demo
  .item
    color #FFF
    -webkit-border-radius 3px
    -moz-border-radius 3px
    border-radius 3px
    text-align center

.customNavigation
  text-align center
  a
    -webkit-user-select none
    -khtml-user-select none
    -moz-user-select none
    -ms-user-select none
    user-select none
    -webkit-tap-highlight-color rgba(0, 0, 0, 0)
.owl-carousel
    position relative
    width 100%
.item img
    width 100%
.contanctanos-text,.contacatanos-redes
    display inline-block
    vertical-align top
.bloque-contactanos
    position absolute
    top 3em
    right 5em
    z-index 1
.contanctanos-text
    padding 1em 2em
    background white
    color rgba(10, 10, 10, 0.7)
    cursor pointer
    transition all ease 0.3s
.contacatanos-redes a
    padding 0.7em 1em
    display inline-block
    border 1px solid white
    margin-left -5px
    opacity 1
    font-size 16px
.contacatanos-redes i
    color white
    font-size 1.5em
@keyframes redBG
    0%
        background: #333    
    50%
        background: #EF0029
    100%
        background: #333
@keyframes hoverRedesFace
    0%
        background: #333    
    50%
        background: #44619d
    100%
        background: #333
@keyframes hoverRedesYoutube
    0%
        background: #333    
    50%
        background: #ef002a
    100%
        background: #333
@keyframes hoverRedesSearch
    0%
        background: #333    
    50%
        background: #fd663a
    100%
        background: #333
.contacatanos-redes-face a:hover
    animation hoverRedesFace 1s infinite
.contacatanos-redes-youtube a:hover
    animation hoverRedesYoutube 1s infinite
.contacatanos-redes-search a:hover
    animation hoverRedesSearch 1s infinite
.tooltip
    position inherit
.tooltipster-default
    background #fd663a
    color white
    border: 2px solid #A09E9E;
.bloque-contactanos
    font-size 14px
#demo
    position relative
.btn-slider-degalli
    position: absolute;
    bottom: 3em;
    z-index: 1;
    display: inline-block;
    width: 2.5em;
    height: 2.5em;
    background: white;
    border-radius: 0px;
.next
    transform: rotate(44deg);
    margin-left 3.5em
.next i
    transform: rotate(-45deg);
.prev
    transform: rotate(-44deg);
.prev i
    transform: rotate(45deg);
.top
    bottom: 4.7em;
    background: #ef002a;
    margin-left: 1.8em;
    transform: rotate(45deg);
.top i
    color: white !important;
    transform: rotate(45deg);
.btn-slider-degalli i
    font-size: 1.5em;
    color: rgba(0, 0, 0, 0.33);
.contanctanos-text:hover    
    animation redBG 1s infinite
    color white
.fwBold
    font-weight bold
.menuDesplegableDegallia
    position: absolute;
    z-index: 1;
    background: #fff;
    width: 18em;
    height: 9em;
    top: 2.4em;
    overflow: hidden;
.contenedor-img-logo img
    width: 100%;
    position: absolute;
    top: -35px;
    right: -14px;
.contenedor-img-logo
    margin-left 2em
.btn-menuDesplegable-logo
    text-align: center;
    position: absolute;
    bottom: 0.5em;
    width: 100%;
    font-size: 22px;
    cursor: pointer;
.contenidoMenuDesplegable
    width: 35%;
    background: red;
    color: white;
    padding: 2em 0em;
    z-index: 2;
    position: absolute;
.inlineMiddle
    display inline-block
    vertical-align middle
.bloque-flecha-flecha
    width 10%
.bloque-flecha-intra a
    color: white;
    font-size: 14px;
    margin-right: 2em;
.bloque-flecha
    position relative
.contenidoMenuDesplegable a
    text-decoration none
    color white
.contenidoMenuDesplegable
    display none
.cerrar-MenuDesplegable
    display: inline-block;
    position: absolute;
    right: 14px;
    bottom: 7px;
    font-weight: bold;
    cursor pointer
.bloque-flecha-idioma
    position: absolute;
    right: 19px;
    top: -11px;
.language-text a
    color white
.language-borde
    border 1px solid rgba(255, 118, 0, 0.71)
.bloque-Cerrar-con-logo
    width 20%
.bloque-nosotros
    padding 5em 0em
.bloque-nosotros  a
    font-size 24px
.selected
    font-weight bold
.contenedor-descarga
    font-size: 0.7em;
    margin-top: 1em;
.contenedor-descarga i
    display inline-block
    margin-left 1em
.hideDiv
    display none
ul
    list-style none
    padding 0
.contenedor-silder-texto
    z-index: 2;
    position: absolute;
    right: 4.5em;
    top: 33%;
.contenedor-relativo
    position: relative;
    width: 19em;
    font-size: 25px;
    text-align: right;
.slider-texto
    position absolute
    top 72%
    right 2em
.slider-cuadro
    width: 8.5em;
    height: 11em;
    background: rgba(128, 128, 128, 0.84);
    position: absolute;
    right: 0;
    top: -2em;
    z-index: -1;
.slider-texto-bold
    font-weight bold
.slider-tecto-subrayado
    position: absolute;
    top: 7.5em;
    z-index: 7;
    width: 19em;
    right: 3.6em;
    font-size: 14px;
    color: ffe0e6;
.borde-medio
    border: 1px solid#ffe0e6;
    width: 7em;
    margin-right: 1em;
.btn-red
    background: #ef002a;
    position: absolute;
    right: 2.7em;
    top: 7.5em;
    font-size: 18px;
    padding: 0.5em 2em;
.flecha-btn-red
    background: white;
    width: 1em;
    display: inline-block;
    color: gray;
    text-align: center;
    position: absolute;
    top: 0.6em;
    right: -6px;
    border-radius: 3px;
@media screen and (max-width: 700px)
    .bloque-contactanos
        font-size 10px
        top 1em
        right 1em
    .contacatanos-redes a
        padding 0.5em 1em
    .contacatanos-redes i
        font-size 1em
@media screen and (max-width: 1060px)
    .bloque-nosotros a
        font-size 22px
    .bloque-flecha-intra a
        font-size 12px
@media screen and (max-width: 1030px)
    .contenidoMenuDesplegable
        width 42%;
    .bloque-nosotros
        padding 4.58em 0em
@media screen and (max-width: 922px)
    .contenedor-silder-texto
        display none
    .bloque-nosotros a
        font-size 18px
    .bloque-nosotros
        padding 4em 0em
    .menuDesplegableDegallia
        background: rgba(255, 0, 0, 0);
        height: 2em;
        top 3.4em
    .contenedor-img-logo
        display none
    .menuDesplegableDegallia
        overflow inherit
    .btn-menuDesplegable-logo
        display: inline-block;
        width: 1.5em;
        background: white;
        padding: 0.2em 0em;
@media screen and (max-width: 830px)        
    .bloque-Cerrar-con-logo
        width 14%    
@media screen and (max-width: 780px)
    .contenidoMenuDesplegable
        width 100%
    .bloque-nosotros
        width 80%
    .bloque-nosotros
        padding 1em
    .btn-slider-degalli
        bottom 1em
    .top
        bottom 2.7em
 @media screen and (max-width: 370px)   
    .bloque-Cerrar-con-logo
        width 10%
