html 
  height 100%
  overflow hidden
  -webkit-text-size-adjust none
  -ms-text-size-adjust 100%
  -webkit-font-smoothing antialiased
  word-break break-all
  color #383838
body
  background-color rgb(255, 255, 255)
  height 100%
  font 400 14px/1.2   "Microsoft YaHei","Hiragino Sans GB",Tahoma,STHeiti,sans-serif

body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, p, blockquote, th, td, hr, article, aside, details, figcaption, figure, footer, header, menu, nav, section, img
  margin 0
  padding 0

article, aside, details, figcaption, figure, footer, header, menu, nav, section, iframe
  display block

audio, canvas, video
  display inline-block

a, input, button, select, textarea
  -webkit-tap-highlight-color transparent

body, button, input, optgroup, select, textarea
  margin 0
  font-family inherit
  font-size inherit

button, input[type="button"], input[type="reset"], input[type="submit"]
  -webkit-appearance button
  cursor pointer

button::-moz-focus-inner, input::-moz-focus-inner
  border 0
  padding 0

button, select 
  text-transform none

button 
  overflow visible
hr
  margin 15px 0
  border 0
  border-top 1px solid #E0DFDF
  border-bottom 1px solid #FEFEFE
  clear both
h1, h2, h3, h4, h5, h6
  font-weight 500

code, kbd, pre, samp
  font-family monospace
  font-size 1em

sub, sup
  font-size 75%
  line-height 0
  position relative
  vertical-align baseline

sup
  top -0.5em

sub
  bottom -0.25em

textarea
  resize none
  overflow-y auto 

img
  border 0
  vertical-align middle

del
  text-decoration line-through
  position absolute

ul, menu
  list-style none

ol 
  list-style-position inside
a 
  text-decoration none
  color #000
  background-color transparent
  cursor pointer
a:active, a:hover
  color #666
  text-decoration none

table
  border-collapse collapse
  border-spacing 0

object, :focus 
  outline none