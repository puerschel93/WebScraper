.c-editor-content-container
  define-container((850/$rfs)rem)
  define-font($conqSans, (20/$rfs)rem, black, 1.7, 300, (0.05/$rfs)rem)
  margin-top: 55px
  margin-bottom: 55px
  min-height: 50vh;

  .md-block-unstyled
    min-height: (34/$rfs)rem

  .md-block-header-one
    font-size: 2em;

  .md-block-header-two
    font-size: 1.7em;

  .md-block-header-three
    define-font($museo, (34/$rfs)rem , black, 1.06, bold)
    margin-bottom: (30/$rfs)rem
    margin-top: (55/$rfs)rem

  .md-block-header-four
    font-size: 1em;

  .md-block-header-five
    font-size: 0.8em;

  .md-block-header-six
    font-size: 0.7em;

  .md-block-header-one,
  .md-block-header-two,
  .md-block-header-four,
  .md-block-header-five,
  .md-block-header-six
    margin-bottom: 5px;


  .md-block-blockquote
    define-font($museo, (24/$rfs)rem, $lightBlue, 1.5, normal)
    padding-right: (100/$rfs)rem
    padding-bottom: (15/$rfs)rem
    position: relative

  .md-block-image
    margin: 0 0 10px 0;

    img
      max-width: 100%;

    figcaption
      text-align: left;
      font-size: 0.9em;
      define-font($mono, (17/$rfs)rem , $black, 1.1, normal)

  .md-block-todo
    margin: 10px 0 10px 0;

    input[type=checkbox]
      float: left;
      position: relative;
      top: 4px;
      left: -4px;

    p
      margin-left: 18px;

  .md-block-ordered-list-item
    margin-left: 33px

  .md-block-unordered-list-item
    padding-left: (80/$rfs)rem

    li
      list-style-type: none;
      position: relative
      margin-left: 0

      &:before
        content: "";
        width: 6px
        height: 6px
        border-radius: 50%
        display: inline-block
        position: absolute
        left: -18px;
        top: 12px;
        background-color: $lightBlue;

  .md-inline-bold
    font-weight: bold

  .md-inline-right_align
    text-align: right

  .md-inline-dim
    color: $dimBlack

  .md-inline-italic
    font-style italic

  .md-inline-font-size-select-156
    font-size: (156/$rfs)rem

  .md-inline-font-size-select-35
    font-size: (35/$rfs)rem

  .md-inline-font-size-select-25
    font-size: (25/$rfs)rem

  .md-inline-font-size-select-12
    font-size: (12/$rfs)rem

  .md-inline-link
    color: $lightBlue

    &:visited
      color: $lightBlue

  .md-inline-highlight
    background: $lightBlue;


  p
    &.md-block-block-quote-caption
      &::before
        content: '\2013';
        color: #999;
        float: left;
        font-weight: bold;
        display: inline;
        margin-right: 10px;


      margin-top: 0;
      font-size: 0.8em;
      padding-top: 5px;
      padding-bottom: 5px;
      padding-left: 10px;
      background: #fafafa;

  .md-green-block
    background-color: $green
    position: relative
    color: white

  .green-block-inner
    min-height: 75px;

    span
      vertical-align: middle

    & > div
      line-height: 1
      padding-top: 1.3rem
      position: relative
      z-index: 5

    &:before
      content: ''
      height: 100%
      width: "calc(%srem + 100vw / 2)" % (716/$rfs)
      right: 0
      background-color: $green
      box-shadow: 0 (20/$rfs)rem (30/$rfs)rem 0 rgba(125, 211, 32, 0.5)
      position: absolute

  .md-quote-block
    background-color: $lightBlue
    position: relative
    color: white
    font-family: $mono

    .quote-block-inner
      min-height: 75px;

      &:before
        content: ''
        height: 100%
        width: 100vw
        margin-left: calc((850px - 100vw) / 2)
        top: 0
        background-color: $lightBlue
        box-shadow: 0 20px 30px 0 rgba(30, 157, 242, 0.5);
        position: absolute

      .quote-block-container
        background-image: url('data:image/svg+xml;base64,')
        padding-left: 85px
        padding-top: 31px
        background-repeat: no-repeat
        background-position: 15px 43px;
        position: relative
        z-index: 5
        min-height: 100px
        padding-bottom: 41px