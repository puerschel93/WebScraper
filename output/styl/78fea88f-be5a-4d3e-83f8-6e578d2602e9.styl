mixin placeholder()
    &::-webkit-input-placeholder
        color #000
    &:-moz-placeholder
        color #000
    &::-moz-placeholder
        color #000
    &:-ms-input-placeholder
        color #000
placeholder()
section.singleMarket
    overflow hidden
    .pageWrap 
        +below(750px)
            margin-left 35px
            margin-right 35px
            
        
.pageWrapSingle
    margin-top 80px
.singleMarketCategoryHeader
    color black
    font-family 'Muller-Bold'
    font-size 18px
    font-weight 700
    letter-spacing 1px
    text-transform none!important

.singleMarketItem
    margin-top 70px 

h1.singleMarketHeader
    color #090909
    font-family 'Muller-Black'
    font-size 40px
    font-weight 900
    letter-spacing -0.11px
    line-height 40px
    padding-bottom 2px 
    
.singleMarkerHeaderDescWrap
    box horizontal space-between
    
.singleMarketsHeaderSchedule
    box horizontal space-between
    +below(750px)
        display none
    
.singleMarketsHeaderScheduleMobile
    display none
    padding-bottom 10px
    &:before
        content ''
        position absolute
        width 100%
        height 1px 
        background-color #e7e7e7
        left 0
        margin-top 156px
    +below(750px)
        box vertical start
        display flex
    .singleMarkerHeaderDescTime
        margin-left 0px
        padding-bottom 30px
        padding-top 2px
    h5.singleMarkerHeaderDescTitle
        padding-bottom 6px

p.singleMarkerHeaderDesc, p.singleMarkerHeaderDescText
    color #746963
    font-family 'Muller-Regular'
    font-size 14px
    font-weight 400
    letter-spacing -0.1px

p.singleMarkerHeaderDesc
    display block
    +below(750px)
        display none
        
p.singleMarkerHeaderDescMobile
    color #746963
    font-family 'Muller-Regular'
    font-size 18px
    font-weight 400
    padding-bottom 8px
    letter-spacing -0.1px
    
h5.singleMarkerHeaderDescTitle
    color black
    font-family 'Muller-Bold'
    font-size 16px
    font-weight 700
    letter-spacing -0.12px
    padding-bottom 2px

.singleMarkerHeaderDescTime
    margin-left 70px
    padding-top 2px
    
.singleMarkerHeaderDescTime
    svg
        size 16px 18px
        float left
        margin-right 5px 
        margin-top -2px
        
.singleMarketPhotoSlider
    width 940px
    margin 0px auto
    margin-top 27px
    +below(1165px)
        width 760px
    +below(980px)
        margin-left 78px 
        margin-right 78px
        width auto
    +below(880px)
        margin-left 40px 
        margin-right 40px
    +below(750px)
        margin-left 0 
        margin-right 0
        margin-top 43px
    .swiper-pagination
        display none
        bottom -28px
        +below(750px)
            display block
        .swiper-pagination-bullet
            size 10px
            background-color black
            border-radius 14px
            opacity 1
        .swiper-pagination-bullet-active
            background-color #ffd427

.assortmentBeerSliderWrap
    width 940px
    margin 0 auto
    padding-top 50px
    margin-bottom -25px
    +below(940px)
        width auto
        margin-left 78px
    +below(750px)
        width auto
        margin-left 40px
    
.singleMarketSlider
    width 100%
    height 400px
    box horizontal middle
    position relative
    +below(940px)
        width 100%
    +below(600px)
        height 213px
    
.assortmentSliderItems
    width calc(100% - 83px)
    margin 0
.singleMarketSliderSwiper
    width 100%!important
.singlemarketslideritem.swiper-slide-next,.singlemarketslideritem.swiper-slide-prev
    opacity 0
    transition 0.3s
    
.singlemarketslideritem
    height 400px!important
    padding 0 0px
    box vertical middle center
    border-radius 16px
    overflow hidden
    +below(750px)
        border-radius 0
    +below(600px)
        height 213px!important
    img 
        width 100%

.assortmentSliderItems.swiper-container-autoheight .swiper-wrapper
    -webkit-box-align end
    -ms-flex-align end
    -webkit-align-items flex-end
    align-items flex-end
    
.singleMarketSliderNextButton
    size 33px 27px
    position absolute
    right -54px
    cursor pointer
    top 180px
    z-index 1000
    +below(880px)
        display none
    svg
        width 100%
        height 27px
.singleMarketSliderPreviousButton
    size 33px 27px
    position absolute
    cursor pointer
    +below(880px)
        display none
    svg
        width 100%
        height 27px
    left -54px
    top 180px
    z-index 99
    
.singleMarketWorkers
    box horizontal 
    margin-top 33px
    +below(1165px)
        box vertical
    
.singleMarketWorker
    width: 33.3333%
    height 100px
    margin 0px 14px
    box horizontal
    +below(1165px)
        width auto
    +below(400px)
        margin-left 0
        margin-right 0

.singleMarketWorkerImage
    size 80px
    border-radius 50%
    margin-right 20px
    background-size cover
    background-position center

.singleMarketWorkerInfo
    box vertical
    
.singleMarketWorkerName
    color black
    font-family 'Muller-Regular'
    font-size 20px
    font-weight 400
    margin-top 20px
    padding-bottom 2px
    letter-spacing -0.06px
    text-transform none
    
.singleMarketWorkerStatus
    color #746963
    font-family 'Muller-Regular'
    font-size 14px
    font-weight 400
    letter-spacing -0.1px
    text-transform none
    
.mobileHidden
    display none
    +below(750px)
        display block
        
.marginTop120
    +below(750px)
        margin-top 120px
.marginTop10
    margin-top 10px 
    
.pageWrapSingleSchedule
    margin-top 10px
    +below(750px)
        margin-top 66px

#map-singleMarkets
    width 100%
    height 588px
    margin-top 46px

.singleMarketsContacts
    position relative
    .contactItems
        height auto
        width 305px
        right 145px
    .contactFeedbackButton
        size 166px 50px
        background-color #ffd427
        border-radius 100px
        color black
        font-family 'Muller-Bold'
        font-size 14px
        font-weight 700
        letter-spacing -0.1px
        box-sizing border-box
        padding 16px 27px
        margin-top 30px
        cursor pointer
        transition 0.3s all
        border 3px solid #ffd427!important
        &:hover
            color #ffd427!important
            background-color #fff

.singleMarketFeedback
    width 100%
    height 100%
    position absolute
    background-color rgba(0, 0, 0, .8)
    z-index 100
    display none

.singleMarketFeedbackWrap
    width 940px
    margin 0 auto
    overflow hidden
    +below(1080px)
        width 100%
        height 100%
        margin 0 auto
        overflow hidden
        padding-right 24px
        position absolute
    

.singleMarketFeedbackWindow
    width 940px
    background-color white
    border-radius 10px
    box-shadow 10px 10px 20px rgba(0, 0, 0, 0.1)
    position fixed
    margin 100px auto
    z-index 100
    padding 60px 80px 
    box-sizing border-box
    +below(1080px)
        width 85%
        margin 100px 5.3%
    +below(550px)
        margin 0
        width 100%
        height 100%
        border-radius 0
        padding 55px 35px
    
.singleMarketFeedbackTitle
    color #090909
    font-family 'Muller-Black'
    font-size 34px
    font-weight 900
    letter-spacing -0.1px
    line-height 40px
    margin-right 70px
    +below(550px)
        padding-bottom 5px

.singleMarketFeedbackForm
    .singleMarketFeedbackFormInputWrap
        float left
        position relative
        margin-top 70px
        +below(1080px)
            width 43.5%
        +below(800px)
            width 100%
        +below(550px)
            margin-top 45px
        &:first-child
            margin-right 95px
            +below(1080px)
                margin-right 12%
    .inputWrap100Per
        width 100%!important
    input, textarea
        width 340px
        border none
        border-bottom 1px solid #9b9b9b
        padding-bottom 2px
        color #000
        float left
        font-family 'Muller-Regular'
        font-size 16px
        font-weight 400
        letter-spacing 0.1px
        +below(1080px)
            width 100%
        &:required + label
            margin-top -24px
        &:invalid + label
            margin-top auto
        &:focus + label
            margin-top -24px
    label
        color #9b9b9b
        font-family 'Muller-Bold'
        font-size 16px
        font-weight 700
        letter-spacing 0.1px
        left 0
        position absolute
        transition 0.3s all
        pointer-events none
        +below(550px)
            font-size 14px
    .singleMarketFeedbackBigInput
        width 780px
        +below(1080px)
            width 100%
    textarea
        line-height 26px
        padding-left 0
            
.singleMarketFeedbackButton
    size 263px 50px
    background-color #ffd427
    border-radius 100px
    color black!important
    font-family 'Muller-Bold'!important
    font-size 14px
    font-weight 700
    letter-spacing -0.1px
    box-sizing border-box
    transition 0.3s all
    cursor pointer
    border 3px solid #ffd427!important
    +below(550px)
        height 57px
    &:focus
            line-height 26px
    &:hover
        color #ffd427!important
        background-color #fff
        

.singleMarketFeedbacButtonClose
    svg
        size 24px
        position absolute
        right 40px
        top 40px
        cursor pointer
        transition 0.3s all
        fill #d8d8d8
        &:hover
            fill #000
        
