.general-title
	font-family: L
	font-size: 38px
	font-weight: normal
	margin-bottom: 70px
	line-height: 120%
	@media screen and (max-width 560px)
		font-size: 22px
		margin-bottom: 30px
.pre-title
	font-size: 24px
	font-weight: 300
	margin-bottom: 30px
	color: #ADADAD;
	@media screen and (max-width 560px)
		font-size: 20px
		margin-bottom: 20px
.general-text
	margin-bottom: 30px
	@media screen and (max-width 992px)
		margin-bottom: 20px
	@media screen and (max-width 560px)
		margin-bottom: 10px
.general-list-ul
	li
		list-style: none
		position relative
		padding-left: 38px
		margin-bottom: 60px
		&:before
			content: ''
			display block
			position absolute
			width 18px
			height: 18px
			border-radius: 50px
			background-color #fff
			top 3px
			left 0
		@media screen and (max-width 560px)
			margin-bottom: 30px
			padding-left: 27px
			&:before
				width 12px
				height: 12px
				top 5px
	&-min
		li
			margin-bottom: 35px
		@media screen and (max-width 560px)
			li
				margin-bottom: 30px
	&-2
		li
			padding-left: 28px
			margin-bottom: 35px
			&:before
				width 13px
				height: 13px
				top 7px
				background-color: #C9918F
			@media screen and (max-width 560px)
				padding-left: 27px
				margin-bottom: 30px
				&:before
					width 12px
					height: 12px
					top 5px
	&-3
		li
			&:before
				background-color #C9918F
.general-sublist-ul
	margin -20px 0 0 40px
	@media screen and (max-width 560px)
		margin: 0
	li
		margin-bottom: 20px
		color #828282
		padding-left: 33px
		&:before
			content: ''
			display block
			width 12px
			height: 2px
			background-color: #C9918F
			top 12px
			left 0
.general-list-ol
	position relative
	padding-bottom: 70px
	width 100%
	@media screen and (max-width 991px)
		// padding-left: 10px
		padding-right: 10px
	@media screen and (max-width 560px)
		padding-bottom: 30px
	&-title
		display table-cell
		font-family: L
		font-size: 38px
		font-weight: normal
		line-height: 45px
		width 37px
		text-align right
		@media screen and (max-width 560px)
			font-size: 30px
			line-height: 36px
			width 30px
	&-text
		display table-cell
		vertical-align top
		font-family: SUD
		padding-left: 20px
		@media screen and (max-width 560px)
			padding-left: 15px
