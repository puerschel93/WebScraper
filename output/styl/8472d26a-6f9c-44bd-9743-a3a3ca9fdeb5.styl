

@font-face {
  font-family: 'LCD';
  src: url('../assets/font/font1/font.eot'); /* IE9 Compat Modes */
  src: url('../assets/font/font1/font.eot?#iefix') format('embedded-opentype'), /* IE6-IE8 */
            url('../assets/font/font1/font.woff') format('woff'), /* Modern Browsers */
            url('../assets/font/font1/font.ttf')  format('truetype'), /* Safari, Android, iOS */
            url('../assets/font/font1/font.svg#MicrosoftYaHei') format('svg'); /* Legacy iOS */
}

.console
  &>div
    height 140px
    &>div
      height 100%
  .el-card__body>span, .handle-writing>span
    font-size 15px
    font-weight 600
    color #1f2d3d
    &::before
      content ''
      width 2px
      height 14px
      vertical-align middle
      background-color #FF6600
      display inline-block
      margin-right 8px
  ul
    margin-top 8px
    li
      line-height 24px
      font-size 13px
      text-indent 8px
      display flex
  .no-default
    .el-card__body
      padding 0
  .handle-writing
    padding 10px 20px
  .turn-type
    text-align center
    height 45px
    line-height 45px
    background-color #F5F5F5
    font-size 14px
    color #000
    cursor pointer
  .light-control
    display flex
    justify-content space-between
    margin 13px 0 12px
    span
      border 1px solid #D9D9D9
      flex 1
      height 35px
      line-height 35px
      display inline-block
      text-align center
      border-radius 3px
      cursor pointer
      &:first-child
        margin-right 20px
      &:last-child
        margin-left 20px
      i
        width 10px
        height 10px
        display inline-block
        background-color #FFBD4F
        // vertical-align middle
        border-radius 50%
        margin-right 6px
        &.red
          background-color #EB4B4B
  .stop
    text-align center
    border 1px solid #D9D9D9
    height 35px
    line-height 35px
    border-radius 3px
    cursor pointer
    .el-icon-remove
      margin-right 6px
      color #EB4B4B
.equipment
  margin-top 10px
  .type-title
    display flex
    h5
      display inline-block
      margin-right 12px
    span
      flex 1
.per-second
  height 180px
  width 100%
  background-color #333333
  padding 10px 10px
  position relative
  overflow: hidden;
  @media screen and (max-width:1440px)
    width 500px
  .time_bar
    height: 37px
    position relative
    .time_bar_poup, .time_bar_num
      height 30px
      width 100%
      position absolute
      bottom 0
      left 0
    .time_bar_num
      .green, .yellow, .red
        height 30px
        line-height 30px
        float left
        margin 0
        text-align center
      .green
        background-color: #00D142
        color: black
      .yellow
        background-color: #F8E71C
      .red
        background-color: #D0021B
    .time_bar_poup
       background-color rgba(0,0,0,0.7)
       width 0%
       span
        width: 0; 
        height: 0;
        border-width: 12px;
        border-style: solid;
        border-color:#fff transparent transparent transparent;
        position absolute
        top -14px
        right -12px
  .roll_phase_img
    height 147px
    width 100%
    overflow-x scroll
    overflow-y hidden
    margin-top 5px
    white-space nowrap
    .part_img
      height 120px
      width 140px
      border 2px solid
      // float left
      display inline-block
      position relative
      margin-right 6px
      background url('../assets/images/svg/consoles/part/partBg.svg') no-repeat
      background-size: 100% 100%
      .part_n,.part_s,.part_w,.part_e
        transform-origin: center center
        width 50px
        position absolute
        top -5px
        left 50px
        .map_phase_img
          position: absolute
          top 20px
          left 0
          // transform rotate(180deg)
          span
            width 5px
            height 15px
            font-size 12px
            color #ffffff
            line-height 20px
            margin 0 5px
            display block
            float left
            transform rotate(180deg)
            .active
              display: none
        .foot_way
          position: absolute
          top 5px
          left 13px
      .part_w
        top 56px
        left -24px
        transform rotate(-90deg)
      .part_s
        top 126px
        left 44px
        transform rotate(180deg)
      .part_e
        top 63px
        left 116px
        transform rotate(90deg)
  .roll_phase_img 
    .part_img.active
      border: 2px solid red
