          left 122px
          transform rotate(90deg)
          img
            width 100%
            height 100%
            position absolute
            top 0px
            left 0px
      .phase
        text-align: center
        position: absolute
        top 5px
        left 16px
        span
          width 20px
          height 25px
          font-size 14px
          color #ffffff
          line-height 25px
          margin 0 8px
          display block
          float left
          background-color black
      .phase_img
        position absolute
        top 52PX
        left 27px
        text-align: center
        span
          width 20px
          height 30px
          font-size 14px
          color #ffffff
          line-height 25px
          margin 0 9px 0 7px
          display block
          float left
          transform: rotate(180deg)
          img
            position absolute
            top 0
            left 11px
    .div_e
      width 160px
      height 105px
      position absolute
      top -48px
      left -73px
      transform rotate(90deg)
      .phase, .foot_way
        span
          transform: rotate(-90deg)
          margin-top -5px
      .phase_img
        left 27px
        top  25px
    .div_s
      top -185px
      left -17px
      transform rotate(180deg)
      .phase, .foot_way
        span
          transform: rotate(-180deg)
      #foot_way_img_s
        left 136px
      .phase_img
        top 36px
        left 30px
      .phase
        left 20px
        span
          width 21px
    .div_w
      top -138px
      left 114px
      transform rotate(-90deg)
      .phase, .foot_way
        span
          transform: rotate(90deg)
          margin-top -5px
      #phase_img_w
        left 27px
        top 35px
