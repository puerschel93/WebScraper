.device
  width 100%
  height 326px
  margin 0 auto 
  .crossBg
    width 383px
    height 326px
    top 10px
    left 73px
    background-color #F5F5F5
    border-radius 3px
    margin 36px auto
    background url('../assets/images/svg/consoles/consolesBg.svg') no-repeat
    background-size 100% 100%
    .div_n, .div_e, .div_s, .div_w
      width 160px
      height 200px
      position absolute 
      top 0px
      left 65px
      transform-origin: 170px 256px
      .foot_way
        width 35px
        height 136px
        top 30px
        position absolute
        left -17px
        text-align: center
        font-size: 14px
        span:nth-child(1)
          width 25px
          height 25px
          line-height: 25px
          color #ffffff
          position absolute
          top 31px
          left 120px
          background-color black
        span:nth-child(2)
