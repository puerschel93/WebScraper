$path           = 'http://r.syyx.com/events/p049/'
$bg             = $path'bg.jpg'
$main           = $path'main.jpg'
$btn            = $path'btn.jpg'
$btn2           = $path'btn2.jpg'
$skill          = $path'skill.png'
$tab            = $path'tab.jpg'
$jws            = $path'jws.jpg'
$qxs            = $path'qxs.jpg'
$ynz            = $path'ynz.jpg'
$gdj            = $path'gdj.jpg'
$right_wrong    = $path'right_wrong.png'
$loader         = $path'ajax-loader.gif'
$float_reg      = $path'float_reg.jpg'
$success        = $path'success.png'
$fail           = $path'fail.png'
$reg_head       = $path'reg_head.jpg'
$reg            = $path'reg.jpg'
$download       = $path'download.jpg'

body
    font-size 12px
    margin 0
    color #26424D
    background #07102f
img
    border none
a
    outline none
    color #4D4A83
    text-decoration none
    &:hover
        color #fff
.disnone
    display none
.wrapper
    width 100%
    height 100%
    background url($main) no-repeat center 480px
.container
    position relative
    width 100%
    height 1030px
    min-width 980px
    background url($bg) no-repeat center top
.main
    height 100%
    width 980px
    margin 0 auto
    position relative
.nav
    color #4D4A83
    padding-left 20px
    line-height 30px
.logo
    width 151px
    height 76px
    position absolute
    left 5px
    top 40px
.download_17173
    position absolute
    width 125px
    height 30px
    top 471px
    left 86px
    background url($btn) no-repeat 999cm
    &:hover
        background-position -13px -157px
.download_nycs
    position absolute
    width 125px
    height 30px
    top 471px
    left 221px
    background url($btn) no-repeat 999cm
    &:hover
        background-position -140px -157px
#flash
    height 238px
    left 4px
    position absolute
    top 540px
    width 425px
#job
    width 540px
    position absolute
    right 1px
    top 30px
    height 755px
    color #8C95A4
    .con
        height 755px
        width 100%
        display none
        position absolute
        top 0
        left 0
    div.qxs
        display block
        .skill
            background-position -8px -6px
    div.ynz .skill
            background-position -8px -45px
    div.jws .skill
            background-position -8px -84px
    div.gdj .skill
            background-position -8px -123px
    #j_tab
        height 38px
        width 100%
        position absolute
        top 508px
        span
            display block
            height 38px
            width 112px
            background url($tab) no-repeat 999cm
            float left
            cursor pointer
        .check_0
            background-position 0 0
        .check_1
            background-position -112px 0
        .check_2
            background-position -224px 0
        .check_3
            background-position -336px 0
    .info
        position absolute
        top 560px
        left 25px
        strong
            color #9082D1
        ul
            padding 5px 0 10px
            width 320px
            height 40px
            margin 0
            li
                list-style none
                width 160px
                float left
                height 20px
                line-height 20px
        p
            line-height 20px
            margin 0 0 10px
        .skill
            height 33px
            margin 5px 0 0
            width 271px
            background url($skill) 999cm no-repeat
            em
                display inline-block
                width 33px
                height 33px
                cursor pointer
                float left
                margin-right 5px
    #j_swf
        width 188px
        height 105px
        border 1px solid #0165ff
        position absolute
        right 20px
        top 565px
        background #000
        span
            visibility hidden
            position absolute
            width 188px
            height 105px
            top 0
            left 0
    #reg_1
        position absolute
        width 185px
        height 37px
        top 460px
        right 0
        background url($btn) no-repeat 999cm
        &:hover
            background-position -13px -13px
    #reg_2
        position absolute
        width 109px
        height 30px
        background url($btn) no-repeat -13px -61px
        top 695px
        right 45px
        &:hover
            background-position -13px -93px
.copy_right
    width 700px
    position absolute
    top 953px
    line-height 23px
    padding-left 240px
    .feet_logo
        position absolute
        width 130px
        height 45px
        left 95px
        top 0
.reg_box
    right 0
    top 80px
    position absolute
    height 462px
    width 70px
    left auto
    z-index 10
    #reg1
        width 70px    
        height 291px
        background url($reg) no-repeat
        text-indent -9999cm
        position absolute
        &:hover
            background-position -70px 0
    #reg2
        display none
        position absolute
        left -212px
        background url($reg_head) #061C3B no-repeat
        width 282px
        height 360px
        padding-top 100px
        border 1px solid #0876B3
        color rgb(189,241,255)
        overflow hidden
        a
            color #00E5E2
            &:hover
                color #fff
        .reg
            padding 0 20px 0 25px
        table
            height 162px
            td
                height 27px
                width 20px
            input
                background #85bbe5
                border 1px solid #009cff
                height 18px
                color #002c7a
                line-height 18px
                width 145px
            .focus_in_inut
                background #cce9ff
        .agreement
            height 21px
        .reg_form_item_name
            width 60px
        .reg_form_item_input
            width 150px
        .validate_num_input
            height 30px
            vertical-align top
            padding-top 3px
        #txtValidate
            width 50px
        #validateimg
            width 90px
            margin-left 5px
            height 27px
            vertical-align top
            cursor pointer
        #reg_step2,#reg_step3,#reg_step4,#reg_step5
            width 250px
            margin 0 auto
            line-height 25px
            display none
            height 150px
            text-align center
            input
                background #85bbe5
                border 1px solid #009cff
                height 18px
                color #002c7a
                line-height 18px
                width 145px
        #reg_step2
            height 241px
            .active
                height 200px
                line-height 30px
        #reg_step3
            height 241px
        #reg_step5
            height 360px
            width 100%
            #active_tip
                padding 20px 25px
                text-align left
        #active_link
            display block
            margin 20px auto
            width 120px
            height 30px
            background url($btn) no-repeat -246px -61px
            &:hover
                background-position -246px -93px
        .download
            height 50px
            width 266px
            margin 0 auto
            background url($download) no-repeat
            position relative
            a
                position relative
                top 60px
            .download_17173_2
                height 30px
                width 125px
                position absolute
                left 0
                background url($btn) no-repeat -13px -125px
                &:hover
                    background-position -13px -157px
            .download_mini_2
                height 30px
                width 125px
                position absolute
                right 0
                background url($btn) no-repeat -140px -125px
                &:hover
                    background-position -140px -157px
        .reg_submit
            height 55px
            position relative
        #reg_button
            width 120px
            height 30px
            position absolute
            left 54px
            top 7px
            background url($btn) no-repeat -124px -61px
            &:hover
                background-position -124px -93px
        .success
            padding 10px 0 10px 40px 
            line-height 50px
            background url($success) no-repeat left center
            color rgb(255,191,0)
            display inline
        .fail
            height 50px
            padding 10px 0 10px 40px 
            line-height 50px
            background url($fail) no-repeat left center
            color rgb(255,191,0)
            display inline
        .back_bg
            background url($download) no-repeat
            height 20px
            width 266px
            position absolute
            top 360px
            left 10px
        #back
            text-decoration underline
            cursor pointer
            position absolute
            top 40px
            width 120px
            height 30px
            left 68px
            background url($btn) -267px -125px
            &:hover
                background-position -267px -157px
        .download_tip
            text-align center
            padding-top 90px
    #close
        width 22px
        height 22px
        cursor pointer
        position absolute
        top 0
        right 0
.check_result_wrong
    background url($right_wrong) no-repeat 0 -32px
    height 100%
    width 16px
.check_result_right 
    background: url($right_wrong) no-repeat 0 4px
    height: 25px;
    width: 16px;

.check_result_loading 
    background url($loader) no-repeat 0 4px
    height 25px
    width 16px