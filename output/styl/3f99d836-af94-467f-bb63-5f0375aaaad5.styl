        &:hover
            background-position -517px -241px
    .download
        height 66px
        width 243px
        padding 0 20px
        position relative
        margin 0 auto
        background url($btn) -27px -300px
        .mini_download
            height 29px
            width 114px
            display block
            position relative
            top 80px
