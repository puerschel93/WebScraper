            height 173px
            position absolute
            left 200px
            top 70px
            ul
                padding 0
                margin 0 0 10px 0
                overflow hidden
                zoom 1
                li
                    list-style none
                    width 50%
                    float left
        strong
            color #B39CEC
        .feat
            width 200px
            position absolute
            top 100px
            left 320px
        .video
            right -185px
            top 6px

    .video
