        &:hover
            background-position -517px -165px
    .success
        background url($success) no-repeat
        padding 10px 40px
        display inline
        color #FAAB10
    .fail
        background url($fail) no-repeat
        padding 10px 40px
        display inline
        color #FAAB10
    #back
        display block
        text-decoration underline
        cursor pointer
        margin 20px auto
    .check_result_wrong
        background url($right_wrong) no-repeat 0 -32px
        height 100%
        width 16px
    .check_result_right 
        background: url($right_wrong) no-repeat 0 4px
        height: 25px;
        width: 16px;

    .check_result_loading 
        background url($loader) no-repeat 0 4px
        height 25px
        width 16px
.disnone
    display none
