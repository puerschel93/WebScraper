    line-height 0
    height 350px
.ui-dialog .ui-dialog-titlebar-close
    background url($close) no-repeat
object
    outline none
.reg_panel
    width 283px
    padding-top 50px
    color #c998ff
    position absolute
    top 506px
    right 3px
    overflow hidden
    height 390px
    .reg
        margin-left 40px
    a
        color #A696C7
        text-decoration underline
        &:hover
            color #fff
    table
        height 162px
        td
            height 27px
            width 20px
        input
            background #776595
            border 1px solid #683ec8
            height 18px
            color #221334
            line-height 18px
            width 145px
        .focus_in_inut
            background #af99d4
    .agreement
