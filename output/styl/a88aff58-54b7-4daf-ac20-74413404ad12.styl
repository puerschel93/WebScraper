.Property
  transform translate(0px, 2px)
  .owl-theme
    .owl-controls
      .owl-page
        span
          size 8px 8px
          background: darkBlue
  .Jumbotron
    background-repeat: no-repeat
    background-size: cover
    background-position: center
    position relative
    size 100% 300px
    .bgTint
      background-color overJumboTronTint
      size 100% 300px
      .jumbotron-wrapper
        align(b)
        width 100%
        .title
          color darkBlue
          font-family: otherFont
          font-weight: 600
          font-size 44px
          text-align: center
          +below(767px)
            font-size 30px
        .border
          border-top 1px solid infoBorder
          +below(767px)
            margin 10px auto
            size 200px auto
          +above(768px)
            margin 20px auto
            size 340px auto
        .info
          color darkBlue
          text-align: center
          font-family: otherFont
          font-weight: 600
          .address
            font-size: 15px
            line-height: 25px
            +below(767px)
              font-size: 14px
          i.fa
            font-size 25px
            +below(767px)
              margin 20px auto 0px
            +above(768px)
              margin 20px auto 0px
  .Details
    margin-bottom 120px
    +below(767px)
      margin-bottom 60px
    .slideShow
      padding 60px 0px 0px
      center(1280px)
      relative()
      .owl-controls
        .owl-prev
          absolute top 235px left -20px
        .owl-next
          absolute top 235px right -20px
        .owl-prev
        .owl-next
          absolute()
          background: none
          opacity: 1
          +below(1024px)
            display: none
          img
            size 100px auto
            &:hover
            &:focus
              opacity: 0.5
      #ShareButton
        z-index: 10000
        +below(1024px)
          display: block
          margin: 20px auto 0px
          text-align: center
        +above(1025px)
          absolute bottom -7px left 20px
        a
          padding 5px
          i
            font-size: 45px
            color darkBlue
      +below(767px)
        padding 30px 0px 0px
      .item.img-wrapper
        background-repeat: no-repeat
        background-size: cover
        background-position: center
        position relative
        size 1280px 600px
        max-width: 100%
        margin auto auto 40px
        +below(767px)
          size 100% 300px
        .specialText
          background-color slideShowSpecialText
          color white
          font-size 25px
          font-weight: 700
          padding 25px 35px
          size auto auto
          min-width: 10px
          letter-spacing: 1px
          text-align: center
          absolute top 0px right 0px
          +below(767px)
            font-size 15px
            padding 8px 20px

    .info
      center(1280px)
      margin 40px auto 60px
      // background-color: propInfoColor
      //   height 350px
      // +above(768px)
      .info-wrapper
        position: relative
        background-color: propInfoColor
        height 175px
        +below(767px)
          span(1/2, cycle: 2)
          height 140px
          // height 100%
        +above(768px)
          span(1/4)
          // height 100%
        .price
          align(b)
          width 100%
          color darkBlue
          text-align: center
          font-weight: 600
          font-family otherFont
          font-size 20px
          line-height 35px
          +above(768px)
            border-right 1px solid infoBorder
          +below(767px)
            font-size 15px
            border-right 1px solid infoBorder
        .bedBath
          align(b)
          width 100%
          color darkBlue
          text-align: center
          font-weight: 600
          font-family otherFont
          font-size 20px
          line-height 35px
          +above(768px)
            border-right 1px solid infoBorder
          +below(767px)
            font-size 15px
        .area
          align(b)
          width 100%
          color darkBlue
          text-align: center
          font-weight: 600
          font-family otherFont
          font-size 20px
          line-height 35px
          +above(768px)
            border-right 1px solid infoBorder
          +below(767px)
            font-size 15px
            border-right 1px solid infoBorder
        .mls
          align(b)
          width 100%
          color darkBlue
          text-align: center
          font-weight: 600
          font-family otherFont
          font-size 20px
          line-height 35px
          +below(767px)
            font-size 15px
    .paragraph
      center(1100px)
      text-align: center
      font-family: mainFont
      color darkBlue
      h1
        h1()
      h2
        h2()
      h3
        h3()
      h4
        h4()
      h5
        h5()
      h6
        h6()
      +below(767px)
        margin-top 50px
        h1
          font-weight: 300
          font-family: mainFont
          text-align: center
          line-height: 40px
          margin-top 0px
          font-size: 30px
        h2
        h3
        h4
        h5
        h6
          font-weight: 300
          font-family: mainFont
          text-align: center
          // margin-top 0px
        p
          font-size 14px
          line-height: 27px
          font-weight: 500
          margin-bottom 0px
          text-align: center
      +above(768px)
        h1
          font-weight: 300
          font-family: mainFont
          text-align: center
          line-height: 40px
        h2
          font-weight: 300
          font-family: mainFont
          text-align: center
          line-height: 35px
        h3
          font-weight: 300
          font-family: mainFont
          text-align: center
          line-height: 35px
        h4
        h5
        h6
          font-weight: 300
          font-family: mainFont
          text-align: center
          // margin-top 0px
        p
          font-size 17px
          line-height: 27px
          font-weight: 500
          margin-bottom 0px
  .Facilities
    padding-bottom 100px
    +below(767px)
      // padding-top 50px
      padding-bottom 50px
    .title
      text-align: center
      h1()
      color: darkBlue
      font-family: otherFont
      font-weight: 500
      +below(767px)
        font-size 30px
    .Tablet
      #facilitiesSlide
        .item
          .img-wrapper
            size 100% 300px
    .Desktop
      #facilitiesSlide
        .item
          .img-wrapper
            size 100% 600px
    #facilitiesSlide
      center(1280px)
      .owl-controls
        margin-top 50px
        .owl-prev
          absolute top 235px left -20px
        .owl-next
          absolute top 235px right -20px
        .owl-prev
        .owl-next
          absolute()
          background: none
          opacity: 1
          +below(1024px)
            display: none
          img
            size 100px auto
            &:hover
            &:focus
              opacity: 0.5
      .item
        .img-wrapper
          background-repeat: no-repeat
          background-size: cover
          background-position: center
          display: block
          margin auto
          position: relative
          // +below(1280px)
          //   size 100% 300px
          // +above(1281px)
          //   size 100% 600px
          .specialText
            background-color rgba(darkBlue, 0.50)
            absolute bottom 0px
            size 100% auto
            color white
            font-size 30px
            font-family: mainFont
            font-weight: 500
            text-align: center
            padding 12px 0px
            display: block
            +below(767px)
              font-size 25px
              padding 6px 0px
  .Featured
    padding-bottom 100px
    +below(767px)
      padding-bottom 50px
    .title
      text-align: center
      h1()
      color: darkBlue
      font-family: otherFont
      font-weight: 700
    #featuredSlide
      center(1280px)
      .owl-controls
        absolute bottom 30px
        width 100%
      .item
        .img-wrapper
          background-repeat: no-repeat
          background-size: cover
          background-position: center
          display: block
          margin auto
          size 530px 300px
          max-width: 100%
          position relative
          overflow hidden
          .specialText
            absolute bottom 0px
            background-color rgba(lightBlue, 0.50)
            size 100% auto
            color white
            font-size 30px
            font-family: mainFont
            font-weight: 500
            text-align: left
            padding 12px 40px
            +below(767px)
              text-align: center
              display: block
              font-size 25px
              padding 6px 0px
        .info
          size 530px auto
          max-width: 100%
          display block
          margin auto
          .name
            color darkBlue
            font-family: otherFont
            font-weight: 700
            +below(767px)
              font-size: 18px
              text-align: center
              margin 15px auto 0px
            +above(768px)
              font-size: 20px
              text-align: left
              padding-left 40px
              margin 20px auto 0px
          .price
            color darkBlue
            +below(767px)
              margin 15px auto 35px
              font-size 16px
              text-align: center
            +above(768px)
              margin 5px auto 75px
              font-size 18px
              text-align: left
              padding-left 40px
