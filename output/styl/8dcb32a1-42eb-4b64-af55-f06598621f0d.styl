
 [class^="icon-"]:before, [class*=" icon-"]:before {
  font-family: "fontello";
  font-style: normal;
  font-weight: normal;
  speak: none;
  display: inline-block;
  text-decoration: inherit;
  width: 1em;
  margin-right: .2em;
  text-align: center;
  font-variant: normal;
  text-transform: none;
  line-height: 1em;
  margin-left: .2em;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-calendar:before { content: '\e800'; } /* '' */
.icon-up-open:before { content: '\e801'; } /* '' */
.icon-user:before { content: '\e802'; } /* '' */
.icon-tag:before { content: '\e803'; } /* '' */
.icon-tags:before { content: '\e804'; } /* '' */
.icon-mail:before { content: '\e805'; } /* '' */
.icon-search:before { content: '\e806'; } /* '' */
.icon-comment:before { content: '\e807'; } /* '' */
.icon-eye:before { content: '\e808'; } /* '' */
.icon-star-empty:before { content: '\e809'; } /* '' */
.icon-right-open:before { content: '\e80a'; } /* '' */
.icon-cog-alt:before { content: '\e80b'; } /* '' */
.icon-twitter:before { content: '\f099'; } /* '' */
.icon-facebook:before { content: '\f09a'; } /* '' */
.icon-rss:before { content: '\f09e'; } /* '' */
.icon-menu:before { content: '\f0c9'; } /* '' */
.icon-linkedin:before { content: '\f0e1'; } /* '' */

.spartan_share{
  display: block;
}

.share-btn{
  padding: 4px 6px!important;
  border-radius: 4px;
  margin-right: 6px;
  color: white;
  cursor: pointer;
  display: inline-block;
margin-top: .2rem;
}

.share-btn:hover{
  color: whitesmoke;
  text-decoration: none;
}

.share_facebook{
  background: #3b5998;
}
.share_linkedin{
  background: #0073b1;
}
.share_twitter{
  background: #00aced;
}

.share_rss{
  background: #ffc107;
}
.spartan_share_block{
  position: fixed;
  z-index: 9999;
  top: calc(50% - 75px);
}
.spartan_share_block.left{
  left: -35px;
  transition: left .5s;
}

.spartan_share_block.right{
  right: 0
}

.share-block{
  text-align: center;
  height: 50px;
  width: 50px;
  font-size: 36px;
  color: white;
  box-shadow: 0 0 6px grey;
  margin: 2.5px 0;
  transition: margin-left .5s;
  cursor: pointer;
}

.fadeIn{
  -webkit-animation: fadein 1s;
  animation: fadein 1s;
}

.fadeOut{
  -webkit-animation: fadeout 1s;
  animation: fadeout 1s;
  visibility: collapse;
  transition: visibility 1s;
}

.spartan_share_block.left .share-block:hover{
  margin-left: 35px;
}

.spartan_share_block.right .share-block:hover{
  margin-right: 35px;
}

::-webkit-scrollbar {
  z-index: 30;
  width: 6px;
}
::-webkit-scrollbar-thumb {
  background-color: rgb(80, 80, 80);
}
::-webkit-scrollbar-track {
  background-color: transparent;
}

#top-nav{
  box-shadow: 0 0 4px #000;
    width: 100%;
    background: white;
    z-index: 99999;
}

.navbar-nav.nav-lnks{
  flex-direction: row!important;
}

.menu-link{
  font-size: 36px;
  transition-duration: 0.5s;
  display: none;
  cursor: pointer;
}

.menu-link:hover{
  -webkit-transform: rotate(90deg);
  transform: rotate(90deg);
}


.navbar-nav a{
  cursor: pointer;
  margin-right: 1rem;
}

.bg-trans{
  background: transparent!important;
}

.hidden{
  display: none!important
}

.cookie-warn{
  width: 100%;
  height: 30px;
  position: fixed;
  background: rgba(31, 31, 31, 0.8);
  color: white;
  bottom: 0;
  left: 0;
  line-height: 2;
  z-index: 999999;
}

.cookie-accept{
  margin-left: 10px;
  cursor: pointer;
}

.cookie-accept:hover{
  text-decoration: underline;
}

.to-top{
  height: 50px;
  width: 50px;
  background: rgba(255, 255, 255, 0.7);
  color: grey;
  box-shadow: 0 0 6px;
  font-size: 28px;
  position: fixed;
  bottom: 35px;
  right: 10px;
  cursor: pointer;
  border-radius: 50%;
  text-align: center;
  z-index: 99999;
}

.to-top:hover{
  box-shadow: 0 0 10px;
}
.cp{
  cursor: pointer;
}

.footer{
  box-shadow: 0 0 6px black;
    padding: 15px;
}

.foot-lnk{
  font-size: .8rem;
  font-weight: bold;
}

.foot-lnk:hover{
  color:grey!important;
}

.w-90{
  width: 90%;
  margin-left: 5%;
}

.sh-9,.sh-95 {
  transition: 1s ease;
}

.sh-9:hover{
  -webkit-transform: scale(0.9);
  -ms-transform: scale(0.9);
  transform: scale(0.9);
  transition: .5s ease;
}

.sh-95:hover{
  -webkit-transform: scale(0.95);
  -ms-transform: scale(0.95);
  transform: scale(0.95);
  transition: .5s ease;
}

#search-text{
  width: 100%;
  text-align: right;
  color: red;
}

#search-btn{
  border-top-right-radius: 4px!important;
  border-bottom-right-radius: 4px!important;
}

#copyright{

}

.sb-main {
  position: fixed;
  top: 0;
  height: 100%;
  width: 300px;
  background: rgba(255, 255, 255, 0.8);
  box-shadow: 0 0 6px grey;
  z-index: 999;
}

.sb-main.sb-left{
  left: -300px;
  transition: left 0.6s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

.sb-main.sb-right{
  right: -300px;
  transition: right 0.6s cubic-bezier(0.68, -0.55, 0.265, 1.55);
}

.sb-main.sb-right.sb-show {
  right: 0;
}

.sb-main.sb-left.sb-show {
  left: 0;
}

.sb-toggle {
  background: black;
  color: white;
  width: 50px;
  height: 50px;
  text-align: center;
  font-size: 24px;
  line-height: 2;
  top: 0;
  box-shadow: -2px 0px 4px grey;
  position: absolute;
  cursor: pointer;
}

.sb-toggle.sb-left{
  right: -50px;
}

.sb-toggle.sb-right{
  left: -50px;
}

.sb-icon {
    transition-duration: 0.5s;
}
.sb-icon:hover {
  -webkit-transform: rotate(90deg);
    transform: rotate(90deg);
}

.sb-body{

}

.sb-body .sb-head{
  height: 64px;
  padding: 10px 20px;
  line-height: 2;
  font-size: 32px;
  font-weight: 600;
  text-align: center;
  text-transform: capitalize;
  border-bottom: 1px solid;
}

.sb-body .sb-link{
  font-size: 24px;
  font-weight: 300;
  padding: 10px 20px;
  border-bottom: 1px solid;
  cursor: pointer;
  text-transform: capitalize;
  transition: all .5s;
}

.sb-body .sb-link:hover{
  background: black;
  color: white;
  padding-left: 30px;

}

.scroll-percent{
  position: fixed;
  bottom: 30px;
  left: 0;
  height: 22.5px;
  border-top-right-radius: 25px;
  border-bottom-right-radius: 25px;
  padding-left: 8px;
  width: 60px;
  background: rgba(255, 255, 255, 0.7);
  box-shadow: 0 0 6px;
  overflow: hidden;
  font-weight: 300;
}

.lightbox-div{
  background: rgba(0, 0, 0, 0.7);
  position: fixed;
  text-align: center;
  height: 100%;
  width: 100%;
  top: 0;
  left: 0;
  z-index: 9999;
}

.lightbox-div.hidden{
  display: none;
}

.lightbox-div div{
  height: 50%;
  top: 25%;
  position: relative;
}

.list-group-item{
  word-break: break-all;
}
.cattag-item{
  transition: padding-right .5s;
}

.cattag-item:hover{
  padding-right: 10px
}

#contact-form{

}

#contact-form iframe{
  width: 100%
}


#sidebar-nav{
  position: fixed;
  left: -300px;
  width: 300px;
  height: 100%;
  background: white;
  box-shadow: 0 0 6px;
  top: 80px;
  overflow: scroll;
  padding-bottom: 80px;
  transition: left 0.6s cubic-bezier(0.68, -0.55, 0.265, 1.55);
  z-index: 99999;
}

#sidebar-nav.show{
  left: 0;
  background: rgba(255, 255, 255, 0.7);
}
.sb_nav_head{
  border-bottom: 1px solid;
    display: inline-block;
    padding: 20px 10px;
    width: 100%;
}

.sb_nav_head span{
  font-size: 28px;
      margin-left: 10px;
      position: absolute;
      margin-top: 8px;
}

.sb_nav_lnk{
  font-size: 1.6rem;
    border-bottom: 1px solid;
    padding: 10px 18px;
    cursor: pointer;
    transition: all 0.6s;
}

.sb_nav_lnk:hover{
  padding-right: 10px;
    background: rgba(0, 0, 0, 0.60);
    color: white;
}

#rest-signup{
  margin: 50px 0;
}

.bnet{
  display: none;
}

#theme-mask{
  position: fixed;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    background: white;
    z-index: 999999;
}

#theme-mask > span {
  width: 100%;
  position: absolute;
  text-align: center;
  bottom: 50px;
  color: grey!important;
}
