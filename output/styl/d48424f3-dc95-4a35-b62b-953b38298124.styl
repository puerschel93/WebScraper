    // 显示二维码弹框 样式
    .code-modal {
      top: 15%;
      .modal-content {
        width: 450px;
        margin: 0 auto;
        .modal-header {
          .modal-title {
            font-size: 16px;
            font-family: "Microsoft YaHei";
            color: #727272
          }
        }
        .modal-body {
          display: flex;
          flex-direction: column;
          justify-content: center;
          align-items: center;
          .content {
            width: 200px
            height: 200px;
            display: flex;
            align-items: center;
          }
          .operation {
            display: flex;
            margin-top: 20px;
            margin-bottom: 20px;
            .download-btn,.print-btn,.cancel-btn {
              color: #68788c;
              border: 1px solid #68788c;
              height: 35px;
              line-height: 35px;
              width: 80px;
              text-align: center;
              cursor: pointer;
              -webkit-border-radius: 4px;
              -moz-border-radius: 4px;
              border-radius: 4px;
              margin-right: 20px;
              font-family: "Microsoft YaHei";
              text-decoration: none;
            }
            .download-btn:hover, .print-btn:hover, .cancel-btn:hover {
              color: #fff;
              background: #1093f4;
              border: transparent;
            }
          }
        }
      }
