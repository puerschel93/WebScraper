@font-face {
  font-family: 'Switzer-Variable';
  src: url('/webfonts/switzer/Switzer-Variable.woff2') format('woff2'),
       url('/webfonts/switzer/Switzer-Variable.woff') format('woff'),
       url('/webfonts/switzer/Switzer-Variable.ttf') format('truetype');
       font-weight: 100 900;
       font-display: swap;
