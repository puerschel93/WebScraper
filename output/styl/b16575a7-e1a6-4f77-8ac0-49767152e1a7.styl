.ra-menu {
    display: block!important;
}

.ra-wrapper {
    min-width: 0px!important;
}

.ra-button {
    display: none;
}

.go-to-top {
    color: rgb(11, 116, 222)!important;
    border-radius: 50%;
    width: 50px;
    height:30px;
    padding: 12px;
    background: white;
    box-shadow: lightgray 0px 1px 26px 0px;

}
 .ra-menu-item.active{
    color: rgb(11, 116, 222)!important;
    border-left: 1px solid #eaecef!important;
}

.ra-menu-item.active:before {
    content: '';
    display: inline!important;;
    position: absolute!important;;
    width: 7px!important;;
    height: 7px!important;;
    border-radius: 7.5px!important;;
    background-color: rgb(11, 116, 222)!important;
    left: -3px!important;;
    margin-top: 7px!important;;
}


.ra-menu-item:hover:before {
    content: '';
    display: inline;
    position: absolute;
    width: 7px;
    height: 7px;
    border-radius: 7.5px;
    background-color: #eaecef;
    left: -3px;
    margin-top: 7px;
}

.ra-menu-item:hover {
    color: rgb(11, 116, 222)!important;
    border-left: 1px solid #eaecef!important;
}

@media (max-width: 768px) {
  .anchor-fixed {
    position: fixed;
    right: -270px;
    top:0px;
    max-height: 100%;
    height: 100%;
    z-index: 100;
    transition: all 350ms cubic-bezier(0.4, 0, 0.2, 1) 0s;
    width: 250px;
    background: white;
  }
}

@media (max-width: 768px) {
  .anchor-fixed-mobile {
    position: fixed;
  }
}