.agenda-view
  .swiper-container
    height 100%
    .swiper-slide
      height 100%
    .fc-head
      .fc-widget-header
        border-bottom-width 0
        border-right-width 0
        border-left-width 0
    .fc-body
      .fc-widget-content
        border-bottom-width 0
        border-right-width 0
        border-left-width 0
      .fc-nonbusiness
        background-color #fff

.agenda-event
  display flex
  width 100%
  height 85px
  border-top none !important
  overflow hidden
  z-index 25
  .foto-wrap
    display flex
    width 7rem
    overflow hidden
  // padding-inline-end 0.5rem
  // -webkit-padding-end 0.5rem
  // -moz-padding-end 0.5rem
  .client-foto
    width 100%
    height 100%
    object-fit cover
.note-info
  box-sizing border-box
  padding-inline-end 0.5rem
  -webkit-padding-end 0.5rem
  -moz-padding-end 0.5rem
  width 100%
  display flex
  justify-content space-between
  .time-block
    box-sizing border-box
    display flex
    flex-direction column
    justify-content space-between
    align-items flex-end
    padding-top 0.5rem
    padding-inline-end 0.6rem
    -webkit-padding-end 0.6rem
    -moz-padding-end 0.6rem
    padding-bottom 0.3rem
    padding-inline-start 0
    -webkit-padding-start 0
    -moz-padding-start 0
    .start-duration
      display flex
      flex-direction column
      align-items flex-end
    .event-start
      font-size 10px
      font-weight bold
      color var(--main-color)
    .duration-wrap
      display flex
    .duration-img
      width 10px
      height 10px
      margin 0 3px
    .event-duration
      font-size 11px
      padding 0
      font-weight bold
      color grey
    .minutes
      padding 0 0.2rem
    .hours
      padding-inline-end 0.2rem
      -webkit-padding-end 0.2rem
      -moz-padding-end 0.2rem
    .img-ok
      width 10px
      height 10px
    .paid
      font-size 13px
      font-weight bold
      color var(--dark-color)
  .note-about-user
    box-sizing border-box
    width 170px
    flex-grow 1
    padding-top .5rem
    -webkit-padding-start  0.5rem
    -moz-padding-start 0.5rem
    padding-inline-start  0.5rem
    .name-wrap
      display flex
      font-size 13px
      font-weight bold
      white-space nowrap
      margin 0
    .client-name
      max-width 70%
      overflow hidden
      text-overflow ellipsis
    .client-icon
      display flex
      align-items center
      .bday
        .bday-img
          width 11px
          height 13px
          margin-inline-end 3px
          -webkit-margin-end 3px
          -moz-margin-end 3px
          margin-inline-start 5px
          -webkit-margin-start 5px
          -moz-margin-start 5px
        .bday-text
          font-size 10px
          line-height 11px
          color var(--dark-color)
      .new-cust
        .new-text
          font-size 10px
          line-height 11px
          color var(--dark-color)
        .new-client
          width 19px
          height 8px
          margin-inline-end 3px
          -webkit-margin-end 3px
          -moz-margin-end 3px
          margin-inline-start 5px
          -webkit-margin-start 5px
          -moz-margin-start 5px
    .dept
      font-size 10px
      line-height 11px
      color #ff4c4c
    img.dept-img
      width 4px
      height 8px
      margin-inline-end 3px
      -webkit-margin-end 3px
      -moz-margin-end 3px
      margin-inline-start 5px
      -webkit-margin-start 5px
      -moz-margin-start 5px

    .services-name
      margin 0
      white-space nowrap
      line-height 16px
      overflow hidden
      text-overflow ellipsis
      padding-bottom 4px
      .single-service
        font-size 13px
        font-weight 500
        // color var(--primaryBlackColor)
        color black
    .note-wrap
      display flex
      align-items center
      .note-img
        display block
        width 10px
        height 10px
        height auto
      .client-note
        box-sizing border-box
        font-size 12px
        line-height 15px
        font-weight 500
        margin 0
        padding-inline-start 0.3rem
        -webkit-padding-start 0.3rem
        -moz-padding-start 0.3rem
        white-space nowrap
        overflow hidden
        text-overflow ellipsis
        color var(--dark-color)
    .address-wrap
      display flex
      align-items center
      .address-img-wrap
        display flex
        align-items center
        .address_pin
          display block
          width 10px
          height auto
      // .img-circle
      //   position absolute
      //   top 17%
      //   width 3.5px
      //   height 3.5px
      // .img-address
      //   display block
      //   width 7px
      //   height 11px
      // height auto
      .address-line
        box-sizing border-box
        // width 97%
        font-size 12px
        line-height 15px
        font-weight 500
        margin 0
        padding-inline-start 0.3rem
        -webkit-padding-start 0.3rem
        -moz-padding-start 0.3rem
        white-space nowrap
        overflow hidden
        text-overflow ellipsis
        color #8b8b8b
.agenda-event
  .note-info
    .time-block
      border-inline-end 7px solid white
      -webkit-border-end 7px solid white
      -moz-border-end 7px solid white
    .address_pin
      display block
      width 10px
      height auto
.agenda-events
  .off_time
    display flex
    justify-content space-between
    width 100%
    height 85px
    overflow hidden
    background-color #ebebeb
    border-bottom 1px solid #ddd
    .off_time_wrap
      display flex
      align-items center
      justify-content center
      width 7rem
      .off_time_img
        width 46px
        height 40px
    .off_time_info
      display flex
    .note-about-user
      .name-wrap
        margin-bottom 5px
  .duration-time
    height 42px
    width 100%
    display flex
    align-items center
    justify-content center
    background-color #e1d4fd
    &-content
      width 100%
      display flex
      align-items center
      justify-content center
      // div
      //   margin 5px
      .duration-time_container
        display flex
        justify-content center
        align-items center
        width 10%
      .free-time
        display flex
        justify-content center
        align-items center
        width 21px
        height 21px
        border-radius 50%
        background-color var(--dark-color)
        .cross
          height 15px
          width 15px
          color #fff
      .free-time_title
      .free-time_value
        flex-grow 1
        width 45%
      .free-time_value
        display flex
        justify-content flex-end
      .free-time_title
        display flex
        justify-content flex-start
  .empty-page-wrapper
    display: flex
    flex-direction column
    align-items center
    min-height calc(100vh - 151px)
    .empty-page
      flex-grow 1
      display flex
      flex-direction column
      align-items center
      padding-bottom 4rem
      justify-content flex-end
      background-position-x center
      background-position-y -2.5rem
      background-repeat no-repeat
      .wrap-img
        text-align center
      .agenda-title
        text-align center
        font-size 24px
        letter-spacing 0.3px
        color var(--dark-color)
      .agenda-text
        display flex
        justify-content center
        flex-wrap wrap
        padding 1rem 4.5rem
        font-size 14px
        color var(--main-color)
        text-align center
      .order-wrap
        // width 10rem
        height 2.9rem
        margin-top 1rem
        display flex
        justify-content space-between
        align-items center
        border-radius 30px
        -webkit-padding-end 5px
        -moz-padding-end 5px
        padding-inline-end 5px
        -webkit-padding-start 24px
        -moz-padding-start 24px
        padding-inline-start 24px
        background-color var(--sec-bgcolor)
        color var(--white-color)
        .order-text
          font-size 13px
          text-transform uppercase
          flex-grow 1
          text-align center
          min-width 2.5rem
        .order-btn
          display flex
          align-items center
          justify-content center
          width 2.4rem
          height 2.4rem
          border-radius 50%
          background-color var(--main-bgcolor)
          margin-inline-start 0.7rem
          -webkit-margin-start 0.7rem
          -moz-margin-start 0.7rem
          .cross
            height 15px
            width 15px

.styleLoaderRTL
  right 30px
// bottom 30px
.styleLoaderLTR
  left 30px
.preloader
  position fixed
  bottom 30px
  // bottom 30px
  justify-content center
  align-items center
  z-index 20
  .loader
    width 56px
    height 56px
    animation 1s spin infinite linear
    opacity .8
  @keyframes spin
    from
      transform rotate(0deg)
    to
      transform rotate(360deg)
@media only screen and (min-width: 610px) {
  .styleLoaderLTR {
    left: 50%;
    transform: translate(-515%);
  }
  .styleLoaderRTL {
    right: 50%;
    transform: translate(515%);
  }
}
