  border-bottom: 2px solid #fff
  transform: rotate(-45deg)
  animation: anchor-animation-7 2s infinite
  opacity: 0
  box-sizing: border-box

.anchor-animation-7 a span:nth-of-type(1)
  animation-delay: 0s

.anchor-animation-7 a span:nth-of-type(2)
  top: 16px
  animation-delay: 0.15s

.anchor-animation-7 a span:nth-of-type(3)
