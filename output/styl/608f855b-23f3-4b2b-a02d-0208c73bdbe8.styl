body
    margin 0
    font 12px/2em "宋体"
    color #7C6F8D
    background rgb(34,28,16)
a
    text-decoration none
    color #786793
    outline none
    &:hover
        color #fff
img
    border none
.container
    background url($bg) no-repeat top center
    min-width 980px
.wrap
    background url($main) no-repeat center 450px
    position relative
.main
    width 980px
    height 1500px
    position relative
    margin 0 auto
.nav
    text-align left
    line-height 30px
    color #d9b070
    padding 0 190px 0 20px
    position relative
    a
        color #d9b070
        &:hover
            color #fff
#num
    font-size 24px
    font-weight 700
    width 120px
    right 185px
    top 3px
    position absolute
    color #fff
    text-align center
.logo
    position absolute
    width 168px
    height 88px
    right 35px
    top 5px
    text-indent -9999cm
#prize
    width 156px
    height 80px
