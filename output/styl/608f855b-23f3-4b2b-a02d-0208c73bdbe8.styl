        margin 10px 0 0 380px
    .feet_logo
        position absolute
        width 140px
        height 60px
        left 225px
        top 30px
.pop_video_detail
    background: #54200A;
    border 1px solid #873410
#pop_flash
    border 1px solid #873410
    background #000
    line-height 0
    height 350px
.ui-dialog .ui-dialog-titlebar-close
    background url($close) no-repeat
object
    outline none
.reg_panel
    width 283px
    color #c998ff
    right 3px
    overflow hidden
    height 390px
    position absolute
    top 518px
    right 5px
    .reg
        margin-left 40px
    a
        color #A696C7
        text-decoration underline
        &:hover
            color #fff
    table
        height 162px
        td
            height 27px
            width 20px
        input
            background #776595
            border 1px solid #683ec8
            height 18px
            color #221334
            line-height 18px
            width 145px
        .focus_in_inut
            background #af99d4
    .agreement
        height 21px
    .reg_form_item_name
        width 60px
    .reg_form_item_input
        width 150px
    .validate_num_input
        height 30px
        vertical-align top
        padding-top 3px
    #txtValidate
        width 50px
    #validateimg
        width 90px
        margin-left 5px
        height 27px
        vertical-align top
        cursor pointer
    #reg_step2,#reg_step3,#reg_step4,#reg_step5
        width 220px
        margin 0 auto
        padding-top 30px
        line-height 25px
        display none
        height 120px
        text-align center
        input
            background #776595
            border 1px solid #683ec8
            height 18px
            color #221334
            line-height 18px
            width 145px
    #reg_step2
        height 211px            
