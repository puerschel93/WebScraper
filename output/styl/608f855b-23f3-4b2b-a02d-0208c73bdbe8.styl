        position relative
    #reg_button
        width 120px
        height 30px
        position absolute
        left 51px
        top 11px
        background url($reg_btn) no-repeat -124px -61px
        &:hover
            background-position -124px -93px
    .success
        background url($success) no-repeat
        padding 10px 40px
        display inline
        color #FAAB10
    .fail
        background url($fail) no-repeat
        padding 10px 40px
        display inline
        color #FAAB10
    .back_bg
        position absolute
        top 360px
        height 20px
        width 100%
        background url($download) no-repeat
    #back
        width 120px
        height 30px
        position absolute
        top 40px
        left 81px
        background url($reg_btn) no-repeat -267px -125px
        &:hover
            background-position -267px -157px
    .check_result_wrong
        background url($right_wrong) no-repeat 0 -32px
        height 100%
        width 16px
    .check_result_right 
        background: url($right_wrong) no-repeat 0 4px
        height: 25px;
        width: 16px;

    .check_result_loading 
        background url($loader) no-repeat 0 4px
        height 25px
        width 16px
.disnone
    display none
