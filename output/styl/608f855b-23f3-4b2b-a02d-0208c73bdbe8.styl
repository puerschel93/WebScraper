            width 238px
            height 134px
            position absolute
            top 22px
            left 233px
            background #000
            overflow hidden
    #flash_but_2 ,#j_video
        cursor pointer
        background none
    #j_video
        background url($btn) no-repeat 999cm
    #to_reg_3
        width 95px
        height 24px
        background url($btn2) no-repeat -5px -95px
        position absolute
        top 220px
        right 50px
        cursor pointer
        &:hover
            background-position -5px -119px
#img_scroll
    width 256px
    height 217px
    position absolute
    top 945px
    right 16px
    .button
        width 17px
        height 58px
        float left
        cursor pointer
        margin-top 75px
    .scroll_box
        overflow hidden
        width 222px
        float left
        height 100%
        #scroll_body
