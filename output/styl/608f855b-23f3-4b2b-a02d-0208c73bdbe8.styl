        margin 20px auto
        width 120px
        height 30px
        background url($reg_btn) no-repeat -2px -61px
        &:hover
            background-position -2px -93px
    .download
        height 66px
        padding 0 10px
        position relative
        margin 0 auto
        background url($download) no-repeat
        .mini_download
            height 30px
            width 125px
            display block
            position relative
            top 80px
            float left
            background url($reg_btn) no-repeat -13px -125px
            &:hover
                background-position:-13px -157px
        .full_download
            height 30px
            width 125px
            display block
            position relative
            top 80px
            float right
            background url($reg_btn) no-repeat -140px -125px
            &:hover
                background-position:-140px -157px
    .download_tip
        height 40px
        line-height 30px
        margin-top 80px
        text-align center
        font-size 14px
        padding 0 40px
        text-align left
    .reg_submit
