.out-side-cart {
  background-color: #f4f4f4;
  min-height: 100vh;
}

.out-side-cart .header-address {
  width: 2.8rem;
  height: .5rem;
  line-height: .5rem;
  padding-left: .35rem;
  font-size: .28rem;
  color: #fff;
  position: absolute;
  left: .2rem;
  top: .19rem;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -3.44rem / .7rem 10rem;
}

.out-side-cart .header-cart-btn {
  display: inline-block;
  height: .5rem;
  line-height: .5rem;
  font-size: .28rem;
  color: #fff;
  position: absolute;
  right: .2rem;
  top: .19rem;
}

.out-side-cart .cart-main {
  background-color: #fff;
  overflow: hidden;
  padding: .88rem 0 2rem;
}

.out-side-cart .cart-main .cart-number {
  height: .66rem;
  line-height: .66rem;
  font-size: .28rem;
  color: #1a1a1a;
  padding-left: .2rem;
}

.out-side-cart .cart-list {
  width: 7.1rem;
  height: 2.5rem;
  box-shadow: 0 0 .05rem .05rem rgba(0, 0, 0, .03);
  margin: 0 auto .2rem;
}

.out-side-cart .cart-list .check-single {
  width: .42rem;
  height: .32rem;
  float: left;
  margin: 1.1rem 0 0 .12rem;
  overflow: hidden;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -.64rem / .45rem 6.43rem;
}

.out-side-cart .cart-list .check-single.this-over {
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -.3rem / .45rem 6.43rem;
}

.out-side-cart .cart-list .cart-img {
  width: 2rem;
  height: 1.9rem;
  overflow: hidden;
  border-radius: .1rem;
  float: left;
  margin: .3rem .15rem 0 0;
}

.out-side-cart .cart-list .cart-img img {
  width: 2rem;
  height: 2rem;
}

.out-side-cart .cart-list .cart-mess-bar {
  width: 4.3rem;
  height: 2.1rem;
  float: left;
  margin-top: .26rem;
  position: relative;
}

.out-side-cart .cart-list .cart-mess-bar .name {
  font-size: .28rem;
  color: #262626;
}

.out-side-cart .cart-list .cart-mess-bar .spec {
  display: inline-block;
  font-size: .22rem;
  color: #b2b2b2;
  position: absolute;
  top: .8rem;
}

.out-side-cart .cart-list .cart-mess-bar .return {
  display: inline-block;
  font-size: .4rem;
  color: #fe7600;
  position: absolute;
  left: -.45rem;
  top: 1.1rem;
  padding: .02rem .06rem;
  border: 1px solid #fe7600;
  border-radius: 2px;
  transform: scale(0.5);
}

.out-side-cart .cart-list .cart-mess-bar .price {
  display: inline-block;
  font-size: .38rem;
  color: #e62343;
  position: absolute;
  top: 1.58rem;
  line-height: .38rem;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box {
  display: inline-block;
  position: absolute;
  right: 0;
  bottom: .16rem;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box span {
  width: .5rem;
  display: inline-block;
  padding: .08rem 0;
  font-size: .22rem;
  color: #262626;
  float: right;
  border-top: .5px solid #dcdcdc;
  border-left: .5px solid #dcdcdc;
  border-bottom: .5px solid #dcdcdc;
  text-align: center;
  overflow: hidden;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box .jia {
  border-right: .5px solid #dcdcdc;
  border-bottom-right-radius: .05rem;
  border-top-right-radius: .05rem;
  color: #a0a0a0;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box .jian {
  border-bottom-left-radius: .05rem;
  border-top-left-radius: .05rem;
  color: #a0a0a0;
}

.out-side-cart .cart-list .cart-mess-bar .cart-btn-box .jian:active,
.out-side-cart .cart-list .cart-mess-bar .cart-btn-box .jia:active {
  background-color: #ececec;
}

.out-side-cart .cart-footer {
  position: fixed;
  width: 100vw;
  height: 1.05rem;
  border-top: .5px solid #eee;
  left: 0;
  bottom: .95rem;
  overflow: hidden;
  background-color: #fff;
}

.out-side-cart .to-pay {
  width: 2.12rem;
  height: 1.05rem;
  line-height: 1.05rem;
  text-align: center;
  position: absolute;
  right: 0;
  top: 0;
  background-color: #e62343;
  font-size: .28rem;
  color: #fff;
}

.out-side-cart .to-pay.gray {
  background-color: #ccc;
}

.out-side-cart .cart-footer-mess {
  padding-right: 2.3rem;
}

.out-side-cart .cart-footer-mess .t {
  padding-top: .1rem;
  text-align: right;
}

.out-side-cart .cart-footer-mess.single .t {
  padding-top: 0;
  line-height: 1.05rem;
}

.out-side-cart .cart-footer-mess .t .f-type1 {
  font-size: .26rem;
  color: #1a1a1a;
}

.out-side-cart .cart-footer-mess .t .f-type2 {
  font-size: .34rem;
  color: #e62343;
}

.out-side-cart .cart-footer-mess .t .f-type3 {
  font-size: .22rem;
  color: #595959;
}

.out-side-cart .cart-footer-mess .b {
  padding-top: .05rem;
  text-align: right;
  font-size: .22rem;
  color: #595959;
}

.out-side-cart .btn-select-all {
  display: inline-block;
  position: absolute;
  padding-left: .4rem;
  height: .35rem;
  overflow: hidden;
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -.63rem / .45rem 6.43rem;
  left: .1rem;
  top: .38rem;
}

.out-side-cart .btn-select-all.this-over {
  background: url('https://api.duishangbao.cn/upload/image/icon_zu.png') no-repeat 0 -.29rem / .45rem 6.43rem;
}

.out-side-cart .btn-cart-del,
.out-side-cart .btn-cart-move {
  display: inline-block;
  font-size: .26rem;
  color: #999;
  border: .5px solid #999;
  padding: .1rem .3rem;
  float: right;
  margin: .22rem .2rem 0 0;
  border-radius: .6rem;
}

.out-side-cart .btn-cart-del {
  color: #e62343;
  border: .5px solid #e62343;
}

.cart-null {
  text-align: center;
}

.cart-null .cart-null-icon {
  padding: 2.8rem 0 .5rem;
}

.cart-null .cart-null-icon img {
  width: 3.08rem;
  height: 2.24rem;
}

.cart-null .note1 {
  font-size: .36rem;
  color: #404040;
}

.cart-null .note2 {
  font-size: .28rem;
  color: #666;
  padding: .2rem 0 .5rem;
}

.cart-null .cart-null-btn a {
  display: inline-block;
  font-size: .36rem;
  color: #e62343;
  border: .5px solid #e62343;
  border-radius: .8rem;
  padding: .2rem 1.8rem;
  margin: 0 auto;

}
