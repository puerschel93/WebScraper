// HELPER Icono
.icon-ionicon
	&:before
		display inline-block
		font-family 'Ionicons'
		speak none
		font-style normal
		font-weight normal
		font-variant normal
		text-transform none
		text-rendering auto
		line-height 1
		--webkit-font-smoothing antialiased
		moz-osx-font-smoothing grayscale
	&--flecha-derecha
		&:before
			content '\f125'
	&--fs-10
		&:before
			font-size 10px
	&--fs-11
		&:before
			font-size 11px
	&--fs-12
		&:before
			font-size 12px
	&--fs-13
		&:before
			font-size 13px
	&--fs-14
		&:before
			font-size 14px
	&--fs-15
		&:before
			font-size 15px
	&--fs-16
		&:before
			font-size 16px
	&--fs-17
		&:before
			font-size 17px
	&--fs-18
		&:before
			font-size 18px

