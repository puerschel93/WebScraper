.banner
	width 100%
	height 720px
	background-color #23242C
	background-image url('../../assets/images/banner3.png')
	background-repeat no-repeat 
	background-size contain
	background-position-x 112%
	display flex
	align-items center
	img
		margin-right 0px
		margin-left 300px

	.banner_wrap
		width 70%
		margin 0 auto
		padding-bottom 128px
		.banner-title
			width 64%
			font-size 48px
			font-weight bold
			color rgba(255,255,255,1)
			line-height 45px;
			letter-spacing 1px
			margin-bottom 40px
			font-weight 600
		.banner-p
			width 64%
			font-size 24px
			color rgba(255,255,255,0.7)
			line-height 30px
			margin-top 48px
