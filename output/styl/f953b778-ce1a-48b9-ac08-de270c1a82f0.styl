/*NameCardHover组件样式*/
.row {
  border-left: 1px solid #314257;
  border-right: 1px solid #314257;

  &:first-child {
    border-top: 1px solid #314257;
  }
}
.row1-left {
  border-right: 1px solid #314257;
}
.row1-left .up {
  .row1-up-left {
    border-right: 1px solid #314257;
    border-bottom: 1px solid #314257;
  }
}
.item {
  border-bottom: 1px solid #314257;
}
// 新版
.processEventDetail, .processModifyDetail, .processDetail
  padding: 20px 20px 10px
  a
    color #199fed
    &:hover
      color hover_text
  .leftMenu
    color: rgba(255, 255, 255, 0.65);
    // padding-bottom 50px
    .title
      display: inline-block;
      width: 90%;
      font-size: 14px
      line-height 36px
      text-align: left
      color link_text
      span
        display inline-block
        width: 2px
        height: 12px
        background original_l1_white
        vertical-align middle
        margin-right 5px
  .formContainer
    padding 0 10px
  .detailBox
    .title
      font-size: 14px
      color link_text
      line-height: 36px
      margin-top: 10px
  .upload
    display inline-block
  .fileIcon
    font-size 18px
    margin-left 5px

.processEventDetail, .processModifyDetail, .processDetail
  .workInfo
    margin-top: 10px
    background-color block_bg
    .title
      border-top 1px dashed original_l1_black
  .success-state
    color green_color
  .fail-state
    color red_color

/* 详情页左侧 */
.process-detail-title
  display: flex
  justify-content: flex-start
  width: 175px
  height: 53px
  color: #fff
  text-align: center
  background: linear-gradient(104.71deg, theme_color_hover 3.04%, theme_iconcolor 95.54%);
  & > span
    height: 53px
    line-height: 53px
    font-size: 14px
    font-weight: 500
    &.process-detail-text
      flex: 1
    &.process-detail-icon
      width: 36px
      margin-left: 25px
      &.event
        background: url('../../../static/images/processCenter/dark/event-detail-title.png') center no-repeat
      &.modify
        background: url('../../../static/images/processCenter/dark/modify-detail-title.png') center no-repeat
      &.problem
        background: url('../../../static/images/processCenter/dark/problem-detail-title.png') center no-repeat

.processDetail, .processModifyDetail
  box-sizing: border-box
  margin: 0
  padding: 20px 20px 0 0
  .leftMenu
    background: #252e39
    color: rgba(255, 255, 255, 0.65);
    .title
      width: 175px
      height: 30px
      line-height: 30px
      font-size: 14px
      text-align: center
      color: #fff
    .s-button
      width: 158px
      height: 40px
      border: none
      background: #252e39
      text-align: left
      text-indent: 6px
      .iconfont
        color: theme_iconcolor
      &:hover
        color: theme_iconcolor
      &.is-disabled
        color: rgba(255, 255, 255, 0.25);
        .iconfont
          color: rgba(255, 255, 255, 0.25);
    .attention
      background: theme_iconcolor
      border: 1px solid theme_iconcolor
      border-radius: 22px
      width: auto;
      height: 32px;
      text-align: center;
      text-indent: 0;
      color: #fff;
      margin-left: 30%
      margin-bottom: 10px
      &.is-disabled
        background: rgba(255, 255, 255, 0.05)
        border: 1px solid rgba(255, 255, 255, 0.05)
        color: rgba(255, 255, 255, 0.25);
      &:hover
        color: #fff
      &.is-disabled:hover
        background: rgba(255, 255, 255, 0.05)
        border: 1px solid rgba(255, 255, 255, 0.05)
        color: rgba(255, 255, 255, 0.25);
    .attention-empty
      background: none
      color: theme_iconcolor;
      margin-left: 24%
      &:hover
        color: theme_iconcolor
.processModifyDetail, #taskOrderDetail, .processProblemDetail, .processEventDetail
  .disabledIcon
    color gary_l1_black !important
    cursor not-allowed !important
  .clickLink, .addWorkInfo .title
    color link_text

.name-card
  .key
    color #aec2d6

.flowchart-float-window
  width 100%
  padding: 10px
  .float-title
    line-height 20px
    height 20px
    padding-top 5px
    color #2aa8ff
  th, td
    text-align left
  thead
    tr th
      color #e5f5fa
      background-color #092f51
      line-height 28px
      text-indent 4px
  tbody
    tr td
      max-width 35px
      color #e5f5fa
      line-height 28px
      border-bottom 1px #1d1e21 solid
      white-space nowrap
      text-overflow ellipsis
      overflow hidden
      text-indent 4px

.templateDialog
  .s-radio-group
    width 100%
    cursor pointer
    .s-table
      .s-radio__inner
        vertical-align middle
      .s-radio__label
        display none
  .temDetailBox
    border: 1px solid original_l1_black
    padding 15px 5px 0
    position relative
    &:after
      content: '详细信息'
      color: title_3
      font-weight: 600
      font-size: 14px
      padding: 5px 10px
      position absolute
      top: 0
      left: 10px
      transform: translateY(-50%)
      background block_bg

.s-table tbody .cell .iconfont
  &.failed-state
    color: red_color
  &.success-state
    color: green_color

