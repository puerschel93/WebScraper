.service-level-agreement
  .sla-title
    background: linear-gradient(104.71deg, theme_color_hover 3.04%, theme_iconcolor 95.54%);
    color rgba(255,255,255,85)
    display: flex
    justify-content: center
    align-items:center
    height: 42px
    // background: url('../../../static/images/processCenter/fanta/title-bg.png') center no-repeat
    background-size: 150% 150%
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    // margin:0 3px;
    font-weight: 600;
    .sla-title-icon
      width: 36px;
      height: 36px;
      margin-right: 5px;
      margin-left: -8px;
      // background: url('../../../static/images/processCenter/fanta/SLA.png') center no-repeat

  .slaAddDialog legend
    color: title_1

  .s-tree-node.is-current > .s-tree-node-content
    border: solid 1px #e4e7ed;
    background-color: #fff;

  .listChunk
    position: relative;
    .searchFormChunk
      padding: 10px 15px;
      border: solid 1px #e4e7ed;
      border-radius: 5px;
      background: #f2f6fc;
      margin-bottom: 8px;
      .s-form-item
        margin-bottom: 0;
      .right
        float: right;
        margin-top: 3px;
  .sidebar
    .s-tabs
      position: relative;
      padding-top:20px;
      margin:0 3px;
      // background: url('../../../static/images/processCenter/fanta/menu-bg.png') center
      background-size: 120% 100%;
      .s-tabs-content
        overflow: visible;
      .slaplus
        color: link_text
        font-size:20px;
        cursor:pointer;
        position: absolute;
        right: 0;
        top: -42px;
        z-index: 100;

  .leftMenu
    background #f5f7fa
    .title
      height: 41px
      line-height: 41px
      font-size: 14px
      text-align: center
      color: rgba(0,0,0,.85)
      // background: url('../../../static/images/processCenter/fanta/menu-title-bg.png') center no-repeat
      background-size: 114.5% 100%;
      background-position: 43% 50%;
    .leftMenu-item
      display: flex;
      margin:0 3px;
      .s-button
        flex:1;
        margin:0;
        height: 40px
        border: none
        color: rgba(0,0,0,.85)
        background #f5f7fa
        // background: url('../../../static/images/processCenter/fanta/menu-bg.png') center no-repeat
        // color: link_text
        text-align: left
        text-indent: 20px
        background-size: 120%;
        i
          color: theme_iconcolor
        &:hover
          // background: url('../../../static/images/processCenter/fanta/menu-bg-hover.png') center no-repeat
          color: theme_iconcolor
          background-size: 114.5% 100%;
          background-position: 60% 50%;
        &.is-disabled
          // background: url('../../../static/images/processCenter/fanta/menu-bg-disabled.png') center no-repeat
          color: gray_l2_text
          background-size: 114.5% 100%;

