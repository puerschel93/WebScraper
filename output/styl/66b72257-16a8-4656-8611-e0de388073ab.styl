.popup-info
  display inline-block
  padding 18px 32px
  margin-top -86px
  margin-bottom 15px
  text-align left
  border-radius 16px
  background-color $colorWhite
  box-shadow 0 2px 15px 0 $colorDarkA15
  letter-spacing -0.2px
  margin-right -10px
  height 154px
  width 341px

  @media screen and (max-width: $mobile)
    box-shadow none
    border-radius 0
    padding 20px
    margin 0
    height 106px
    width 100%


.popup-info__title
  font-plain()
  font-bold()
  font-default()
  line-height 20px

  @media screen and (max-width: $mobile)
    display none

.popup-info__price
  font-plain()
  font-regular()
  font-default()
  margin 13px 0

  & .popup-info__number
    // background url("data:image/svg+xml;base64,") no-repeat 102% 2px
    background-size 14px
    font-bold()

.popup-info__price-commission
  font-small-1()
  font-regular()
  font-secondary()

.popup-info__number
  // background url("data:image/svg+xml;base64,") no-repeat 102% 1px
  // background url("data:image/svg+xml;base64,") no-repeat 102% 1px
  background-size 11px
  padding-right 15px
  float right

.popup-info__count
  font-small-1()
  font-regular()
  font-default()

  @media screen and (max-width: $mobile)
    display none
