html
  height:100%
body
  height:100%
  position:relative
.login
  background:#F7F7F7
  width:100%
  height:100%
  input:focus
    outline:none
.headerlogin
  width:100%
  height:100%
  color:#191919
  height:40px
  top: 0px;
  .header-wrapper
    display:flex
    display: -webkit-flex 
    display: -ms-flexbox 
    height:100%
    .header-left
      flex:1
      -webkit-box-flex: 1
      -webkit-flex: 1
      height:100%
      line-height:40px
      text-align:left
      .icon-arrow_lift
        font-weight:300
        margin-left:10px
      .goback
        font-size:14px
        position:relative
        font-weight:400
        top:-.0625rem
    .header-content
      flex:2
      line-height:40px
      text-align:center
      .text
        font-size:16px
    .header-right
      flex:1  
      -webkit-box-flex: 1
      -webkit-flex: 1
      text-align:right
      .text
        color:#0087BF
        line-height: 40px
        margin-right:10px
.login-center
  flex-direction: column
  display: flex
  text-align:center
  .login-logo
    flex:1
    padding:60px 0px 60px 0px
    font-weight: 700
    font-size:32px
    width:100%
    .loginLogo
      width:80%
      margin:0px auto
      .logoImg
        width:100%
    .login-logo-text
      color:#999999
      font-size:14px
  .login-form
    width:90%
    margin:0px auto
    .login-user
      width:100%
      height:40px
      border-1px(#E3E3E3)
      margin-bottom: 5px
      &.registersType
        display:flex
        display: -webkit-flex 
        display: -ms-flexbox
        position:relative
        &:after
          border-bottom: none
        .registerType
          flex:1  
          -webkit-box-flex: 1
          -webkit-flex: 1
          line-height:40px
          color:#989898
          .registerLabels
            display: inline-block
          &.on
            color:#0087BF
          .registerRaio
            position:absolute
            z-index:-1
            opacity: -1
      .setNewPasswordText
        color:#9D9D9D
        font-size:14px
        display: inline-block
        margin-top:10px
      .user-input
        width:100%
        height:100%
        background:#F7F7F7
        transition: all 0.5s
        &.registerVCode
          padding-right:100px
        &.findpassword-text
          font-weight: 700
          padding:0
          width:100%
          padding-right:20%
        &.setVcode
          color:#999
          font-size:14px
          padding:0px 25% 0 0
          width:100%
          &:focus
            color:#333
            outline:none
      .registerVCodeSpan
        display: inline-block
        width:30%
        height:100%
        position:absolute
        top:0
        right:0
        line-height:40px
        color:#008CD0
        &:before
          content:" "
          display:block
          width:1px
          height:20px
          background: #ccc
          position:absolute 
          top:10px
          left:0
      .vcodeText
        color:#9E9E9E
        position:absolute
        top:9px
        right:15px
        .vCodeIcon
          display: inline-block
          width:30px
          height:30px
          font-size:22px
          margin-right:50px
          opacity: 0
      .icon-keyboard_arrow_right
        display: inline-block
        content:' '
        font-size:30px
        width:20px
        height:20px
        position:absolute
        top:6px
        right:10px
        font-weight: 400
        color:#c0c0c0
        z-index:2
      .icon-Eyes-closed,.icon-eyes
        font-size:30px
        position:absolute
        top:7px
        right:0
        transition: all 0.5s
    .login-user-bottom
      margin-top:30px
      border-radius:2px
      box-shadow: 1px 3px 3px #ccc
    .login-user-text
      text-align: right
      margin-top:20px
      span
        a
          color:#0087bf
.login-footer
  width:100%
  text-align: center
  position:absolute
  bottom:10%
  left:0
  span
    color:#999999
    font-size:14px
  a
    color:#0087bf
.signOut
  width:100%
  height:100%
  bg-image('img/signOut')
  background-size:contain
  background-repeat:no-repeat
  .signOut-top
    width:100%
    height:60%
    display:flex
    flex-direction:column
    position:relative
    padding:10px 0px
    color:#FEFFFD
    .orderNumber
      padding:0px 15px
      font-size:14px
      .aRight
        color:#fff
        float:right
    .CanvasPoint
      #CanvasPoint
        display:block
        margin:0px auto
    .price
      width:100%
      height:40px
      line-height:20px
      position:absolute
      text-align: center
      top:45%
      margin-top:-10px
      font-size:25px
      .priceText
        font-size:16px
  .pointul
    width:100%
    height:40px
    display:flex
    display:-webkit-flex
    display:-webkit-box-flex
    display:-ms-flexbox
    position:relative
    top:-25px
    .pointullist
      width:100%
      height:100%
      line-height:40px
      position:relative
      .pointullistLeft
        display:inline-block
        width:100%
        height:100%
        text-align:right
        padding-right:15px
        color:#000
        &:after
          content:' '
          display:inline-block
          width:1px
          height:20px
          background:#ccc 
          position:absolute
          top:11px
          right:0
      .pointullistRight
        display:inline-block
        width:100%
        height:100%
        padding-left:15px 
        color:#000
  .pointInfoList
    width:100%
    height:auto
    
  .signOut-bottom
    width: 100%
    height:40%
    padding-top:20%
    display:flex
    flex-direction:column
    .siginOut-buttom
      text-align: center
      margin-bottom:10px
      a
        color:#FEFFFD
        width:100px
        height:30px
        line-height:30px
        font-weight: 700
        border-radius: 15px
        -moz-box-shadow:1px 6px 18px #FAE7C7, 0px 5px 10px #FAE7C7; -webkit-box-shadow:1px 6px 18px #FAE7C7, 0px 5px 10px #FAE7C7; box-shadow:1px 6px 18px #FAE7C7, 0px 5px 10px #FAE7C7;
        l-gradient()
        &:after
          border:none
    .signOut-location
      width:100%
      padding:0px 15px
      color:#ff5200
      font-weight: 700
      .icon-location2
        margin-right:5px
.xiaoxi
  position:relative
  .infomationNone
    width:100%
    position:absolute
    top:50%
    margin-top:-120px
    text-align: center
    img
      display:inline-block
      margin-bottom:10px
    p
      color:#999999
  .infomation
    padding:10px 15px
    display: flex
    flex-direction:column
    .messages
      display:flex
      border-1px(#F2F2F3)
      &.systemMessages
        .messimg
          bg-image("img/systemMessages")
      &.orderMessages
        .messimg
          bg-image("img/orderMessages")
      .photo
        flex:0 0 80px
        width:80px
        height:80px
        padding:10px
        .messimg
          display:inline-block
          width:100%
          height:100%
          position:relative
          background-size:cover
          .newsmess
            display:inline-block
            width:10px
            height:10px
            background:#FF3B00
            position:absolute
            top:0
            right:0
            border-radius:50%
      .messContent
        padding:10px 0px
        .messTitle
          color:#686868
          font-size:16px
          font-weight: 700
          margin-bottom:10px
        .messhint
          display: block
          font-size:14px
          padding:5px 10px
          background:#f2f2f2
          color:#B4B4B4
.infomationList
  width:100%
  height:100%
  background:#F2F2F2
  .messagesList
    width:100%
    display: flex
    flex-direction:column
    padding:15px 10px 0px
    .messTime
      text-align:center
      margin-bottom:10px
      .messTimes
        display: inline-block
        font-size:14px
        width:190px
        background:#D4D4D4
        height:20px
        line-height:20px
        color:#FDFDFD
        border-radius:5px
    .messContent
      padding:15px
      background:#fff
      .messContentTitle
        color:#3D3D3D
        font-size:15px
        margin-bottom:20px
        font-weight: 700
      .messContentInfo
        padding-bottom:15px
        border-1px(#F4F3F4)
        margin-bottom:10px
        span
          color:#9E9E9E
          font-size:14px
      .messContentDel
        text-align: right
        .delete
          color:#1EB1F6
 
      
