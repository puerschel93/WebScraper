@css {
  .kompas-live {
    @apply pb-4
  }

  .kompas-live .article-headline {
    @apply font-bold;
    @apply font-chronicle-display;
    @apply leading-tight;
  }

  .kompas-live .article-headline.small {
    @apply text-3xl;
  }

  .kompas-live .article-headline.smallest {
    @apply mb-2;
    @apply text-base;  
  }

  .kompas-live .article-headline a {
    @apply text-gray-600;  
  }

  .kompas-live .article-headline a:hover {
    text-decoration: underline;
  }

  .kompas-live .border-separator-bottom {
    @apply border-b-3;
    @apply border-gray-100;
    border-style: double;  
  }

  .kompas-live .live-blog-belt-container {
    @apply pb-4
  }

  .kompas-live .live-blog-belt-container a {
    @apply text-white;
  }

  .kompas-live .live-blogging-load-more {
    @apply pb-4;
    border-top: solid 1px #ddd;
    margin: 16px 0 0 -80px;
  }

  .kompas-live .live-blogging-load-more a {
    @apply capitalize;
    @apply font-bold;
    @apply font-pt-sans;
    @apply text-gray-600;
    @apply text-sm;
  }

  .kompas-live .live-blogging-timeline {
    @apply mb-5;
    @apply mt-0;
    @apply mx-0;
    @apply relative;
    @apply overflow-auto;
    list-style: none;
    padding: 1.25rem 1.25rem 0 5.3125rem;
    max-height: 37.5rem;
  }

  .kompas-live .live-blogging-timeline::-webkit-scrollbar {
    width: 0.5rem;
  }
  
  .kompas-live .live-blogging-timeline::-webkit-scrollbar-thumb {
    border-radius: 0.625rem;
    background: #cacaca;
  }
  
  .kompas-live .live-blogging-timeline::-webkit-scrollbar-track {
    -webkit-border-radius: 0.625rem;
    border-radius: 0.625rem;
    background-color: #eee;
  }

  .kompas-live .live-blogging-timeline .live-blogging-list {
    @apply block;
    @apply ml-5;
    @apply relative;
    padding-bottom: 0.625rem;
  }

  .kompas-live .live-blogging-timeline .live-blogging-list:before {
    @apply absolute;
    content: '';
    width: 0.0625rem;
    height: 100%;
    border-left: solid 0.125rem #cacaca;
    left: -1.25rem;
  }

  .kompas-live .live-blogging-timeline .live-blogging-list .live-blogging-content p {
    @apply font-pt-sans;
    @apply leading-tight;
    @apply mb-4;
    @apply text-sm;
  }

  .kompas-live .live-blogging-timeline .live-blogging-list .live-blogging-content .article-tools-list-item-block {
    @apply hidden;  
  }

  .kompas-live .live-blogging-timeline .live-blogging-list .live-blogging-content .live-blogging-saparator {
    @apply absolute;
    @apply top-0;
    left: -1.875rem;
  }

  .kompas-live .live-blogging-timeline .live-blogging-list .article-tools-list-item-block {
    @apply block;
    @apply float-right;
  }

  .kompas-live .live-blogging-timeline .live-blogging-time:before {
    content: "";
    width: 12px;
    height: 12px;
    border-radius: 50%;
    border: solid 3px #cacaca;
    position: absolute;
    left: -25px;
    background-color: #fefefe;
    top: 7px;
  }

  .kompas-live .live-blogging-timeline .live-blogging-time span {
    @apply absolute;
    @apply bg-gray-10;
    @apply font-pt-sans;
    @apply top-0;
    @apply text-xs;
    left: -102px;
    padding: 3px 5px;
    border: solid 1px #ddd;
  }

  .kompas-live .live-blogging-timeline .live-blogging-time span:after {
    @apply absolute;
    @apply block;
    content: '';
    top: 50%;
    right: -10px;
    bottom: auto;
    left: auto;
    border-width: 7px 0 7px 10px;
    border-color: transparent #ddd;
    border-style: solid;
    width: 0;
    transform: translateY(-50%);
  }

  .kompas-live .live-blogging-timeline .live-blogging-time span:before {
    @apply absolute;
    @apply block;
    content: '';
    right: -8px;
    top: 50%;
    bottom: auto;
    left: auto;
    border-width: 7px 0 7px 10px;
    border-color: transparent #fefefe;
    border-style: solid;
    width: 0;
    transform: translateY(-50%);
    z-index: 2;
  }

  .kompas-live .live-section-name-container {
    @apply bg-red-thunderbird-700;
    @apply flex;
    @apply items-center;
    @apply leading-tight;
    @apply font-pt-sans;
    @apply text-sm;
    @apply uppercase;
    @apply px-4;
    @apply py-2;
  }

  .kompas-live .text-bold {
    @apply font-bold;  
  }

}




