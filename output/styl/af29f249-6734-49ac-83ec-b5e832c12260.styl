.social
	pos relative
	df(stretch)
	cursor pointer
	width vw(23)
	z-index 1
	fill #fff
	transition all .3s
	margin-bottom vw(16)
	z-index 3
	.ya-share2
		position absolute
		top 50%
		transform translate(0, -50%)
		width 0
		@media $mobile
			top -73%
	@media $mobile
		width auto
		margin-right auto
		align-items center
		justify-content flex-end
	&:hover
		.ya-share2
			position absolute
			w vw(225)
			opacity 1
			visibility visible
			@media $mobile
				pr 0
				height vm(148)
				mr 0
				width vm(20)
			&__container
				@media $mobile
					/.ios &
						w 100%
		& + .social__mobiler
			@media $mobile
				size vm(50)
	&__mobiler
		size 0
		transition 0s .1s width, 0s .1s height
		@media $mobile
			pos absolute
			r 0
			t 0
			z-index 3
	&__bg
		dn()
		pos fixed
		bgc alpha(black, .62)
		t 0
		l 0
		size 100%
		z-index -1
	&__share
		size vh(19) vh(17)
		@media $tovw
			size vw(19) vw(17)
		@media $mobile
			size vm(37) vm(31)
		&-wrapper
			pos relative
			z-index 1
			df(center, center)
			size vw(22) vw(34)
			@media $mobile
				size vm(21)
				mb vm(5)
	.ya-share2
		df(center, center)
		w 0px
		opacity 0
		visibility hidden
		overflow hidden
		transition .35s opacity, .35s visibility, width .35s
		pl vw(36)
		pb vw(3)
		@media $mobile
			position absolute
			bottom 140% 
			pl 0
			w vm(20)
			h 0
		&__list
			df()
			@media $mobile
				flex-direction column
		.ya-share2__list.ya-share2__list_direction_horizontal
			@media $mobile
				mt	vm(-71)
		&__item
			margin-right vw(17) !important
			&:last-child
				margin-top vw(3) !important
				@media $mobile
					margin-top vm(10) !important
			@media $mobile
				margin-top vm(10) !important
			&:last-child
				margin-right 0 !important
			&:first-child
				margin-bottom 0 !important
				@media $mobile
					margin-top 0 !important
			& + .ya-share2__item
				@media $desktop
					ml vw(5)
				@media $mobile
					ml vm(10)
					mb vm(10) !important
		&__container
			w 100%
			@media $mobile
				/.ios &
					w 0
		&__link
			db()
			flex 0 0 1em
			font-size vw(30)
			line-height 1
			c white
			transition color 0.2s, .35s opacity
			size vw(22)
			@media $mobile
				size vm(20)
		&__title
			display none
		&__title,
		&__badge
			vertical-align middle
		&__badge
			background transparent !important
		&__badge,
		&__icon,
		.icon
			display block
			box-sizing border-box
			width 100%!important
			height 100%!important
		&__icon
			padding 0
			background-repeat no-repeat
			background-size cover !important
			@media $desktop
				transition .35s opacity
				&:hover
					opacity 1
			@media $mobile
				background-position center
