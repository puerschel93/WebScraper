	.video-container
		iframe
			max-height 300px
			@media $small-medium
				max-height initial
	a.zoom
		cursor pointer
		position relative
		display block
		text-align center
		vertical-align middle
		// max-height 300px
		overflow hidden
		@media $small-medium
			max-height initial
		icomoon('e63e')
		&:before
			display inline-block
			position absolute
			width 100%
			height 100%
			line-height 100%
			top 36%
			left 0
			color #0075BB
			font-size 5em
			z-index 2
			transition color .25s ease-in-out
		&:after
			width 100%
			height 100%
			content ""
			background #000
			position absolute
			left 0
			top 0
			opacity 0
			z-index 1
			transition opacity .25s ease-in-out
		&:hover
			&:after
				opacity 0.5
			&:before
				color #fff
			// text-shadow 10px 10px 10px #fff
	#cotizar
		// p
		// 	display none
		h2
			padding-bottom 0
	form
		display inline-block
		float none
		// margin-top 4em
		padding-bottom 2em
		input, textarea
			margin-bottom 0px
			// background-color lighten(#ECECEC,3)
			// border-bottom 0
			// border-left 10px solid @background-color
			// font-weight 400
			// border-radius 5px
			&:focus
				// border-bottom 0
				box-shadow none
			&::placeholder
				color #8c8c8c
		textarea
			height em
			padding-top .5em
			padding-bottom .5em
		button
			// float right
			// background-color #63BDBD
			margin-right -7px
	.actions
		display none
		position fixed
		bottom 0px
		z-index 2
		width 100%
		// display inline-block
		ul
			margin 0 auto
			padding 0
			display flex
			width 100%
			// margin 0 auto
			text-align center
			li
				flex-grow 1
				flex-shrink 1
				flex-basis auto
				a
					display block
					box-shadow inset 0px 0px 50px 0px #09598a
					text-shadow: 3px 1px 5px #09598a
					height 55px
					line-height @height
					// min-width 300px
					width 100%
					// margin-right 1%
					// padding-left 5px
					display inline-block
					background-color #0d9aee
					color #fff
					border-radius 25px 25px 0 0
					text-align center
					// position relative
					cursor pointer
					font-size 50px
					@media $small
						font-size 30px
					&:hover
						text-decoration none
					&:before
						content ""
						width 44px
						height 38px
						top 10px
						left -7px
						position relative
						// top 20px
						// left 50px
						display inline-block
						// margin-left 10px
						background-repeat no-repeat
						@media $small
							background-size @width @height
					&.email:before
						background-image url(imgs+'proy-email.png')
					&.call:before
						background-image url(imgs+'proy-llamar.png')
	@media $small
