//推荐首页 运维人员
.diy-ramd .text {
  font-size: 14px;
}
.diy-ramd .item {
  padding: 18px 0px;
}
.diy-ramd .clearfix:before,
.diy-ramd .clearfix:after {
  display: table;
  content: "";
}
.diy-ramd .clearfix:after {
  clear: both;
}
.diy-ramd .box-card {
  border-radius: 10px;
  background: #fff;
  height: 150px;
}
.diy-ramd .table_card {
  height:605px;
  border-radius: 10px;
  background: #fff;
}
.diy-ramd .right-card {
  border-radius: 10px;
  background: #fff;
  height: 310px;
}
.diy-ramd .box-card .team_Rectangle {
  height: 100%;
  width: 33%;
  background: #EFEFEF;
  border-radius: 10px;
  margin-left: -10px;
  float: left;
  position: relative;
}
.diy-ramd .box-card .team_Rectangle .team_icon {
  position: absolute;
  top: 45px;
  left: 0;
  right: 0;
  bottom: 0;
  text-align: center;
}
.diy-ramd .box-card .team_Approval {
  height: 100%;
  width: 66%;
  float: left;
  position: relative;
}
.diy-ramd .box-card .team_Approval .team_me {
  width: 50%;
  height: 100%;
  float: left;
}
.diy-ramd .font_color{
  color #168BF5
}
.diy-ramd .box-card .team_Approval .team_he {
  float: left;
  width: 50%;
  height: 100%;
}
.diy-ramd .right-card .team_li .team_box {
  width: 80px;
  margin: 20px;
  height: 80px;
  border-radius: 5px;
}
.diy-ramd .right-card .team_li {
  float: left;
  margin: 0px 26px;
  width: 80px;
  height: 130px;
  cursor: pointer;

}
.diy-ramd .table_card .table_id {
  font-weight: bold;
  font-size: 15px;
  margin: 20px;
  font-family: PingFang SC;
  color: #000009;
  opacity: 0.85;
}
.diy-ramd .notice_card .table_id {
  font-weight: bold;
  font-size: 15px;
  margin: 20px;
  font-family: PingFang SC;
  color: #000009;
  opacity: 0.85;
}
.diy-ramd .notice_card {
  height: 450px;
  margin-top: 20px;
  border-radius: 10px;
  background: #fff;
}
.diy-ramd .notice_card .circular {
  width: 14px;
  height: 14px;
  border: 1px solid #fda85c;
  border-radius: 50%;
  float: left;
}
.diy-ramd .notice_card .notice_time {
  float: left;
  font-family: PingFang SC;
  margin-left: 20px;
  font-size: 14px;
  line-height: 17px;
  color: #000000;
  opacity: 0.45;
}

.diy-ramd .notice_card .notice_maintenance {
  float: right;
  width: 80px;
  text-align: center;
  height: 20px;
  line-height: 20px;
  font-size: 14px;
  border: 1px solid #cccccc;
  border-radius: 2px;
  color: #000000;
  opacity: 0.65;
  cursor: pointer;
}
.diy-ramd .notice_card .notice_content {
  padding: 0px 0 0 30px;
  margin-left: 7px;
  height: 150px;
  border-left: 1px solid #e1e1e1;
}
.diy-ramd .notice_card .notice_content .notice_font {
  font-family: PingFang SC;
  font-size: 14px;
  margin-top: 5px;
  line-height: 20px;
  color: #000000;
  opacity: 0.65;
}

