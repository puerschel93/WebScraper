//vprocessbar条状图样式
.vprocessbar
  height: 40px;
  margin: 4px 0 7px 0;
  .vprocessbar-unit
    .vprocess-title
      color: #000;
      opacity: 0.65
  .s-progress-bar
    width:100%
  .vprocessbar-nounit
      height: 40px;
      line-height: 40px;
      font-size: 16px;
  .vprocessbar-font
      font-size: 12px;
      color: #392C28;
    .vprocessbar-font1
        margin-top: 4px;
        margin-bottom: 5px;
  .blue
    .s-progress-bar
      .s-progress-bar-inner
        background:#60ACFC !important;
      .s-progress-bar-outer
        border: unset !important;
        background-color: unset !important;
  .blue2
    .s-progress-bar
      .s-progress-bar-inner
        background:#32D3EB !important;
