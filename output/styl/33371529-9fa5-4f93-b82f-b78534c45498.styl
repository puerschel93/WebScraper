//vcard卡片样式
.vcard
  height: 100%;
  background: #FFFFFF;
  border: 1px solid #E1E1E1;
  padding: 0;
  .header
    height: 32px;
    line-height: 32px;
    padding: 0 16px;
    font-size: 14px;
    font-weight: bold;
    font-family: PingFang SC;
    border-bottom: 1px solid #e5e5e5;
    background: #F2F6FC;
    color: #392C28;
  .body
    padding: 5px 0;
  .clearfix
    height: 32px;
  .clearfix:before,
  .clearfix:after
    display: table;
    content: "";
    clear: both
//vcirclar圆图样式
.vcirclar
  .nature
    width: 100%;
    text-align: center;
    .text
      text-overflow: ellipsis;
      white-space: nowrap;
      overflow: hidden;
      font-family: PingFang SC;
      font-size: 12px;
    .text1
      color: #9C9796;
    .text2
      color: #392C28;
//vline单折现样式
