:root
  --vs-primary 215, 122, 97
  --vs-success 23, 201, 100
  --vs-danger 206, 91, 59
  --vs-warn 216, 180, 160
  --vs-dark 112, 118, 124
  --vs-background 21, 35, 40
  --vs-text 239, 241, 243
  --vs-gray-1 24,25,28
  --vs-gray-2 20,20,23
  --vs-gray-3 15,16,19
  --vs-gray-4 10,11,14
  --vs-shadow-opacity .3
  --vs-background-opacity .6
  --vs-button-border-radius 12px

$primary = 'rgb(%s)' % var(--vs-primary)
$secondary = #D8B4A0
$primary-light = #de927c
$primary-dark = #ce5b3b
$success = 'rgb(%s)' % var(--vs-success)
$danger = 'rgb(%s)' % var(--vs-danger)
$warn = 'rgb(%s)' % var(--vs-warn)
$dark = 'rgb(%s)' % var(--vs-dark)
$background = 'rgb(%s)' % var(--vs-background)
$background-lighter = #2c393e
$text = 'rgb(%s)' % var(--vs-text)
$shadow-opacity = var(--vs-shadow-opacity)
$background-opacity = var(--vs-background-opacity)
$border-radius = var(--vs-button-border-radius)
$border-color = #2d3237
