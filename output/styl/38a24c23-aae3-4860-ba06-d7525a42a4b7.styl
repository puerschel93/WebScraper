@import 'reset.css'

/* @import "function.css"; */
/*

-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=1,startColorstr=#99ffffff,endColorstr=#99ffffff)";  
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#99ffffff,endColorstr=#99ffffff);
background:rgba(255,255,255,.8);


filter:alpha(opacity=40); 针对 IE8 以及更早的版本 */
.w
	margin 0 auto
	max-width 1100px
	width 100%
.ff-GV
	font-family 'Great Vibes',arial
	font-size 50px
	font-style normal
	line-height 40px
	color #c19b77
h2
	.small
		position absolute
		top 50px
		left 0
		right 0
		text-align center
.line-public
	position relative
	&:after
		content ''
		position absolute
		left -200px
		width 190px
		height 2px
		background #c19b77
		top 50%
		margin-top -1px
	&:before
		content ''
		position absolute
		right -200px
		width 190px
		height 2px
		background #c19b77
		top 50%
		margin-top -1px
body
	font-size 100%

.header
	position fixed
	top 0
	left 0
	right 0
	min-width 1100px
	width 100%
	height 150px
	background #000000
	text-align center
	font-size 0
	z-index 2

	&>*
		display inline-block

	i
		width 0
		height 100%
		vertical-align middle

	.share-layout
		position absolute
		bottom 0
		z-index 2
		display none
		transform translateY(100%)
		width 586px
		height 424px
		background #f6f1e2
		.item
			width 50%
			float left
			padding 0
			text-align center
		h3
		p
			width 200px
			margin 0 auto
			text-align left
			font-size 12px
			color #4c4c4b
		p
			padding-bottom 20px
		h3
			padding-top 70px
			font-size 18px
			color #4c4c4b
			line-height 28px
			padding-bottom 10px

		.close
			position absolute
			right 30px
			top 30px
			width 36px
			cursor pointer
			z-index 1
			img
				display block
				width 100%

	.child-menu
		position absolute
		bottom 0
		z-index 2
		display none
		padding 5px 20px
		background #ffffff
		background rgba(#000, 0.8)
		transform translateY(100%)

		a
			display block
			margin 10px 0
			width 120px
			color #ffffff
			text-align left
			font-size 12px
			line-height 22px
			opacity 0.6

	.item
		position relative
		padding 0 12px 30px
		vertical-align bottom

		a
			color #aeaeae
			font-size 14px
			transition 0.3s

			&:hover, &.active
				color #b18361

	.share
		a
			padding-left 24px
			background url('../img/4b7fdc1d5609720262a67af49420d4c3-16+16.png') left center no-repeat

			&:hover, &.active
				background-image url('../img/376565f501c10f936fb46c35c6870afe-16+16.png')
				color #b18361

	.child
		&>a
			padding-right 20px
			background url('../img/216b6e604af956a3f9fe274164702f51-10+6.png') right center no-repeat

			&:hover, &.active
				background-image url('../img/5d0f2fd67f944a778fa9d8a3bef0bfdd-10+6.png')
				color #b18361

.main
	padding-top 150px
.banner-block


	position relative

	a, img
		display block
		width 100%

	.right-arrow
		position absolute
		top 50%
		right 0
		z-index 2
		margin-top -37.5px

	.left-arrow
		position absolute
		top 50%
		left 0
		z-index 2
		margin-top -37.5px

	.swiper-pagination-bullet-active
		background #80b435

.case-block
	padding-bottom 30px
	background #1c1c1d

	h2
		position relative
		padding 90px 0 50px
		text-align center
		font-size 30px
		line-height 40px
		color #c19b77
		.small
			position absolute
			top 50px
			left 0
			right 0
			text-align center
			

	.w
		padding-bottom 40px

		.container
			margin-left -0.3%

		a
			position relative
			margin-left 0.3%
			width 33%

			&:hover
				
				.title
					opacity 1
				img
					opacity 1
					width 100%

			.pic, img
				display block
				width 100%
				transition 0.3s
			img
				opacity 0.6

			.title
				position absolute
				top 50%
				left 50%
				opacity 0.6
				transition 0.3s
				transform translate(-50%, -50%)

				h3
					color #fff2ad
					text-align center
					white-space nowrap
					font-size 30px
					line-height 40px

					small
						display block
						font-size 12px
						line-height 22px

.news-block
	height 683px
	background #f5f5f5
	h2
		position relative
		font-size 30px
		color #1c1c1d
		text-align center
		line-height 40px
		padding-top 85px
	.w
		margin-top 25px
		background #ffffff
		border 1px solid #ececec
		border-bottom-width 5px
		position relative
		overflow hidden
		padding 20px 10px
		box-sizing border-box
		&:after
			content ''
			position absolute
			left 50%
			top 0
			bottom 0
			border-left 1px solid #eaeaea
		.item
			width 50%
			box-sizing border-box
			padding 10px 20px
			.pic
				img
					max-width 100%
					display block
					margin 0 auto
					width 100%
				
			h3
				font-size 18px
				color #b04c02
				padding-top 10px
				padding-bottom 10px
				line-height 28px
			p
				font-size 12px
				color #909090
				line-height 22px
.about-us-block
	background #ffffff
	// background url('../img/a2db09dcdd33e52870de262b3100c861-376+143.png') center 55px no-repeat
	h2
		position relative
		font-size 30px
		color #1c1c1d
		line-height 40px
		padding-top 110px
		text-align center
		.small
			top 60px
	.tabs-control
		padding 40px 0 30px
		border-bottom 1px solid #dee2e6
		a
			position relative
			width 20%
			text-align center
			&:after
				content ''
				position absolute
				left 50%
				transform translateX(-50%)
				bottom -30px
				width 75px
				height 3px
				background transparent
				transition 0.3s
			&:hover
			&.active
				display block
				&:after
					background-color #c19b77
			.icon
				width 54px
				height 54px
				margin 0 auto
				background url('img/32ee7a774da44e9bf65043738274a23b-902+54.png') left center no-repeat
				&.icon-2
					background-position -217px center
				&.icon-3
					background-position (-217px*2+5) center
				&.icon-4
					background-position (-217px*3+15) center
				&.icon-5
					background-position (-217px*4+20) center
			h3
				font-size 14px
				color #737373
				line-height 24px
	.tabs-wrapper
		padding 70px 0 100px
		.left
			width 400px
			img
				display block
				width 100%
		.right
			width 675px
			font-size 14px
			color #4c4c4b
			line-height 24px
			p
				padding-top 25px
.footer
	position relative
	background #1c1c1d
	.w
		height 468px
		position relative
	.tel
	.mail
		position absolute
		text-align center
		transform translate(0, -50%)
		top 50%
		h4
			padding-top 20px
		h4
		.value
			font-size 24px
			color #ffffff
			line-height 40px
	.tel
		left 0%
	.mail
		right 0%
	.nav
		padding-top 30px
		font-size 14px
		color #b4b4b4
		text-align center
		a
			padding 0 30px
	.copy-right
		position absolute
		left 0
		right 0
		height 60px
		padding 10px 0
		box-sizing border-box
		font-size 12px
		color #b4b4b4
		line-height 22px
		text-align center
		bottom 0
		background #151516
	.qr-code
		position absolute
		left 50%
		top 50%
		transform translate(-50%, -50%)
		text-align center
		img
			border 1px solid #c19b6e
			display block
			width 122px
			margin 25px auto

.no-js-banner-block
	a
	img
		display block
		width 100%

.public-title
	margin-bottom 20px
	text-align center
	h2
		position relative
		font-size 30px
		color #7b0000
		line-height 1
		margin-top 60px
		padding 0 10px
		&.ti10
			text-indent 25px
		.small
			top -50px

.product-detalis-block
	padding 60px 0
	.pic
		width 490px
		.big-pic
			width 100%
			height 400px
			img
				width 100%
				height 100%
		.swiper-container
			width 90%
			margin-top 20px
		.swiper-button-next
			right 0
			background-size 10px auto
		.swiper-button-prev
			left 0
			background-size 10px auto
		a
		img
			display block
			width 100%
		a
			border 1px solid #e5e5e5
			box-sizing border-box

	.content
		width 570px
		.bdb
			border-bottom 1px dotted #dddddd
			height 54px
			.star
				height 100%
				background url('../img/227b890acf0e9cd75dc2ce99174db4dd-58+14.jpg') left center no-repeat
				width 60px
			.review
				font-size 12px
				color #6daf23
				line-height 54px
			.look
				margin-top 10px
				line-height 30px
				border 2px solid #d8a44e
				border-radius 15px
				font-size 14px
				color #d8a44e
				line-height 30px
				padding 0 15px 0 30px
				background url('../img/f1b12334a8f4b5cf6008b106b556e271-12+10.jpg') 10px center no-repeat
		p
			font-size 14px
			color #767676
			line-height 34px
			&.pt20
				padding-top 20px
.product-detalis-info-block
	padding-bottom 40px
	font-size 14px
	color #767676
	line-height 34px
	.content
		padding 0 50px
		box-sizing border-box
		&.mih400
			min-height 400px
		.w560
			width 560px
.product-detalis-list-block
	padding-bottom 50px
	.list
		margin-left -2%
		padding-bottom 50px
		li
			width 23%
			margin-left 2%
			.pic
				width 258px
				height 268px
				box-sizing border-box
				border 1px solid #eaeaea
				img
					width 100%
					height 100%
					display block
			h3
				font-size 14px
				color #b04c02
				text-align center
				line-height 24px
				padding-top 10px

.page-control
	text-align center
	font-size 0
	a
	span
	input
		display inline-block
		font-size 14px
		color #787878
		line-height 40px
		box-sizing content-box
		border 1px solid transparent
		margin 0 5px
		padding 0 5px
	a
		min-width 20px
		border-color #ececec
		transition 0.3s
		&:hover
		&.active
			color #ffffff
			background #513f35
	input
		width 40px
		outline none
		border-color #ececec
	
				

// todos
.goto-top
	position fixed
	bottom 15px
	right 5px
	transform translatex(100%)
	opacity 0
	transition 0.3s
	&.active
		opacity 1
		transform translatex(0)
	img
		display block
		width 100%